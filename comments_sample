aV\u0420\u043e\u043c\u0430, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0442\u0432\u043e\u0439 \u043f\u0430\u043f\u0430 \u0441 \u0440\u043e\u0434\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a\u043e\u043c \u043f\u043e\u0442\u044b\u0440\u0438\u043b \u0434\u0435\u043d\u0435\u0433 \u0438\u0437 \u0440\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0433\u043e \u0431\u044e\u0434\u0436\u0435\u0442\u0430, \u0442\u043e \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u0434\u043e\u043b\u0436\u0435\u043d \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u044b \u0438 \u043e\u043d\u0438 \u043c\u043d\u043e\u0433\u0438\u0435 \u0433\u043e\u0434\u044b, \u0438 \u043d\u0435 \u0432 \u0424\u0438\u043d\u043b\u044f\u043d\u0434\u0438\u0438, \u0430 \u0432 \u041c\u0430\u0433\u0430\u0434\u0430\u043d\u0435. \u0420\u0430\u0431\u043e\u0442\u043e\u0434\u0430\u0442\u0435\u043b\u0438 \u0437\u0430 \u0447\u0443\u0436\u043e\u0439 \u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0439 \u0441\u0447\u0435\u0442.
p515
aV\u0427\u0442\u043e \u0437\u0430 \u043c\u0443\u0436\u0447\u0438\u043d\u044b \u0442\u0430\u043a\u0438\u0435, \u043d\u0443 \u043d\u0435\u0443\u0436\u0435\u043b\u0438 \u044d\u0442\u043e\u043c\u0443 \u0434\u0435\u0434\u043a\u0443 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0435\u043d \u0440\u0435\u0431\u0435\u043d\u043e\u043a, \u043f\u0440\u0438\u0447\u0435\u043c \u0441 \u0442\u0430\u043a\u0438\u043c\u0438 \u0432\u043e\u0442 \u043d\u0430\u043c\u0435\u0440\u0435\u043d\u0438\u044f\u043c\u0438. \u041f\u0440\u043e\u0441\u0442\u043e \u0432 \u0433\u043e\u043b\u043e\u0432\u0435 \u043d\u0435 \u0443\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u0435\u0442\u0441\u044f. \u041d\u0443 \u043d\u0430\u0439\u0434\u0438 \u0442\u044b \u0441\u0435\u0431\u0435 \u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u043d\u043e\u043b\u0435\u0442\u043d\u044e\u044e \u0438 \u043e\u0442\u0441\u044b\u043b\u0430\u0439 \u0444\u043e\u0442\u043e \u0442\u0430\u043a\u043e\u0439 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u0438. \u041a\u043e\u0448\u043c\u0430\u0440, \u0441\u043b\u0430\u0442\u044c \u0444\u043e\u0442\u043e \u0441\u0432\u043e\u0435\u0433\u043e \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u0430, 50 \u043b\u0435\u0442 \u043c\u0443\u0436\u0438\u043a\u0443 \u043f\u043e\u0447\u0442\u0438, \u0430 \u043c\u043e\u0437\u0433\u043e\u0432 \u043a\u0430\u043a \u0443 11 \u043b\u0435\u0442\u043d\u0435\u0433\u043e.
p516
aV\u0422\u0430\u043d\u044f, \u0441\u043b\u0435\u0434\u0443\u044f \u0432\u0430\u0448\u0435\u0439 \u0436\u0435 \u043b\u043e\u0433\u0438\u043a\u0435 \u0434\u043e\u043f\u0443\u0441\u043a\u0430\u044e, \u0447\u0442\u043e \u0432\u044b \u043b\u0436\u0435\u0442\u0435 \u0438 \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u0440\u0435\u0447\u0438\u0442\u0435 \u0441\u0430\u043c\u0438 \u0441\u0435\u0431\u0435, \u0442\u0430\u043a \u043a\u0430\u043a \u0440\u0430\u0441\u0442\u0435\u043d\u0438\u044f \u0442\u043e\u0436\u0435 \u0436\u0438\u0432\u044b\u0435. \u041f\u043e\u0443\u0447\u0430\u0435\u0442\u0441\u044f \u0432\u044b \u0438\u0445 \u0443\u0431\u0438\u0432\u0430\u0435\u0442\u0435... \u041f\u0440\u0438\u0447\u0438\u043d\u044f\u0435\u0442\u0435 \u0438\u043c, \u0442\u0430\u043a \u0441\u043a\u0430\u0437\u0430\u0442\u044c, \u0442\u0440\u0430\u0432\u043c\u044b \u043d\u0435 \u0441\u043e\u0432\u043c\u0435\u0441\u0442\u0438\u043c\u044b\u0435 \u0441 \u0436\u0438\u0437\u043d\u044c\u044e... \u041a\u0430\u043a-\u0442\u043e \u0442\u0430\u043a...
p517
aV\u041f\u0440\u043e\u0448\u0443 \u0438\u0437\u0432\u0438\u043d\u0438\u0442\u044c \u2014 \u0415\u043b\u0435\u043d\u0430.
p518
aV\u041d\u043e \u0447\u0442\u043e \u0442\u043e \u0432 \u043d\u0435\u043c \u0435\u0441\u0442\u044c!
p519
aV\u0414\u0410 \u043d\u0435 \u043f\u0440\u043e\u0441\u0438\u043b \u043e\u043d \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u044b, \u0437\u0430\u0447\u0435\u043c \u043f\u0438\u0441\u0430\u0442\u044c \u0442\u0430\u043a\u043e\u0435, \u0434\u043e\u043c\u044b\u0441\u043b\u0438\u0432\u0430\u0442\u044c \u0438 \u0434\u043e\u0434\u0443\u043c\u044b\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u0435? \u041e\u043d \u043f\u0440\u043e\u0441\u0438\u043b \u043f\u043e\u043c\u043e\u0449\u0438, \u0441\u0441\u044b\u043b\u0430\u044f\u0441\u044c \u043d\u0430 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0443 \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u044f \u0436\u0438\u043b\u0438\u0449\u043d\u044b\u0445 \u0443\u0441\u043b\u043e\u0432\u0438\u0439. \u041d\u0430\u043f\u0440\u044f\u043c\u0443\u044e \u043e\u043d \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u043f\u0440\u043e\u0441\u0438\u043b
p520
aV\u042f \u0447\u0438\u0442\u0430\u043b\u0430 \u0438\u043d\u0442\u0435\u0440\u0432\u044c\u044e \u043e\u0434\u043d\u043e\u0439 \u0430\u043a\u0442\u0440\u0438\u0441\u044b, \u043d\u0435 \u043f\u043e\u043c\u043d\u044e \u043a\u0430\u043a \u0437\u043e\u0432\u0443\u0442. \u0422\u0430\u043a \u0432\u043e\u0442 \u043e\u043d\u0430 \u0433\u043e\u0432\u043e\u0440\u0438\u043b\u0430, \u044f \u0435\u043c \u0442\u043e, \u0447\u0442\u043e \u0432\u044b\u0440\u0430\u0449\u0438\u0432\u0430\u044e. \u0418 \u0432 \u043b\u0435\u0441 \u0445\u043e\u0436\u0443 \u043d\u0430 \u043c\u0435\u0434\u0432\u0435\u0434\u044f \u0438 \u043b\u043e\u0441\u044f. \u0418 \u044f \u0434\u0443\u043c\u0430\u044e, \u0447\u0442\u043e \u043d\u0438 \u0440\u0430\u0431\u043e\u0439 \u043d\u0438 \u0432\u043e\u0440\u043e\u043c \u043e\u043d\u0430 \u0441\u0435\u0431\u044f \u043d\u0435 \u043e\u0449\u0443\u0449\u0430\u0435\u0442.
p521
aV\u0410 \u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0432\u043e\u043e\u0431\u0449\u0435 \u0418\u043d\u0442\u0435\u0440\u043d\u0435\u0442 \u0437\u0430\u043f\u0440\u0435\u0442\u0438\u043c, \u0447\u0435\u0433\u043e \u0443\u0436 \u0442\u0430\u043c. \u0411\u0443\u0434\u0435\u043c \u043a\u0430\u043a \u0432 \u041a\u041d\u0414\u0420. \u0422\u043e\u0433\u0434\u0430 \u0438 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u0435\u0447\u0435\u0433\u043e \u0431\u0443\u0434\u0435\u0442, \u0438 \u043d\u0435\u043a\u043e\u043c\u0443.
p522
aV\u041d\u0435 \u043d\u0430\u0441\u043e\u2026, \u0430 \u043d\u0430\u043f\u043b\u0430\u043a\u0430\u043b!!! \u041d\u0430\u043f\u043b\u0430\u043a\u0430\u043b \u0441\u0435\u0431\u0435 \u043f\u043e\u043c\u0435\u0449\u0435\u043d\u044c\u0438\u0446\u0435 \u043f\u043e\u0434 \u0447\u0430\u0441\u0438\u043a\u0438 \u0420\u0443\u0441\u043b\u0430\u043d\u0447\u0438\u043a.
p523
aV\u041c\u0438\u043c\u043e \u043f\u0440\u043e\u0445\u043e\u0434\u044f\u0449\u0435\u043c\u0443\u2026 \u041d\u0443 \u0438 \u043b\u0430\u0434\u043d\u0435\u043d\u044c\u043a\u043e\u2026\u0440\u0430\u0434 \u0437\u0430 \u0442\u0435\u0431\u044f\u2026\u0410 \u0442\u043e \u0432\u0434\u0440\u0443\u0433 \u043f\u043e\u043b\u0435\u0437\u0443\u0442, \u0430 \u0442\u043e \u0433\u043b\u044f\u0434\u0438\u0448\u044c \u0438 \u0437\u0430\u043b\u0435\u0437\u0443\u0442???
p524
aV\u0422\u0440\u0443\u0434\u043e\u0432\u044b\u0435 \u043f\u0435\u043d\u0441\u0438\u0438 -\u043d\u0430 1,7%.  \u0418 \u044d\u0442\u043e \u043d\u0435 \u0440\u043e\u0437\u044b\u0433\u0440\u044b\u0448. \u0421\u043f\u0430\u0441\u0438\u0431\u043e \u0437\u0430 \u043d\u0430\u0448\u0443 \u0441\u0447\u0430\u0441\u0442\u043b\u0438\u0432\u0443\u044e \u0441\u0442\u0430\u0440\u043e\u0441\u0442\u044c
p525
aV\u0410 \u043d\u0430 \u044d\u0442\u043e\u043c \u043f\u0435\u0440\u0435\u043a\u0440\u0435\u0441\u0442\u043a\u0435 \u0440\u0430\u0437\u0432\u0435 \u0435\u0441\u0442\u044c \u043f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u044b\u0439 \u043f\u0435\u0440\u0435\u0445\u043e\u0434?
p526
aV\u0441\u0442\u0430\u043b\u043a\u0435\u0440 \u0445\u0430\u0434\u043b\u0444 \u043b\u0430\u0439\u04442 \u043c\u0430\u0439\u0434\u0430\u043d\u0423\u0448\u043a\u0430
p527
aV\u043c\u0443\u043a. \u0430 \u043d\u0435 \u0437\u0430\u0441\u043b\u0430\u043d\u043d\u044b\u0439 \u043b\u0438 \u0442\u044b \u043a\u0430\u0437\u0430\u0447\u043e\u043a?
p528
aV\u0444\u0443\u043d\u0434\u0443\u043a\u043e\u043207.04.2014 10: 56: 06 \u0418\u0441\u0442\u043e\u0447\u043d\u0438\u043a: http://www.bk55.ru/mc2/news/article/2366 \u0414\u0430 \u043e\u043d\u0438 \u043f\u0440\u0438\u0432\u044b\u043a\u043b\u0438 \u043d\u0430 \u0445\u0430\u043b\u044f\u0432\u0443\u2026\u0425\u0430\u043b\u044f\u0432\u0430 \u044d\u0442\u043e \u0434\u043b\u044f \u043d\u0438\u0445 \u0432\u0442\u043e\u0440\u043e\u0435 \u042f. \u0414\u0430 \u0436\u0435 \u0441\u0430\u043c \u043d\u0435 \u0438\u0434\u0435\u0442, \u043d\u043e \u0441\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u0438\u0446\u0430 \u0441 \u043c\u0430\u043c\u043e\u0439 \u043f\u0440\u0438\u0442\u0430\u0449\u0438\u043b\u0430\u0441\u044c-\u0425\u0410\u041b\u042f\u0412\u0410\u2026
p529
aV"\u0422\u043e, \u0447\u0442\u043e \u0432\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0430\u043c\u0438 \u044d\u0442\u0438\u0445 \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u0439 \u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u0443\u0447\u0435\u0431\u043d\u044b\u0445 \u0437\u0430\u0432\u0435\u0434\u0435\u043d\u0438\u0439, \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0434\u0438\u043b\u0430 \u0438 \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0415\u043a\u0430\u0442\u0435\u0440\u0438\u043d\u0430 \u0421\u043f\u0435\u0445\u043e\u0432\u0430" \u0414\u0418\u0420\u0415\u041a\u0422\u041e\u0420 \u041e\u0411\u0420\u0410\u0417\u041e\u0412\u0410\u041d\u0418\u042f \u044d\u0442\u043e \u0431\u043b\u0438\u043d \u041a\u0422\u041e?
p530
aV\u041a\u0430\u043a \u0436\u0435 \u0442\u0430\u043a \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442\u0441\u044f? \u0414\u043e \u0430\u0432\u0433\u0443\u0441\u0442\u0430 \u043f\u043b\u0430\u0442\u0438\u043b \u043d\u0430\u043b\u043e\u0433\u0438 \u0438\u0441\u043f\u0440\u0430\u0432\u043d\u043e, \u0430 \u043f\u043e\u0441\u043b\u0435 \u0432\u043e\u0437\u0431\u0443\u0436\u0434\u0435\u043d\u0438\u044f \u043e\u043a\u0430\u0437\u0430\u043b\u043e\u0441\u044c, \u0447\u0442\u043e 1 \u043c\u043b\u0440\u0434. \u043d\u0435 \u0437\u0430\u043f\u043b\u0430\u0442\u0438\u043b. \u041d\u0435 \u0428\u0438\u0448\u043e\u0432\u0430 \u043d\u0430\u0434\u043e \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0442\u044c \u043a \u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u0438, \u0430 \u043d\u0430\u043b\u043e\u0433\u043e\u0432\u0438\u043a\u043e\u0432 \u0433\u043d\u0430\u0442\u044c \u0432 \u0412\u043e\u0440\u043a\u0443\u0442\u0443 \u0438 \u041c\u0430\u0433\u0430\u0434\u0430\u043d \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043d\u0430\u0447\u0438\u0441\u043b\u044f\u0442\u044c \u043d\u0430\u043b\u043e\u0433\u0438!
p531
aV\u0434\u0435\u0434 \u043f\u043e\u043a\u0430 \u0436\u0438\u0432\u043e\u0439 \u0431\u044b\u043b \u043f\u043e \u0433\u0440\u0438\u0431\u044b \u044f\u0433\u043e\u0434\u044b \u0445\u043e\u0434\u0438\u043b \u0434\u0430 \u0435\u0449\u0435 \u0438 \u043d\u0430 \u0440\u044b\u0431\u0430\u043b\u043a\u0443 \u043d\u0430 \u0430\u043b\u0442\u0430\u0439 \u0435\u0437\u0434\u0438\u043b. \u0445\u043e\u0442\u044f \u0440\u0430\u043d\u0435\u043d\u044b\u0439 \u0444\u0440\u043e\u043d\u0442\u043e\u0432\u0438\u043a
p532
aV\u0412\u0430\u0441\u0438\u043b\u044c, 5. \u041a\u0430\u043a \u044d\u0442\u043e \u043d\u0435\u0442 \u043f\u0440\u043e\u0444\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f? \u0412 \u041e\u043c\u0441\u043a\u0435 \u0435\u0441\u0442\u044c \u043f\u0435\u0434 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u043c \u0435\u0441\u0442\u044c \u0444\u0430\u043a\u0443\u043b\u044c\u0442\u0435\u0442 \u0434\u043e\u0448\u043a\u043e\u043b\u044c\u043d\u043e\u0433\u043e \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f.
p533
aV\u0422\u043e \u0435\u0441\u0442\u044c \u043c\u043e\u0439 \u043d\u043e\u0441 \u0433\u043e\u0440\u0430\u0437\u0434\u043e \u0431\u043e\u043b\u0435\u0435 \u0447\u0443\u0432\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u0435\u043d, \u0447\u0435\u043c \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u0443\u0440\u0430 \u0420\u043e\u0441\u043f\u0440\u0438\u0440\u043e\u0434\u043d\u0430\u0434\u0437\u043e\u0440\u0430 \u0438 \u0426\u0435\u043d\u0442\u0440\u0430 \u041c\u043e\u043d\u0438\u0442\u043e\u0440\u0438\u043d\u0433\u0430 \u043e\u043a\u0440\u0443\u0436\u0430\u044e\u0449\u0435\u0439 \u0441\u0440\u0435\u0434\u044b?
p534
aV\u0423\u043a\u0430\u0437\u0430 \u041f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u0430 \u0420\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0439 \u0424\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u0438 \u043e\u0442 07.05.2013 \u2116596 \u041e \u0434\u043e\u043b\u0433\u043e\u0441\u0440\u043e\u0447\u043d\u043e\u0439 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u044d\u043a\u043e\u043d\u043e\u043c\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u043f\u043e\u043b\u0438\u0442\u0438\u043a\u0435 "\u0433) \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0443\u043b\u0443\u0447\u0448\u0435\u043d\u0438\u044f \u0443\u0441\u043b\u043e\u0432\u0438\u0439 \u0432\u0435\u0434\u0435\u043d\u0438\u044f \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438: \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0438\u0442\u044c \u0434\u043e 1 \u044f\u043d\u0432\u0430\u0440\u044f 2015 \u0433. \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435 \u0441\u043e\u043a\u0440\u0430\u0449\u0435\u043d\u0438\u0435 \u0441\u0440\u043e\u043a\u043e\u0432 \u043f\u0440\u043e\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440 \u0441\u0443\u0431\u044a\u0435\u043a\u0442\u0430\u043c\u0438 \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0438 \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u044d\u0442\u0438\u0445 \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440 \u0432 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0445 \u0441\u0444\u0435\u0440\u0430\u0445 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u0440\u0435\u0433\u0443\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u
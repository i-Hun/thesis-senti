p635
aV\u044d\u0442\u043e \u044f, \u0412\u043e\u0442, \u0432\u043e\u0442 \u0442\u0443\u0442 \u0432\u0441\u0451 \u0434\u0435\u043b\u043e \u0432 \u0441\u043b\u043e\u0432\u0435 \u0447\u0430\u0441\u0442\u043d\u043e\u043c. \u0411\u044b\u043b \u0431\u044b \u043c\u0443\u043d\u0438\u0446\u0438\u043f\u0430\u043b\u044c\u043d\u044b\u043c, \u043f\u043e\u0436\u0430\u043b\u0443\u0439\u0441\u0442\u0430, \u0440\u0443\u0431\u0438\u0442\u0435 \u0441\u043a\u0432\u0435\u0440 \u0438 \u0441\u0442\u0440\u043e\u0439\u0442\u0435 10 \u043b\u0435\u0442.
p636
aV\u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0439 \u0441\u043f\u0435\u043a\u0442\u0430\u043a\u043b\u044c, \u0441\u043e\u0432\u0435\u0442\u0443\u044e \u0432\u0441\u0435\u043c
p637
aV\u0410 \u0432 \u0447\u0435\u043c \u043d\u0435 \u043f\u0440\u0430\u0432 \u0430\u0432\u0442\u043e\u0440? \u0432 \u0442\u043e\u043c \u0447\u0442\u043e \u0412\u0430\u043c \u041e\u0414\u041d \u043d\u0430\u0441\u0447\u0438\u0442\u0430\u043b\u0438?, \u0430 \u0442\u0435\u043f\u0435\u0440\u044c \u043d\u0435 \u0437\u043d\u0430\u044e\u0442 \u043a\u0430\u043a \u0432\u0435\u0440\u043d\u0443\u0442\u044c? \u0412 \u0442\u043e\u043c, \u0447\u0442\u043e \u0441\u043d\u043e\u0432\u0430 \u0434\u0435\u043d\u0435\u0433 \u0445\u043e\u0442\u044f\u0442 \u0438 \u0434\u0443\u043c\u0430\u044e\u0442 \u043a\u0430\u043a \u0438\u0445 \u0432\u0437\u044f\u0442\u044c? \u043d\u0430 \u0441\u0442\u043e\u0440\u043e\u043d\u0435 \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442\u0430 \u2014 \u0444\u0430\u043a\u0442\u044b.
p638
aV\u0410 \u043f\u043e\u0447\u0435\u043c\u0443, \u0441\u043a\u0430\u0436\u0438\u0442\u0435 \u043d\u0430 \u043c\u0438\u043b\u043e\u0441\u0442\u044c, \u044f \u0434\u043e\u043b\u0436\u043d\u0430 "\u0443\u043d\u0438\u043c\u0430\u0442\u044c\u0441\u044f", \u043a\u043e\u0433\u0434\u0430 \u0432\u0438\u0436\u0443, \u043a\u0430\u043a \u043f\u043e\u0448\u043b\u043e\u0441\u0442\u044c \u0438 \u043e\u0442\u043a\u0440\u043e\u0432\u0435\u043d\u043d\u0443\u044e \u043f\u0440\u043e\u0444\u0430\u043d\u0430\u0446\u0438\u044e \u0441\u0432\u043e\u0435\u0439 \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u0438 \u043f\u044b\u0442\u0430\u044e\u0442\u0441\u044f \u043d\u0430\u043c \u043f\u043e\u0434\u0430\u0442\u044c \u043f\u043e\u0434 \u0432\u0438\u0434\u043e\u043c \u0448\u0435\u0434\u0435\u0432\u0440\u0430? \u041a\u043e\u0433\u0434\u0430 \u043f\u0443\u0441\u0442\u044b\u0448\u043a\u0443 \u043d\u0430\u043c \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0442 "\u0442\u043e\u043d\u043a\u0438\u043c \u043c\u0430\u0441\u0442\u0435\u0440\u043e\u043c"? \u0418\u0437\u0432\u0438\u043d\u0438\u0442\u0435, \u043d\u043e \u0432 \u044d\u0442\u043e\u0442 \u0442\u0435\u0430\u0442\u0440 \u0438 \u043c\u043e\u0438 \u0434\u0435\u0442\u0438 \u0445\u043e\u0434\u044f\u0442, \u0438 \u0421\u043b\u0430\u0432\u0430 \u0411\u043e\u0433\u0443, \u0442\u0430\u043b\u0430\u043d\u0442\u043b\u0438\u0432\u0443\u044e \u0438\u0433\u0440\u0443 \u043e\u0442 "\u0444\u0430\u043d\u0442\u0438\u043a\u0430" \u043e\u043d\u0438 \u0440\u0430\u0437\u043b\u0438\u0447\u0438\u0442\u044c \u0443\u043c\u0435\u044e\u0442.
p639
aV\u041d\u0443 \u0434\u043b\u044f \u043b\u0438\u0431\u0435\u0440\u0430\u043b\u043e\u0432 * \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u0441\u0442\u043e\u0438\u0442, \u0442\u0430\u043a \u0447\u0442\u043e \u043f\u0440\u043e 146% \u043c\u043e\u0436\u043d\u043e \u0431\u044b\u043b\u043e \u0438 \u043d\u0435 \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u044c, \u044d\u0442\u0438 \u0431\u0430\u0439\u043a\u0438 \u043c\u044b \u0432 90\u0445 \u0441\u043b\u044b\u0448\u0430\u043b\u0438)))
p640
aV\u041e\u0442\u043b\u0438\u0447\u043d\u0430\u044f \u0441\u0442\u0430\u0442\u044c\u044f.
p641
aV\u043e\u043e\u043e \u043d\u0435 \u043f\u043e\u0440\u044f\u0434\u043e\u043a, \u043d\u0430\u0434\u043e \u043f\u043e\u0431\u0438\u0442\u044c \u0440\u0435\u043a\u043e\u0440\u0434 \u043f\u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0443 \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u0439, \u0430 \u0442\u043e \u0443 \u043c\u0435\u043d\u044f \u0432\u0441\u0435\u0433\u043e \u0438\u0445 30\u201350 \u0431\u044b\u043b\u043e =) \u0445\u043e\u0442\u044f \u0435\u0441\u043b\u0438 \u0443\u0447\u0435\u0441\u0442\u044c, \u0447\u0442\u043e \u043d\u0430 \u043e\u0434\u043d\u043e \u0443\u0442\u0435\u043f\u043b\u0435\u043d\u0438\u0435 \u0442\u0440\u0443\u0431 \u043e\u043a\u043e\u043b\u043e \u0434\u043e\u043c\u0430 \u044f \u043d\u0430\u043f\u0438\u0441\u0430\u043b\u0430 \u0442\u0440\u0438 \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u044f \u0438 \u0440\u0430\u0437 \u043f\u043e\u0437\u0432\u043e\u043d\u0438\u043b\u0430, \u0430 \u0432\u043e\u0437 \u0438 \u043d\u044b\u043d\u0435 \u0442\u0430\u043c. \u0431\u0443\u0434\u0443 \u043f\u0438\u0441\u0430\u0442\u044c \u0447\u0435\u0442\u0432\u0435\u0440\u0442\u044b\u0439 \u0443\u0436\u0435 \u0432 \u043f\u0440\u043e\u043a\u0443\u0440\u0430\u0442\u0443\u0440\u0443! \u0442\u043e \u0432 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u043c \u0433\u043e\u0434\u0443 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u0439 \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u0441\u044f \u043c\u043d\u043e\u0433\u043e\u043a\u0440\u0430\u0442\u043d\u043e! \u0430 \u043d\u0435 \u043f\u0438\u0448\u0443\u0442 \u0433\u043b\u0430\u0432\u0430\u043c \u0440\u0430\u0439\u043e\u043d\u043e\u0432, \u043d\u0435 \u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u0443 \u043d\u0438\u0445 \u0445\u043e\u0440\u043e\u0448\u0430\u044f \u043f\u043b\u0430\u043d\u043e\u0432\u0430\u044f \u0440\u0430\u0431\u043e\u0442\u0430, \u0430 \u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u0447\u0435\u043c \u0432\u044b\u0448\u0435 \u0440\u0430\u0441\u0441\u043c\u043e\u0442\u0440\u0435\u043d\u043e, \u0442\u0435\u043c \u044d\u0444\u0444\u0435\u043a\u0442\u0438\u0432\u043d\u0435\u0439 (\u0442\u0430\u043a \u043a\u0430\u0436\u0435\u0442\u0441\u044f)!, \u0430 \u0435\u0441\u043b\u0438 \u043d\u0430\u0434\u043e \u0432\u0441\u0451 \u0440\u0430\u0432\u043d\u043e \u0441\u043f\u0443\u0441\u0442\u044f\u0442 \u0432\u043d\u0438\u0437 \u043a \u044d\u0442\u043e\u0439 \u0431\u0435\u0441\u0442\u043e\u043b\u043a\u043e\u0432\u043e\u0439 \u0433\u043b\u0430\u0432\u0435 =)
p642
aV\u041d\u0435\u0433\u0440\u0430\u043c\u043e\u0442\u043d\u044b\u0445 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u043e\u0432, \u041d\u0430\u0442\u0430\u043b\u044c\u044f, \u043f\u0440\u043e\u0441\u0442\u043e \u0441\u0442\u0430\u043b\u043e \u0431\u043e\u043b\u044c\u0448\u0435. \u042d\u0442\u043e \u0432\u0435\u044f\u043d\u0438\u0435 \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u0438 \u043f\u043b\u043e\u0434\u044b \u0435\u043b\u044c\u0446\u0438\u043d\u0441\u043a\u043e\u0433\u043e \u0431\u0435\u0437\u0432\u0440\u0435\u043c\u0435\u043d\u044c\u044f \u0438 \u0431\u0435\u0441\u0448\u0430\u0431\u0430\u0448\u043d\u043e\u0441\u0442\u0438. \u0412\u0430\u043b\u0438\u0442\u044c \u0432\u0441\u0451 \u043d\u0430 \u0415\u0413\u042d \u0441\u043c\u044b\u0441\u043b\u0430 \u043d\u0435\u0442, \u0445\u043e\u0442\u044f \u043a\u043e\u043d\u0435\u0447\u043d\u043e \u0441\u0438\u0441\u0442\u0435\u043c\u0430 \u044d\u0442\u0430 \u0434\u0430\u043b\u0435\u043a\u0430 \u043e\u0442 \u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u0441\u0442\u0432\u0430. \u0423\u0442\u0435\u0440\u044f\u043d \u0441\u0430\u043c \u0438\u043d\u0442\u0435\u0440\u0435\u0441 \u043a \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u044e \u0417\u043d\u0430\u043d\u0438\u044f \u0438 \u043c\u043e\u0442\u0438\u0432\u0430\u0446\u0438\u044f. \u041d\u0430\u0434 \u044d\u0442\u0438\u043c \u043d\u0430\u0434\u043e \u0434\u0443\u043c\u0430\u0442\u044c.
p643
aV\u0412\u044b\u0448\u0435\u043b \u043d\u0430 \u0440\u0435\u0430\u043a\u0442\u0438\u0432\u043d\u043e\u0439 \u0442\u044f\u0433\u0435 \u0438\u0437 \u043d\u0435\u0433\u043e
p644
aV\u0423\u043a\u0440\u0430\u0438\u043d\u0430 \u0441\u043e\u0431\u0438\u0440\u0430\u0435\u0442\u0441\u044f \u043f\u0435\u0440\u0435\u043f\u0438\u0441\u0430\u0442\u044c \u043a\u043e\u043d\u0442\u0440\u0430\u043a\u0442, \u043d\u0435 \u0440\u0430\u0441\u043f\u043b\u0430\u0447\u0438\u0432\u0430\u044f\u0441\u044c \u0437\u0430 \u0441\u0442\u0430\u0440\u044b\u0435 \u0434\u043e\u043b\u0433\u0438. \u041d\u043e \u0442\u0430\u043a \u043d\u0435 \u0431\u044b\u0432\u0430\u0435\u0442. \u0421\u043d\u0430\u0447\u0430\u043b\u0430 \u043e\u043f\u043b\u0430\u0442\u0430, \u043f\u043e\u0442\u043e\u043c \u043f\u0435\u0440\u0435\u0441\u043c\u043e\u0442\u0440.
p645
aV\u043a\u0430\u043a \u0438 \u0432\u0441\u044f \u0443\u043a\u0440\u0430\u0438\u043d\u0430 \u043f\u043e\u0434 \u044e\u0431\u043a\u043e\u0439 \u0430\u043c\u0435\u0440\u0438\u043a\u0438
p646
aV\u0421\u0432\u0438\u043d\u0435\u0439-\u0442\u043e \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u0432 \u0445\u043e\u0445\u043b\u044f\u043d\u0434\u0438\u0438 \u043b\u044e\u0431\u044f\u0442...
p647
aV\u0421 \u043e\u0431\u043d\u043e\u0432\u043a\u043e\u0439 \u0420\u043e\u043c\u0430!
p648
aV\u0410\u0433\u0430 \u043d\u0435\u0442  \u043a\u0440\u0438\u043c\u0438\u043d\u0430\u043b\u0430,\u0447\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0438 \u0432\u044b\u0441\u0448\u0435\u0433\u043e \u044d\u0448\u0435\u043b\u043e\u043d\u0430 -\u0432\u0441\u0435 \u043f\u043e\u0433\u043e\u043b\u043e\u0432\u043d\u043e -\u0412\u041e\u0420\u042b,\u0414\u0415\u041f\u0423\u0422\u0410\u0422\u042b -\u0415\u0414\u0420\u041e\u0421\u042b -\u0412\u041e\u0420\u042b,\u0411\u0410\u041d\u041a\u0418-\u0412\u041e\u0420\u042b,\u041f\u0420\u041e\u041a\u0423\u0420\u041e\u0420\u042b -\u0412\u041e\u0420\u042b-\u041f\u041e\u041b\u0418\u0426\u0410\u0418-\u0412\u041e\u0420\u042b-\u0445\u0430-\u0445\u0430 \u043a\u0440\u0438\u043c\u0438\u043d\u0430\u043b\u0430 \u041d\u0415\u0422?!
p649
aV\u041a\u0438\u0442\u0430\u0439\u0446\u044b \u043e\u0442\u0432\u0435\u0434\u0443\u0442 \u0442\u0440\u0435\u0442\u044c \u0432\u043e\u0434\u044b \u0438\u0437 \u0418\u0440\u0442\u044b\u0448\u0430 \u0432\u043e\u0442 \u0438 \u0431\u0443\u0434\u0435\u0442 \u043a\u0430\u0442\u0430\u0441\u0442\u0440\u043e\u0444\u0430. \u0412\u0441\u0435 \u044d\u0442\u0438 \u043c\u0435\u0442\u0430\u043d\u0438\u044f: \u043f\u043e \u043f\u043b\u0430\u0442\u0438\u043d\u0435, \u043c\u0435\u0442\u0440\u043e, \u0424\u0435\u0434\u043e\u0440\u043e\u0432\u043a\u0435 \u043f\u043e\u0434\u043e\u0433\u0440\u0435\u0432\u0430\u044e\u0442\u0441\u044f \u043d\u0435\u0445\u0432\u0430\u0442\u043a\u043e\u0439 \u0431\u044e\u0434\u0436\u0435\u0442\u0430 \u0438 \u043f\u0440\u0435\u0434\u0441\u0442\u043e\u044f\u0449\u0438\u043c\u0438 \u0443\u0440\u0435\u0437\u0430\u043d\u0438\u044f\u043c\u0438 \u0438\u0437 \u0446\u0435\u043d\u0442\u0440\u0430 \u043f\u043e\u0441\u043b\u0435 \u0441\u043e\u0447\u0438\u043d\u0441\u043a\u043e\u0439 \u043e\u043b\u0438\u043c\u043f\u0438\u0430\u0434\u044b. \u0414\u0430\u0432\u0430\u0439\u0442\u0435 \u0437\u0430\u0441\u044b\u043f\u0438\u043c \u0434\u0435\u0441\u044f\u0442\u043e\u043a \u043a\u043c \u0442\u043e\u043d\u043d\u0435\u043b\u0435\u0439 \u043c\u0435\u0442\u0440\u043e \u0438 \u0432 \u041e\u043c\u0441\u043a\u0435 \u043d\u0435 \u0431\u0443\u0434\u0435\u0442 \u043d\u0430 \u0431\u043b\u0438\u0436\u0430\u0439\u0448\u0438\u0435 10\u201315 \u043b\u0435\u0442 \u043f\u0435\u0440\u0441\u043f\u0435\u043a\u0442\u0438\u0432\u044b \u0445\u043e\u0442\u044c \u043a\u0430\u043a-\u0442\u043e \u0434\u043e\u0431\u0440\u0430\u0442\u044c\u0441\u044f \u0434\u043e \u0440\u0430\u0431\u043e\u0442\u044b \u0431\u0435\u0437 \u043f\u0440\u043e\u0431\u043e\u043a. \u0424\u0435\u0434\u0435\u0440\u0430\u043b\u044b \u0442\u043e\u043b\u044c\u043a\u043e \u044d\u0442\u043e\u0433\u043e \u0438 \u0436\u0434\u0443\u0442: \u0437\u0430\u043a\u0440\u044b\u0442\u044c \u043e\u0431\u0440\u0435\u0437\u0430\u0442\u044c, \u0430 \u0442\u0430\u043c \u0432\u0430\u0448\u0438 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u044b. \u041d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u0431\u0440\u043e\u0441\u0430\u0442\u044c, \u0442\u043e\u043b\u044c\u043a\u043e \u0432\u043f\u0435\u0440\u0435\u0434 \u0438 \u0434\u0430\u0432\u0438\u0442\u044c \u0446\u0435\u043d\u0442\u0440.
p650
aV\u0421\u043b\u0430\u0431\u043e\u0441\u0442\u044c \u0421\u0430\u0448\u0438 \u0431\u044b\u043b\u0430 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0430 \u0422\u0438\u0442\u043e.. \u0412 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u043c \u0431\u043e\u044e \u0442\u043e\u0436\u0435 \u0434\u0432\u0430\u0436\u0434\u044b \u043e\u043d \u0437\u0430\u043f\u0443\u0441\u043a\u0430\u043b \u0437\u0430 \u0441\u043f\u0438\u043d\u0443 \u0441\u043e\u043f\u0435\u0440\u043d\u0438\u043a\u0430.\u0422\u0430\u043a\u0430\u044f \u0436\u0435 \u0431\u0435\u0434\u0430 \u0438 \u0443 \u0421\u0430\u0440\u043d\u0430\u0432\u0441\u043a\u043e\u0433\u043e. \u041e\u043d\u0438 \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0442 \u0431\u043e\u043b\u0435\u0435 \u043f\u043e-\u0441\u0430\u043c\u0431\u0438\u0441\u0442\u0441\u043a\u0438 (\u0438\u043b\u0438 \u043f\u043e \u043a\u043b\u0430\u0441\u0441\u0438\u043a\u0435, \u043e\u043d \u0436\u0435 \u043d\u0430\u0447\u0438\u043d\u0430\u043b \u0441 \u043d\u0435\u0451), \u043f\u043e \u0443\u0441\u0442\u0430\u0440\u0435\u0432\u0448\u0435\u0439 \u0442\u0440\u0430\u043a\u0442\u043e\u0432\u043a\u0435 \u0437\u0434\u0435\u0441\u044c. \u0410 \u0432\u0435\u0434\u044c \u0435\u0441\u0442\u044c \u0443 \u043a\u043e\u0433\u043e \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u043d\u0430\u0432\u044b\u043a\u0438 \u0432 \u041e\u043c\u0441\u043a\u0435, \u0412\u0438\u0442\u0430\u043b\u044f \u041a\u043e\u043c\u0430\u0440\u043e\u0432 \u043c\u043e\u0433 \u0431\u044b \u043d\u0430\u0443\u0447\u0438\u0442\u044c \u044d\u0442\u0438\u043c \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u0430\u043c \u043d\u0430 \u043e\u0447\u0435\u043d\u044c \u0434\u043e\u0441\u0442\u043e\u0439\u043d\u043e\u043c \u0443\u0440\u043e\u0432\u043d\u0435. \u0418 \u0432 \u0442\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043e\u0447\u043d\u043e\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u0435 \u043e\u043d \u0447\u0438\u0441\u043b\u0438\u0442\u0441\u044f..\u041f\u043e\u0447\u0435\u043c\u0443 \u0442\u0430\u043a\u0430\u044f \u043d\u0435\u0434\u043e\u0440\u0430\u0431\u043e\u0442\u043a\u0430, \u043d\u0435\u043f\u043e\u043d\u044f\u0442\u043d\u043e.. \u0421\u043a\u043e\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043e \u0438\u0437-\u0437\u0430 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u043f\u043e\u0437\u0438\u0446\u0438\u0438 \u0421\u0430\u0448\u0438: "\u042d\u0442\u043e \u043c\u043e\u0439 \u0441\u0442\u0438\u043b\u044c".
p651
aV\u0410\u043b\u0435\u043a\u0441, \u0430 \u043f\u043e\u0447\u0435\u043c\u0443 \u043b\u044e\u0434\u0438, \u043e\u0442\u0432\u0435\u0447\u0430\u044e\u0449\u0438\u0435 \u0437\u0430 \u0436\u0438\u0437\u043d\u044c \u0438 \u0437\u0434\u043e\u0440\u043e\u0432\u044c\u0435 \u0431\u043e\u043b\u044c\u0448\u043e\u0433\u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u043f\u043e\u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0438\u0445 \u043b\u044e\u0434\u0435\u0439, \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442 \u043f\u043e 18 \u0447\u0430\u0441\u043e\u0432?
p652
aV\u0445\u043e\u0445\u043b\u044b \u0441 2005 \u0433\u043e\u0434\u0430 \u0443\u0441\u0442\u0440\u0430\u0438\u0432\u0430\u044e\u0442 \u043c\u0430\u0439\u0434\u0430\u043d\u044b, \u043d\u0438\u0447\u0435\u0433\u043e \u0445\u043e\u0440\u043e\u0448\u0435\u0433\u043e \u043e\u0442 \u044d\u0442\u043e\u0433\u043e \u043d\u0435 \u043f\u0440\u043e\u0438\u0437\u043e\u0448\u043b\u043e. \u0421\u0435\u0439\u0447\u0430\u0441 \u0441\u043c\u0435\u043d\u044f\u0442 \u0432\u043b\u0430\u0441\u0442\u044c, \u0432\u043e\u0439\u0434\u0443\u0442 \u0432 \u0415\u0441 \u043f\u043e\u0434\u043d\u0438\u043c\u0435\u0442\u0441\u044f \u0432\u0441\u0435 \u043a\u0440\u043e\u043c\u0435 \u0437\u0430\u0440\u043f\u043b\u0430\u0442, \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0441\u0442\u0432\u043e \u043f\u0430\u0434\u0435\u0442.
p653
aV\u0412\u044b\u0431\u0438\u0442\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u0441\u0442\u0435\u043a\u043b\u0430 \u043d\u0430 \u0438\u0437\u043e\u0431\u0440\u0430\u0436\u0435\u043d\u0438\u0438 \u043d\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u0435\u0442 \u043f\u043e \u0444\u043e\u0440\u043c\u0435 \u041e\u043c\u0441\u043a\u0443\u044e \u043e\u0431\u043b\u0430\u0441\u0442\u044c)
p654
aV\u0447\u0442\u043e \u0432 \u043d\u0435\u0439 \u0441\u0435\u043a\u0441\u0443\u0430\u043b\u044c\u043d\u043e\u0433\u043e? 
p655
aV\u0422\u0430\u043a \u0447\u0442\u043e, \u0432 \u0438\u0442\u043e\u0433\u0435, \u0431\u0443\u0434\u0435\u0442 - \u043b\u0435\u0433\u043a\u043e\u0440\u0435\u043b\u044c\u0441\u043e\u0432\u043e\u0435 \u043c\u0435\u0442\u0440\u043e \u0438\u043b\u0438 \u0441\u043a\u043e\u0440\u043e\u0441\u0442\u043d\u043e\u0439 \u0442\u0440\u0430\u043c\u0432\u0430\u0439? \u0422\u0430\u043a \u0438 \u0431\u0443\u0434\u0443\u0442 \u043c\u0435\u0442\u0430\u043d\u0438\u044f \u0442\u0443\u0434\u0430 -\u0441\u044e\u0434\u0430 \u0441 \u0431\u044e\u0434\u0436\u0435\u0442\u043d\u044b\u043c \u0444\u0438\u043d\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c "\u043f\u0440\u043e\u0436\u0435\u043a\u0442\u043e\u0432"....?
p656
aV\u0435\u0441\u0442\u044c \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c, \u0447\u0442\u043e \u043f\u043e\u0441\u043b\u0435 \u0435\u0433\u043e \u043f\u0440\u0438\u0435\u0437\u0434\u0430 \u043d\u0430\u0448 \u0434\u0435\u0439\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0439 \u0433\u0440\u0430\u0434\u043e\u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b\u044c \u043e\u0442\u043f\u0440\u0430\u0432\u0438\u0442\u0441\u044f \u0432 \u043e\u0442\u0441\u0442\u0430\u043a\u0443 
p657
aV3 \u043c\u043e\u0441\u0442\u0430 \u0431\u0443\u0434\u0443\u0442 \u0440\u0435\u043c\u043e\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c, \u043c\u0430\u0440\u043a\u0441\u0430 \u043f\u0435\u0440\u0435\u043a\u0440\u043e\u044e\u0442 \u043d\u0430 \u0433\u043e\u043b\u0443\u0431\u043e\u043c \u043e\u0433\u043e\u043d\u044c\u043a\u0435, \u0432\u0441\u0435 \u043f\u043e\u0432\u0435\u0437\u0443\u0442 \u0441\u0432\u043e\u0438\u0445 \u0434\u0435\u0442\u0435\u0439 \u0432 \u0448\u043a\u043e\u043b\u044b. \u0442\u0440\u0430\u0444\u0438\u043a \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0435\u043d. \u0437\u0430\u0431\u044b\u0432\u0430\u0439\u0442\u0435 \u043f\u0440\u043e \u0434\u043e\u0440\u043e\u0433\u0438 \u0432 \u0446\u0435\u043d\u0442\u0440\u0435. \u0435\u0437\u0436\u0430\u0439\u0442\u0435 \u043e\u0431\u044a\u0435\u0437\u0434\u043d\u044b\u043c\u0438 \u0434\u043e\u0440\u043e\u0433\u0430\u043c\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043d\u0435\u0442
p658

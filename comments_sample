p565
aV\u0414\u0440\u0443\u0436\u0438\u0449\u0435, \u043d\u0435 \u0434\u0443\u043c\u0430\u044e, \u0447\u0442\u043e \u0412.\u0412. \u0437\u0434\u0435\u0441\u044c \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043f\u0440\u0438\u0447\u0438\u043d\u043e\u0439.
p566
aV\u0415\u0441\u043b\u0438 \u044d\u0442\u0438\u043c \u0443\u0436 \u0437\u0430\u043d\u044f\u043b\u0441\u044f \u043c\u044d\u0440, \u0432\u0438\u043d\u043e\u0432\u043d\u043e\u0433\u043e \u043e\u0431\u044f\u044f\u044f\u044f\u044f\u044f\u044f\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043d\u0430\u0439\u0434\u0443\u0442. \u041c\u0438\u0444\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0441\u0442\u0440\u0435\u043b\u043e\u0447\u043d\u0438\u043a\u0438 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u044e\u0442
p567
aV\u044b\u0444\u0432\u0430, \u0441 \u0442\u043e\u0433\u043e \u0445\u043e\u0442\u044f \u0431\u044b, \u0447\u0442\u043e \u0432\u0430\u0448\u0430 \u0442\u0440\u044f\u043f\u043a\u0430 \u0431\u0443\u0434\u0435\u0442 \u043f\u043e\u0440\u0442\u0438\u0442\u044c \u0432\u043d\u0435\u0448\u043d\u0438\u0439 \u0432\u0438\u0434 \u0434\u043b\u044f \u0432\u0441\u0435\u0445, \u0430 \u0441\u043e\u043c\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043e\u043b\u044c\u0437\u0443 \u043f\u0440\u0438\u043d\u043e\u0441\u0438\u0442\u044c \u0442\u043e\u043b\u044c\u043a\u043e \u0432\u0430\u043c. \u0412\u043d\u0443\u0442\u0440\u0438 \u0441\u0432\u043e\u0435\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u044b \u0432\u044b \u0445\u043e\u0442\u044c \u0432\u043c\u0435\u0441\u0442\u043e \u043e\u0431\u043e\u0435\u0432 \u0440\u0435\u043a\u043b\u0430\u043c\u0443 \u043a\u043b\u0435\u0438\u0442\u044c \u043c\u043e\u0436\u0435\u0442\u0435
p568
aV\u041c\u043e\u043b\u043e\u0434\u0446\u044b, \u0434\u0435\u0432\u0447\u043e\u043d\u043a\u0438! \u0413\u043b\u0430\u0432\u043d\u043e\u0435, \u0447\u0442\u043e\u0431\u044b \u0442\u0435\u043f\u0435\u0440\u044c \u043a\u043e\u043c\u0430\u043d\u0434\u0443 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u044c \u0438 \u0432\u044b\u0431\u0438\u0442\u044c \u0443 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u0430 \u043d\u043e\u0432\u044b\u0439 \u0437\u0430\u043b! \u0410 \u0442\u0430\u043c - \u0432 \u043f\u0443\u0442\u044c \u0437\u0430 \u0437\u043e\u043b\u043e\u0442\u043e\u043c!
p569
aV\u0423\u0440\u0430. \u0421\u043a\u043e\u043b\u044c\u043a\u043e \u043b\u0435\u0442 \u0431\u043e\u0440\u043e\u043b\u0438\u0441\u044c \u0441 \u044d\u0442\u0438\u043c \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u043b\u043e\u043c, \u0434\u0430\u0436\u0435 \u0443\u0436\u0435 \u043e\u0431\u043b\u0430\u0441\u0442\u043d\u044b\u0435 \u0432\u043b\u0430\u0441\u0442\u0438 (\u043f\u043e\u0437\u043e\u0440\u043d\u0438\u043a\u0438) \u0441\u0434\u0430\u043b\u0438\u0441\u044c, \u043d\u0430 \u043c\u0438\u0440\u043e\u0432\u0443\u044e \u043f\u043e\u0448\u043b\u0438.
p570
aV\u043c\u043d\u0435 \u043f\u043e\u0445\u0435\u0440, \u044f \u043d\u0430 \u043a\u043e\u0431\u044b\u043b\u0435
p571
aV\u0415\u043b\u0435\u043d\u0430 \u042f\u0440\u043e\u0432\u0430\u044f \u043b\u0430\u0436\u0430\u0435\u0442 \u0441\u0442\u0440\u0430\u0448\u043d\u043e. \u0422\u043e \u0421\u0438\u043d\u044e\u0433\u0438\u043d \u0412\u0438\u043a\u0442\u043e\u0440, \u0442\u043e \u0413\u0430\u043c\u0431\u0443\u0440\u0433 \u0432\u0438\u0446\u0435-\u043c\u044d\u0440.
p572
aV\u041e\u0442\u0432\u0435\u0447\u0430\u044e \u043d\u0430 \u0432\u043e\u043f\u0440\u043e\u0441 \u043f\u0440\u043e \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0438\u0435 \u0441\u043e\u0431\u044b\u0442\u0438\u044f. \u043f\u0440\u0438\u0435\u0445\u0430\u043b\u0430 \u0441\u043a\u043e\u0440\u0430\u044f, \u044d\u0442\u043e\u0433\u043e \u043c\u0443\u0436\u0447\u0438\u043d\u0443 \u043f\u043e\u0441\u0430\u0434\u0438\u043b\u0438 \u0432 \u0441\u043a\u043e\u0440\u0443\u044e, \u043d\u043e \u0447\u0435\u0440\u0435\u0437 \u043f\u0430\u0440\u0443 \u043c\u0438\u043d\u0443\u0442 \u043e\u0442\u043f\u0443\u0441\u0442\u0438\u043b\u0438, \u0441\u043a\u0430\u0437\u0430\u043b\u0438 \u0447\u0442\u043e \u043e\u043d \u043e\u0442\u043a\u0430\u0437\u0430\u043b\u0441\u044f \u043e\u0442 \u0433\u043e\u0441\u043f\u0438\u0442\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0438. \u042f \u0431\u044b\u043b \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438 \u043f\u0440\u043e\u0442\u0438\u0432 \u0442\u0430\u043a\u043e\u0433\u043e \u0440\u0435\u0448\u0435\u043d\u0438\u044f, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u0435\u0441\u043b\u0438 \u043a\u0440\u043e\u0432\u044c \u043d\u0430 \u0441\u0430\u043c\u043e\u043c \u0434\u0435\u043b\u0435 \u0438\u0437 \u0443\u0445\u0430 (\u0438\u0437\u043d\u0443\u0442\u0440\u0438), \u0442\u043e \u044d\u0442\u043e \u043f\u0440\u0438\u0437\u043d\u0430\u043a \u0442\u0440\u0430\u0432\u043c\u044b \u0433\u043e\u043b\u043e\u0432\u043d\u043e\u0433\u043e \u043c\u043e\u0437\u0433\u0430). \u041a\u043e\u0433\u0434\u0430 \u044f \u043d\u0430\u0447\u0430\u043b \u0432\u044b\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0442\u044c \u043f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u0438 \u0441\u043a\u043e\u0440\u043e\u0432\u0438\u043a\u0430\u043c, \u043f\u0435\u0440\u0435\u0434\u043e \u043c\u043d\u043e\u0439 \u0433\u0440\u043e\u043c\u043a\u043e \u0445\u043b\u043e\u043f\u043d\u0443\u043b\u0438 \u0434\u0432\u0435\u0440\u044c\u044e \u0438\u0445 \u0433\u0430\u0437\u0435\u043b\u0438. \u0425\u043e\u0440\u043e\u0448\u043e, \u0447\u0442\u043e \u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u0441\u043e \u043c\u043d\u043e\u0439 \u0431\u044b\u043b \u0433\u043b\u0430\u0432\u0432\u0440\u0430\u0447 \u043e\u0434\u043d\u043e\u0439 \u0438\u0437 \u0431\u043e\u043b\u044c\u043d\u0438\u0446 \u0438 \u0433\u0440\u0430\u043c\u043e\u0442\u043d\u043e \u0438\u043c \u0432\u0441\u0435 \u043f\u043e\u044f\u0441\u043d\u0438\u043b, \u043f\u0440\u0438\u0433\u0440\u043e\u0437\u0438\u043b \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0435\u043c. \u0412 \u0438\u0442\u043e\u0433\u0435 \u043c\u0443\u0436\u0447\u0438\u043d\u0443 \u0442\u043e\u0433\u043e \u0443\u0432\u0435\u0437\u043b\u0438, \u0438 \u043a\u0430\u043a \u044f \u043f\u043e\u0442\u043e\u043c \u0443\u0437\u043d\u0430\u043b, \u0435\u0433\u043e \u0432\u0441\u0435-\u0442\u0430\u043a\u0438 \u0433\u043e\u0441\u043f\u0438\u0442\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u043b\u0438. \u041d\u0435 \u0441\u0447\u0438\u0442\u0430\u044e \u0441\u0432\u043e\u0439 \u043f\u043e\u0441\u0442\u0443\u043f\u043e\u043a \u043a\u0430\u043a\u0438\u043c \u0442\u043e \u043f\u0440\u0438\u0437\u043d\u0430\u043a\u043e\u043c \u0433\u0435\u0440\u043e\u0439\u0441\u0442\u0432\u0430 \u0438\u043b\u0438 \u0447\u0435\u0433\u043e \u0442\u043e \u043f\u043e\u0434\u043e\u0431\u043d\u043e\u0433\u043e, \u043f\u0440\u043e\u0441\u0442\u043e \u0432 \u0433\u043e\u043b\u043e\u0432\u0443 \u043d\u0435 \u0443\u043a\u043b\u0430\u0434\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u0447\u0442\u043e \u0430\u0434\u0435\u043a\u0432\u0430\u0442\u043d\u044b\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043c\u043e\u0433 \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u043f\u043e-\u0434\u0440\u0443\u0433\u043e\u043c\u0443.
p573
aV\u0410 \u0432 \u0447\u0451\u043c \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0430-\u0442\u043e? \u041e\u043d \u0432\u0430\u0441 \u043f\u043e\u0441\u043b\u0430\u043b \u043c\u0430\u0442\u043e\u043c, \u0442\u0430\u043a \u0438 \u0432\u044b \u0435\u0433\u043e \u043c\u0430\u0442\u043e\u043c \u043f\u043e\u0448\u043b\u0438\u0442\u0435. \u042d\u0442\u043e \u0436\u0435 \u0438\u0433\u0440\u043e\u043a \u0410\u0432\u0430\u043d\u0433\u0430\u0440\u0434\u0430. \u041e\u043d\u0438 \u0434\u0440\u0443\u0433\u043e\u0433\u043e \u044f\u0437\u044b\u043a\u0430 \u043d\u0435 \u0437\u043d\u0430\u044e\u0442. \u041c\u0430\u043c\u0430 \u0441 \u043f\u0430\u043f\u043e\u0439 \u043d\u0435 \u043d\u0430\u0443\u0447\u0438\u043b\u0438.
p574
aV\u043c\u043e\u0433\u0443\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u0437\u0430\u043f\u0440\u0435\u0449\u0430\u0442\u044c, \u0441\u0435\u0431\u044f \u0431\u044b \u0437\u0430\u043f\u0440\u0435\u0442\u0438\u043b\u0438
p575
aV\u041c\u043e\u0436\u043d\u043e \u0442\u0440\u0438\u0441\u0442\u0430 \u0440\u0430\u0437 \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u044c, \u043a\u0442\u043e \u043f\u0440\u0430\u0432 \u0438\u043b\u0438 \u043d\u0435 \u043f\u0440\u0430\u0432. \u041d\u043e \u043b\u044e\u0431\u043e\u0435 \u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u043e \u0434\u043b\u044f \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0430 \u0441\u0442\u0440\u0435\u043c\u0438\u0442\u0441\u044f \u0441\u043e\u0437\u0434\u0430\u0442\u044c \u0441\u0440\u0435\u0434\u043d\u0438\u0439 \u043a\u043b\u0430\u0441\u0441, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0438\u0441\u043a\u043e\u043d\u043d\u043e \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0435\u0433\u043e \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u043e\u0439. \u0410 \u0441\u0443\u0434\u044f \u043f\u043e \u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440\u0443 \u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440\u043e\u0432\u0438\u0447\u0443, \u043e\u043d \u0445\u043e\u0447\u0435\u0442 \u043f\u043e\u0441\u0442\u0440\u043e\u0438\u0442\u044c \u0442\u043e\u0442\u0430\u043b\u0438\u0442\u0430\u0440\u043d\u043e\u0435 \u0433\u043e\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u043e, \u0430 \u043d\u0435 \u0434\u0435\u043c\u043e\u043a\u0440\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0435.
p576
aV\u0410 \u0442\u0443\u0434\u0430 \u0432\u043e\u043e\u0431\u0449\u0435 \u043b\u044e\u0434\u0438 \u0445\u043e\u0434\u044f\u0442??,\u044f \u0432\u043e\u043e\u0431\u0449\u0435 \u0434\u0443\u043c\u0430\u044e \u0447\u0442\u043e \u0442\u0430\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u0442\u043e\u044f\u043d\u043a\u0430 \u0432 \u043f\u043b\u044e\u0441\u0435 \u0431\u044b\u043b\u0430))
p577
aV\u0434\u0430 \u0438 \u0442\u0430\u043a \u0432 \u041c\u0438\u043d\u043a\u0443\u043b\u044c\u0442\u0435 \u043d\u0435 \u0432\u0441\u0451 \u0432 \u043f\u043e\u0440\u044f\u0434\u043a\u0435 \u0431\u044b\u043b\u043e, \u0430 \u0442\u0435\u043f\u0435\u0440\u044c \u0443 \u043d\u0430\u0441 \u0441 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043e\u0439 \u0411\u0435\u0434\u0430 \u0431\u0443\u0434\u0435\u0442\u2026
p578
aV\u041c\u043e\u0436\u0435\u0442 \u044f \u0432\u044b\u0441\u043a\u0430\u0437\u0430\u043b\u044c\u0441\u044f \u0438\u0437\u043b\u0438\u0448\u043d\u0435 \u0440\u0435\u0437\u043a\u043e, \u043d\u043e \u043a\u0430\u043a \u043c\u043e\u0436\u043d\u043e \u043d\u0435 \u0443\u0447\u0438\u0442\u044b\u0432\u0430\u0442\u044c \u043f\u0440\u0438\u0432\u0435\u0434\u0435\u043d\u043d\u044b\u0435 \u043f\u043e\u0441\u0442\u0443\u043f\u043a\u0438 \u041d\u043e\u0432\u043e\u0434\u0432\u043e\u0440\u0441\u043a\u043e\u0439? \u042f \u043e\u0447\u0435\u043d\u044c \u0412\u0430\u0441 \u043f\u0440\u043e\u0448\u0443, \u0413\u0435\u043e\u0440\u0433\u0438\u0439, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0439\u0442\u0435 \u0412\u0430\u0448 \u0442\u0430\u043b\u0430\u043d\u0442, \u0430 \u043e\u043d \u0443 \u0412\u0430\u0441 \u043d\u0435\u0441\u043e\u043c\u043d\u0435\u043d\u043d\u043e, \u0435\u0441\u0442\u044c, \u043d\u0430 \u0431\u043e\u043b\u0435\u0435 \u0434\u043e\u0441\u0442\u043e\u0439\u043d\u044b\u0435 \u0432\u0435\u0449\u0438.
p579
aV\u0416\u0430\u043b\u044c, \u043d\u043e \u043d\u0435 \u043f\u0440\u0438\u0437\u043d\u0430\u0435\u0442
p580
aV\u0422\u0443\u0441\u044f, \u0430 \u0432\u044b \u043f\u043e\u0439\u0434\u0435\u0442\u0435 \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u043f\u043e 18 \u0447\u0430\u0441\u043e\u0432?
p581
aV\u041e\u043b\u044c\u0433\u0430, \u0437\u043d\u0430\u0435\u0442\u0435 \u044f \u0442\u0430\u043a \u0442\u043e \u0442\u043e\u0436\u0435 \u0441 \u0434\u0435\u0442\u044c\u043c\u0438 \u043f\u0440\u0438\u0435\u0437\u0436\u0430\u044e \u0442\u0443\u0434\u0430. \u0415\u0441\u043b\u0438 \u0443 \u0432\u0430\u0441 \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0441 \u0442\u0435\u043b\u0435\u0436\u043a\u043e\u0439 \u043f\u043e \u043f\u0430\u0440\u043a\u043e\u0432\u043a\u0435, \u0442\u043e \u0438 \u0445\u043e\u0434\u0438\u0442 \u0442\u0430\u043c. \u0423 \u043c\u0435\u043d\u044f \u043a\u0430\u043a \u0442\u043e \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u044f \u0432 \u0436\u0438\u0437\u043d\u0438 \u0445\u0432\u0430\u0442\u0430\u0435\u0442. \u0418 \u043e\u0431\u044b\u0447\u043d\u043e \u044f \u0441 \u043f\u0430\u043a\u0435\u0442\u0430\u043c\u0438 \u0432\u044b\u0431\u0435\u0433\u0430\u044e \u0438\u0437 \u0410\u0448\u0430\u043d\u0430 \u0432 22 \u0447\u0430\u0441\u0430, \u043e\u0431\u0440\u0430\u0442\u043d\u043e \u0443\u0436 \u0442\u043e\u0447\u043d\u043e \u043d\u0435 \u043f\u0443\u0441\u0442\u044f\u0442 \u0447\u0442\u043e\u0431\u044b \u044f \u043c\u043e\u0433\u043b\u0430 \u0437\u0430\u0431\u0440\u0430\u0442\u044c \u0441\u0432\u043e\u0438 10 \u0440\u0443\u0431\u043b\u0435\u0439. \u041c\u043d\u0435 \u043a\u0441\u0442\u0430\u0442\u0438 \u0432\u043e\u043e\u0431\u0449\u0435 \u043d\u0435 \u0436\u0430\u043b\u043a\u043e 10 \u0440\u0443\u0431\u043b\u0435\u0439, \u043d\u043e \u0432\u043e\u0442 \u0441\u0447\u0438\u0442\u0430\u044e \u0447\u0442\u043e \u044d\u0442\u043e \u043d\u043e\u0432\u0448\u0435\u0441\u0442\u0432\u043e \u043d\u0435 \u043d\u0430\u0434\u043e\u043b\u0433\u043e, \u043b\u044e\u0434\u0435\u0439 \u0441\u0442\u0430\u043d\u0435\u0442 \u0432 \u0440\u0430\u0437\u044b \u043c\u0435\u043d\u044c\u0448\u0435, \u0434\u0443\u043c\u0430\u044e \u043d\u0435 \u044f \u043e\u0434\u043d\u0430 \u0440\u0435\u0448\u0438\u043b\u0430 \u044d\u0442\u0443 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0443 \u0434\u0440\u0443\u0433\u0438\u043c\u0438 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430\u043c\u0438
p582
aV\u041c\u0435\u0434\u0438, \u043d\u0443 \u0438 \u0447\u0442\u043e \u0436\u0435 \u0434\u0430\u043b\u044c\u0448\u0435 @ \u043f\u043e\u044d\u043c\u0430?
p583
aV\u041d\u0430\u0434\u043e \u043a\u0430\u043a \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u0441\u0435 \u0434\u0435\u043b\u0430\u0435\u0442\u0441\u044f \u0432 \u043d\u0430\u0448\u0435\u043c \u041e\u043c\u0441\u043a\u0435. \u041a\u0430\u043a \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u0441\u044f, \u043a\u0442\u043e \u0438\u0449\u0435\u0442, \u0442\u043e\u0442 \u0432\u0441\u0435\u0433\u0434\u0430 \u043d\u0430\u0439\u0434\u0435\u0442.
p584
aV\u041b\u044e\u0431\u043b\u044e \u0433\u043e\u0440\u043e\u0434 \u041e\u041c\u0421\u041a, \u043e\u0447\u0435\u043d\u044c \u0431\u044b \u0445\u043e\u0442\u0435\u043b\u043e\u0441\u044c \u0447\u0442\u043e\u0431\u044b \u043d\u0430\u0448 \u0433\u043e\u0440\u043e\u0434 \u043f\u0440\u043e\u0446\u0432\u0435\u0442\u0430\u043b \u0438 \u0432\u0435\u0440\u043d\u0443\u043b \u0441\u0435\u0431\u0435 \u0437\u0432\u0430\u043d\u0438\u0435 \u0433\u043e\u0440\u043e\u0434-\u0441\u0430\u0434
p585
aV\u0410 \u043a\u043e\u043c\u0443 \u0447\u0435\u0440\u0435\u0437 vseplatezi.ru \u0437\u0430 \u044d\u043b-\u0432\u043e \u043f\u043b\u0430\u0442\u0438\u0442\u044c \u0442\u043e\u0433\u0434\u0430?
p586
aV\u0421\u0438\u043d\u044e\u0433\u0438\u043d \u2014 \u0442\u0430\u043a\u043e\u0439 \u0436\u0435 \u043f\u043e\u0440\u044f\u0434\u043e\u0447\u043d\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u043a\u0430\u043a \u0412\u044b \u2014 \u0434\u0443\u043c\u0430\u044e\u0449\u0438\u0439 \u0438 \u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u0443\u044e\u0449\u0438\u0439)))). \u0412\u044b \u0438\u043b\u0438 \u043d\u0435 \u0437\u043d\u0430\u043a\u043e\u043c\u044b \u0441 \u043d\u043e\u0432\u0435\u0439\u0448\u0435\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u0435\u0439 \u0420\u043e\u0441\u0441\u0438\u0438, \u0438\u043b\u0438, \u0438\u0437\u0432\u0438\u043d\u0438\u0442\u0435, \u0438\u043d\u0442\u0435\u043b\u043b\u0435\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u0430\u044f \u0434\u0435\u0431\u0438\u043b\u043a\u0430.
p587
aV\u041c\u043e\u043b\u043e\u0434\u0447\u0438\u043a \u0421.\u041d.
p588
aV\u0414\u0430 \u0438 \u043e\u0431\u044a\u044f\u0432\u043b\u0435\u043d\u0438\u0435 \u043d\u0430 \u0434\u0432\u0435\u0440\u044f\u0445 \u043f\u043e\u0447\u0442\u044b: \u043d\u0443\u0436\u043d\u044b \u043f\u043e\u0447\u0442\u0430\u043b\u044c\u043e\u043d\u044b, \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u0430 6000 \u0440\u0443\u0431\u043b\u0435\u0439. \u0422\u0443\u0442 \u0434\u043e 15000 \u0442\u043e \u0434\u0430\u043b\u0435\u043a\u043e, \u0430 \u0434\u043e 25000 \u0442\u0430\u043a \u0432\u043e\u043e\u0431\u0449\u0435 \u043f\u043e\u043b \u0433\u043e\u0434\u0430 \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c
p589
aV\u041a\u0440\u0443\u0433\u043e\u0432\u043e\u0435 \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435 \u043a\u0430\u043a \u0431\u044b\u043b\u043e \u0430\u0434\u0441\u043a\u043e\u0435, \u0442\u0430\u043a \u0438 \u0431\u0443\u0434\u0435\u0442. \u041b\u0443\u0447\u0448\u0435 \u0431\u044b \u0440\u0435\u0433\u0443\u043b\u0438\u0440\u0443\u0435\u043c\u044b\u0439 \u043f\u0435\u0440\u0435\u043a\u0440\u0451\u0441\u0442\u043e\u043a \u0441\u0434\u0435\u043b\u0430\u043b\u0438. \u041a\u0440\u0443\u0433\u043e\u0432\u043e\u0435 \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435 \u0432\u043e\u043e\u0431\u0449\u0435 \u043d\u0430\u0434\u043e \u0441\u0442\u0430\u0440\u0430\u0442\u044c\u0441\u044f \u043f\u043e \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u0438 \u0438\u0437\u0431\u0435\u0433\u0430\u0442\u044c. \u041b\u0443\u0447\u0448\u0435 \u043f\u043e \u043a\u0440\u0443\u0433\u0443 \u0433\u0434\u0435-\u043d\u0438\u0431\u0443\u0434\u044c \u043f\u0440\u043e\u0435\u0445\u0430\u0442\u044c, \u0447\u0435\u043c \u043d\u0430 \u043a\u043e\u043b\u044c\u0446\u0435 \u043a\u0430\u043a \u0438\u0434\u0438\u043e\u0442 \u0434\u0451\u0440\u0433\u0430\u0442\u044c\u0441\u044f.
p590
aV\u0422\u043e\u0433\u0434\u0430 \u043d\u0435 \u0440\u0430\u0441\u0441\u043b\u0430\u0431\u043b\u044f\u0439\u0441\u044f \u2014 \u043d\u0430\u043f\u0440\u044f\u0433\u0430\u0439\u0441\u044f)) \u0414\u043e\u0441\u0442\u0430\u043b \u0443\u0436\u0435 ((
p591
aV\u041f\u0440\u0438\u0447\u0435\u043c \u0442\u0443\u0442 \u0440\u0443\u043b\u044c, \u0438\u0434\u0438\u043e\u0442\u044b? \u0414\u0430 \u0445\u043e\u0442\u044c \u043f\u043e\u0441\u0435\u0440\u0435\u0434\u0438\u043d\u0435 \u043e\u043d \u0431\u0443\u0434\u0435\u0442, \u0442\u043e\u043b\u043a\u0443 \u0442\u043e, \u0435\u0441\u043b\u0438 \u043c\u043e\u0437\u0433\u0430 \u043d\u0435\u0442\u0443, \u0442\u043e \u043c\u043e\u0436\u043d\u043e \u0434\u0430\u0436\u0435 \u043d\u0430 \u0441\u0430\u043c\u043e\u043a\u0430\u0442\u0435 \u0443\u043b\u0435\u0442\u0435\u0442\u044c \u0432 \u043a\u044e\u0432\u0435\u0442.
p592
aV\u042f \u043d\u0435 \u0437\u043d\u0430\u044e \u044d\u0442\u043e\u0433\u043e \u0433\u043e\u0441\u043f\u043e\u0434\u0438\u043d\u0430, \u043d\u043e \u043d\u0430\u0434\u043e \u0431\u044b\u0442\u044c \u043e\u0431\u044a\u0435\u043a\u0442\u0438\u0432\u043d\u044b\u043c\u0438. \u041f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e, \u0447\u0442\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043d\u0430\u043f\u0438\u0441\u0430\u043b. \u0422\u043e\u043b\u044c\u043a\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u043b \u043d\u0435 \u043f\u043e \u0442\u0435\u043c\u0435. \u0418 \u0437\u0430\u0447\u0435\u043c \u043e\u0441\u043a\u043e\u0440\u0431\u043b\u044f\u0442\u044c \u041e\u043c\u0441\u043a\u0443\u044e \u043f\u0440\u0430\u0432\u0434\u0443 \u2014 \u0441\u0442\u0430\u0440\u0435\u0439\u0448\u0443\u044e \u043e\u043c\u0441\u043a\u0443\u044e \u0433\u0430\u0437\u0435\u0442\u0443, \u0433\u0434\u0435 \u0435\u0441\u0442\u044c \u043e\u0447\u0435\u043d\u044c \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0435 \u0430\u0432\u0442\u043e\u0440\u044b. \u0412 \u043a\u0430\u0436\u0434\u043e\u043c \u0438\u0437\u0434\u0430\u043d\u0438\u0438 \u0435\u0441\u0442\u044c \u0432\u0435\u0434\u0443\u0449\u0438\u0435 \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442\u044b \u0438 \u0442\u0430\u043a \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043c\u0435\u043d\u0435\u0435 \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u044b\u0435 \u2014 \u0437\u0430\u0447\u0435\u043c \u043e\u0441\u043a\u043e\u0440\u0431\u043b\u044f\u0442\u044c \u0434\u0440\u0443\u0433 \u0434\u0440\u0443\u0433\u0430. \u0412\u043e\u0442 \u043f\u043e \u0434\u0435\u043b\u0443 \u043f\u0443\u0441\u0442\u044c \u0433-\u043d \u0410\u0440\u0436\u0430\u043d\u043d\u0438\u043a\u043e\u0432 \u0441\u043a\u0430\u0436\u0435\u0442 \u043d\u0430\u043c \u043e\u0434\u043d\u043e: \u043f\u0440\u0430\u0432\u0434\u0430, \u0447\u0442\u043e \u0432 \u0441\u0442\u0430\u0442\u044c\u0435 \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u0430 \u043f\u043e \u0441\u0443\u0442\u0438 \u043f\u0440\u043e\u0441\u0442\u0430\u044f \u0432\u0435\u0449\u044c \u2014 \u0442\u043e, \u0447\u0442\u043e \u043a\u0430\u043a \u0440\u0430\u0437 \u043d\u0430\u0437\u044b\u0432\u0430\u044e\u0442 \u043c\u043e\u0448\u0435\u043d\u043d\u0438\u0447\u0435\u0441\u0442\u0432\u043e\u043c \u2014 \u0447\u0442\u043e \u043e\u043d \u0440\u0430\u0431\u043e\u0442\u0430\u044f \u0432 \u0433\u0430\u0437\u0435\u0442\u0435, \u0441\u043e\u0437\u0434\u0430\u043b "\u043f\u0440\u043e\u043a\u043b\u0430\u0434\u043a\u0443" \u0434\u043b\u044f \u043f\u0435\u0440\u0435\u043f\u0440\u043e\u0434\u0430\u0436\u0438 \u2014 \u0432\u043e\u0442 \u043f\u0443\u0441\u0442\u044c \u043f\u043e\u044f\u0441\u043d\u0438\u0442 \u2014 \u0432\u0440\u0435\u0442 \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442 \u0438\u043b\u0438 \u043f\u0440\u043e\u043a\u043b\u0430\u0434\u043a\u0430 \u0431\u044b\u043b\u0430. \u0424\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u043e \u0443\u0433\u043e\u043b\u043e\u0432\u043d\u043e\u043c\u0443 \u0417\u0430\u043a\u043e\u043d\u0443 \u0442\u0443\u0442 \u043a\u0440\u0438\u043c\u0438\u043d\u0430\u043b\u0430 \u043d\u0435\u0442, \u044f \u0441 \u041e\u0411\u042d\u041f\u043e\u0432\u0446\u0430\u043c\u0438 \u0441\u043e\u0433\u043b\u0430\u0441\u0435\u043d, \u044d\u0442\u043e \u043d\u0435 \u0431\u044e\u0434\u0436\u0435\u0442\u043d\u044b\u0435 \u0434\u0435\u043d\u044c\u0433\u0438 \u2014 \u0442\u043e\u043b\u044c\u043a\u043e \u0441 \u0442\u0430\u043a\u0438\u043c \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u043e\u043c \u043f\u043e\u0442\u043e\u043c \u043d\u0438\u043a\u0442\u043e \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u043d\u0435 \u0431\u0443\u0434\u0435\u0442. \u0422\u043e\u0433\u0434\u0430 \u043d\u0443\u0436\u043d\u043e \u043d\u0435\u0441\u0442\u0438 \u044d\u0442\u0443 \u0441\u0442\u0430\u0442\u044c\u044e \u043f\u0440\u044f\u043c\u043e \u043d\u0430 \u0441\u0442\u043e\u043b \u043a \u042e.\u0413\u0430\u043c\u0431\u0443\u0440\u0433\u0443 \u0438\u043b\u0438 \u0412.\u041a\u0443\u043f\u0440\u0438\u044f\u043d\u043e\u0432\u0443. \u041d\u0435\u043b\u044c\u0437\u044f \u0438\u0437-\u0437\u0430 \u043e\u0434\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0445\u043e\u0434\u0438\u043c\u0446\u0430 \u0433\u0443\u0431\u0438\u0442\u044c \u0445\u043e\u0440\u043e\u0448\u0443\u044e \u0438\u0434\u0435\u044e \u0421.\u041a\u043e\u0440\u0430\u0431\u0435\u043b\u044c\u043d\u0438\u043a\u043e\u0432\u0430 \u2014 \u0441\u043e\u0437\u0434\u0430\u0442\u044c \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u0443\u044e \u0433\u0430\u0437\u0435\u0442\u0443 \u0434\u043b\u044f \u043b\u0438\u0446 \u0441\u0442\u0430\u0440\u0448\u0435\u0433\u043e \u0438 \u0441\u0440\u0435\u0434\u043d\u0435\u0433\u043e \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0430. \u0412\u043e\u0442 \u043f\u0443\u0441\u0442\u044c \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043e\u0442\u0432\u0435\u0442\u0438\u0442 \u043f\u0440\u044f\u043c\u043e: \u0431\u044b\u043b\u043e \u0438\u043b\u0438 \u043d\u0435 \u0431\u044b\u043b\u043e. \u0415\u0441\u043b\u0438 \u0432 \u0441\u0442\u0430\u0442\u044c\u0435 \u0432\u0440\u0430\u043d\u044c\u0435, \u0442\u043e\u0433\u0434\u0430 \u044d\u0442\u043e \u043d\u0435 \u0441\u0434\u0435\u043b\u0430\u0435\u0442 \u0447\u0435\u0441\u0442\u0438 \u0440\u0435\u0434\u0430\u043a\u0446\u0438\u0438.
p593
aV\u0422\u044b \u043d\u0435\u0441\u0435\u0448\u044c \u0431\u0440\u0435\u0434 \u043f\u043e\u0447\u0438\u0449\u0435 \u0421\u0435\u0440\u0433\u0435\u0439 \u0413\u043d\u0438\u0434\u044b.
p594
aV\u0422\u0430\u043a, \u043e\u043d \u0442\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0434 \u0431\u0443\u0434\u0435\u0442 ,\u0435\u0441\u043b\u0438 \u043a\u0442\u043e - \u043d\u0438\u0431\u0443\u0434\u044c \u0435\u0433\u043e \u043d\u0430 \u043d\u0451\u043c \u043f\u0440\u0438\u043c\u0435\u0442...
p595
aV\u041a\u0441\u0442\u0430\u0442\u0438, \u0443\u0434\u0430\u0440 \u043f\u043e \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0443 \u0421\u0443\u0441\u043b\u0438\u043a\u043e\u0432 \u043d\u0430\u043d\u0435\u0441 \u0433\u043e\u0440\u0430\u0437\u0434\u043e \u0441\u0438\u043b\u044c\u043d\u0435\u0435 \u0438 \u0431\u043e\u043b\u044c\u043d\u0435\u0435. \u0412\u0435\u0434\u044c \u0441\u043e\u0433\u043b\u0430\u0441\u0438\u0442\u0435\u0441\u044c: \u0432 \u0442\u0430\u043a\u043e\u043c \u0431\u043e\u043b\u044c\u0448\u043e\u043c \u0433\u043e\u0440\u043e\u0434\u0435 \u043a\u0430\u043a \u041e\u043c\u0441\u043a, \u0435\u0441\u0442\u044c \u0441\u0442\u043e\u043b\u044c\u043a\u043e \u0434\u043e\u0441\u0442\u043e\u0439\u043d\u044b\u0445 \u0434\u0435\u044f\u0442\u0435\u043b\u0435\u0439 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b, \u043f\u043e\u043c\u0438\u043c\u043e \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u043a\u0438\u0445 \u0442\u0430\u043b\u0430\u043d\u0442\u043e\u0432 \u0438\u043c\u0435\u044e\u0449\u0438\u0445 \u043e\u043f\u044b\u0442 \u0430\u0434\u043c\u0438\u043d\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f \u0432 \u044d\u0442\u043e\u0439 \u0441\u0444\u0435\u0440\u0435, \u0447\u0442\u043e \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435 \u043d\u0430 \u0438\u0445 \u0444\u043e\u043d\u0435 \u041b\u0430\u043f\u0443\u0445\u0438\u043d\u0430 \u0432\u044b\u0433\u043b\u044f\u0434\u0438\u0442 \u043c\u044f\u0433\u043a\u043e \u0433\u043e\u0432\u043e\u0440\u044f \u043d\u0435\u043f\u043e\u043d\u044f\u0442\u043d\u044b\u043c. \u0425\u043e\u0442\u044f \u0443 \u043d\u0435\u0433\u043e \u043f\u0435\u0440\u0435\u0434 \u043d\u0438\u043c\u0438 \u0435\u0441\u0442\u044c \u043d\u0435\u043e\u0441\u043f\u043e\u0440\u0438\u043c\u043e\u0435 \u0434\u043e\u0441\u0442\u043e\u0438\u043d\u0441\u0442\u0432\u043e \u2014 \u043c\u043e\u0434\u043d\u043e\u0435 \u0442\u0430\u0442\u0443 \u043d\u0430 \u0440\u0443\u043a\u0435 \u0432 \u0432\u0438\u0434\u0435 \u0441\u
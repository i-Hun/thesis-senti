p457
aV\u0412\u0430\u0441\u044f, \u0442\u044b \u043a\u0430\u043a\u043e\u0439 \u0442\u043e \u043d\u0435 \u043b\u043e\u0433\u0438\u0447\u043d\u044b\u0439, \u0442\u043e *, \u0442\u043e \u043b\u0443\u0433\u0430\u043d\u0447\u0430\u043d\u0435.
p458
aV\u0422\u0430\u043a \u0442\u043e \u044d\u0442\u043e \u0431\u044b\u043b\u0430 \u0438\u043d\u0438\u0446\u0438\u0430\u0442\u0438\u0432\u0430 \u043d\u0435 \u0433\u0443\u0431\u0435\u0440\u043d\u0430\u0442\u043e\u0440\u0430, \u0430 \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442\u0430, \u043a\u043e\u0442\u043e\u0440\u0430\u044f, \u043a\u0441\u0442\u0430\u0442\u0438, \u0442\u043e\u0436\u0435 \u043f\u0435\u0440\u0435\u0447\u0438\u0441\u043b\u0438\u043b\u0430 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0443 \u0441\u0432\u043e\u0435\u0439 \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u044b.)) \u0418\u0441\u043a\u0430\u0436\u0430\u0442\u044c \u0444\u0430\u043a\u0442\u044b \u0442\u043e \u0437\u0430\u0447\u0435\u043c?
p459
aV\u0412\u0441\u0435 \u0441\u0432\u0435\u0440\u0448\u0438\u043b\u043e\u0441\u044c \u0443\u0432\u0430\u0436\u0430\u0435\u043c\u044b\u0435. \u0414\u0430\u0432\u0430\u0439\u0442\u0435 \u043f\u043e\u0434\u043e\u0436\u0434\u0435\u043c. \u041c\u043e\u0436\u0435\u0442 \u043f\u0440\u0430\u0432\u0434\u0430, \u0447\u0442\u043e-\u0442\u043e \u0431\u0443\u0434\u0435\u0442 \u043f\u043e \u0434\u0440\u0443\u0433\u043e\u043c\u0443. \u0423\u0432\u0430\u0436\u0430\u044e \u0412\u0430\u043b\u0435\u043d\u0442\u0438\u043d\u0443 \u041d\u0438\u043a\u043e\u043b\u0430\u0435\u0432\u043d\u0443. \u041d\u043e \u0432\u0440\u0435\u043c\u044f \u0441\u0442\u0440\u0435\u043c\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0431\u0435\u0436\u0438\u0442 \u0432\u043f\u0435\u0440\u0435\u0434. \u0412\u043e\u0437\u0440\u0430\u0441\u0442 55 \u043d\u0430\u0432\u0435\u0440\u043d\u043e\u0435 \u043d\u0435 \u0437\u0440\u044f. \u041e\u0445, \u043d\u0435 \u0437\u0440\u044f. \u0422\u043e\u043b\u044c\u043a\u043e \u043e\u0442\u043f\u0443\u0441\u043a\u0430\u0442\u044c \u0432 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u044d\u0442\u0430\u043f \u0436\u0438\u0437\u043d\u0438 \u043c\u043e\u0436\u043d\u043e \u043f\u043e \u0434\u043e\u0431\u0440\u043e\u043c\u0443 \u0441 \u043f\u043e\u0447\u0435\u0441\u0442\u044f\u043c\u0438 \u0438 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u043d\u043e\u0441\u0442\u044c\u044e, \u043c\u043e\u0436\u0435\u0442 \u0432\u0441\u0435-\u0442\u0430\u043a\u0438 \u043e\u043d\u0430 \u044d\u0442\u043e\u0433\u043e \u0437\u0430\u0441\u043b\u0443\u0436\u0438\u043b\u0430. \u041a\u0430\u0436\u0435\u0442\u0441\u044f, \u0447\u0442\u043e \u044d\u0442\u043e\u0442 \u0432\u043e\u0437\u0440\u0430\u0441\u0442 \u043d\u0430\u0441\u0442\u0443\u043f\u0430\u0435\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u0443 \u0441\u0435\u0433\u043e\u0434\u043d\u044f\u0448\u043d\u0438\u0445 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439, \u043f\u043e\u0432\u0435\u0440\u044c\u0442\u0435 \u043e\u043d \u043f\u0440\u0438\u0434\u0435\u0442 \u0438 \u043a \u0412\u0430\u043c \u0438 \u0431\u044b\u0441\u0442\u0440\u0435\u0435 \u0447\u0435\u043c \u0412\u044b \u0434\u0443\u043c\u0430\u0435\u0442\u0435 \u043c\u043e\u043b\u043e\u0434\u0435\u0436\u044c. \u0441\u0442\u0440\u0435\u043c\u044f\u0449\u0430\u044f\u0441\u044f \u043b\u044e\u0431\u044b\u043c \u043f\u0443\u0442\u0435\u043c \u043a \u0432\u043b\u0430\u0441\u0442\u0438.
p460
aV\u0415\u0449\u0435 \u043e\u0434\u0438\u043d \u0441\u0438\u0434\u0435\u043b\u0435\u0446 \u0441\u043a\u043e\u0440\u043e \u0431\u0443\u0434\u0435\u0442! \u0422\u0430\u043c \u0447\u0435\u0441\u0442\u043d\u044b\u043c \u0431\u044b\u0442\u044c \u043d\u0435 \u0434\u0430\u043b\u0438 \u0431\u044b \u043e\u0441\u0442\u0430\u0442\u044c\u0441\u044f. \u0423 \u041d\u0430\u0437\u0430\u0440\u0430 \u0433\u043e\u043b\u043e\u0432\u0430 \u0447\u0435\u043c \u0434\u0443\u043c\u0430\u0435\u0442?
p461
aV\u0410 \u0447\u0435\u0433\u043e\u0439 \u0442\u043e \u0432\u0441\u0435 \u0442\u043e \u0432\u0438\u043d\u043e\u0432\u0430\u0442\u044b,\u0435\u0441\u043b\u0438 \u0448\u043a\u043e\u043b\u044c\u043d\u0438\u0446\u0430 \u0441\u0430\u043c\u0430 \u043a\u0443\u0440\u0438\u0446\u0430,\u0433\u0434\u0435 \u043f\u043e\u043f\u0430\u043b\u043e \u0448\u043b\u0430.\u0442\u0430\u043a\u0438\u0445 \u0434\u0443\u0440 \u043d\u0430 \u0443\u043b\u0438\u0446\u0435 \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0443\u0433\u043e\u0434\u043d\u043e
p462
aV\u042f \u043d\u0435 \u0442\u0440\u0435\u0431\u0443\u044e \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0430\u0437\u0430\u0434, \u043a\u0430\u043a \u0433\u043e\u0441\u043f\u043e\u0434\u0438\u043d \u041b\u0438\u0445\u0430\u0447\u0435\u0432, \u044f \u0433\u043e\u0432\u043e\u0440\u044e, \u0447\u0442\u043e \u0434\u043e\u043b\u0436\u043d\u0430 \u0431\u044b\u0442\u044c \u043b\u043e\u0433\u0438\u043a\u0430 \u0432 \u0440\u0435\u0447\u0430\u0445 \u0438 \u0434\u0435\u043b\u0430\u0445: \u043e\u0441\u0443\u0434\u0438\u0442\u0435 \u0441\u0443\u0434\u043e\u043c \u0415\u043b\u044c\u0446\u0438\u043d\u0430, \u0430 \u043f\u043e\u0442\u043e\u043c \u0443\u0436 \u0441\u0443\u0434\u0438\u0442\u0435 \u043e \u0425\u0440\u0443\u0449\u0435\u0432\u0435 \u0438 \u041a\u0440\u044b\u043c\u0435\u2026
p463
aV\u0412 \u043f\u0440\u043e\u0448\u043b\u043e\u043c \u0433\u043e\u0434\u0443, \u043a\u043e\u0433\u0434\u0430 \u043f\u043e\u0434\u043d\u0438\u043c\u0430\u043b\u0438 \u0446\u0435\u043d\u044b \u0437\u0430 \u043f\u0440\u043e\u0435\u0437\u0434, \u043c\u0430\u0440\u0448\u0440\u0443\u0442\u0447\u0438\u043a\u0438 \u043f\u0440\u043e\u0441\u0438\u043b\u0438 25, \u043d\u0430\u0434\u043e \u0431\u044b\u043b\u043e \u0441\u0440\u0430\u0437\u0443 \u0438 \u0441\u0442\u0430\u0432\u0438\u0442\u044c 25, \u0447\u0435\u043c \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u0442\u0440\u0430\u0432\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043b\u044e\u0434\u0435\u0439 \u043f\u043e\u0434\u043d\u044f\u0442\u0438\u0435\u043c \u0446\u0435\u043d. \u0422\u043e\u0433\u0434\u0430 \u0431\u044b \u043f\u043e\u043c\u0430\u0442\u0435\u0440\u0438\u043b\u0438\u0441\u044c \u0438 \u0443\u0436\u0435 \u043f\u0440\u0438\u0432\u044b\u043a\u043b\u0438. \u0418 \u0446\u0435\u043d\u044b \u043f\u043e 100 \u0440\u0430\u0437 \u0431\u044b \u043d\u0435 \u043f\u043e\u0434\u043d\u0438\u043c\u0430\u043b\u0438. \u042f\u0441\u0435\u043d \u043f\u0435\u043d\u044c \u0433\u0430\u0437\u0435\u043b\u0438\u0441\u0442\u044b \u0431\u0443\u0434\u0443\u0442 \u0433\u043d\u0443\u0442\u044c \u0441\u0432\u043e\u044e \u043b\u0438\u043d\u0438\u044e, \u043d\u0435 \u043c\u044b\u0442\u044c\u0435\u043c, \u0442\u0430\u043a \u043a\u0430\u0442\u0430\u043d\u044c\u0435\u043c
p464
aV\u0412\u0438\u0434 \u0443 \u0432\u0441\u0435\u0445, \u043a\u0440\u043e\u043c\u0435 \u0412\u0430\u0440\u043d\u0430\u0432\u0441\u043a\u043e\u0433\u043e \u0438 \u041d\u0430\u0437\u0430\u0440\u043e\u0432\u0430, \u043a\u0430\u043a\u043e\u0439 \u0442\u043e \u043d\u0435\u043e\u043f\u0440\u044f\u0442\u043d\u044b\u0439. "\u041f\u043e\u043c\u044f\u0442\u044b\u0435" \u043a\u0430\u043a\u0438\u0435 \u0442\u043e \u0432\u0441\u0435, \u0443\u043b\u044b\u0431\u043a\u0438 \u043d\u0430\u0442\u044f\u043d\u0443\u0442\u044b\u0435\u2026
p465
aV\u041d\u0435\u0442, \u043d\u0435 \u0441\u043c\u043e\u0436\u0435\u0448\u044c. \u0422\u0430\u043a\u0438\u0445 \u043d\u0435\u0433\u0440\u0430\u043c\u043e\u0442\u043d\u044b\u0445 \u043e\u0441\u043a\u043e\u043f\u043b\u044f\u0442\u044c \u043d\u0430\u0434\u043e, \u0447\u0442\u043e\u0431\u044b \u043d\u0435 \u043f\u043b\u043e\u0434\u0438\u043b\u0438 \u0441\u0435\u0431\u0435 \u043f\u043e\u0434\u043e\u0431\u043d\u044b\u0445, \u0443\u0449\u0435\u0440\u0431\u043d\u044b\u0445 \u0433\u0435\u043d\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438.
p466
aV\u041b\u0438\u0445\u0430\u043d, \u0441\u043e\u0433\u043b\u0430\u0441\u043d\u0430 \u0432 \u043e\u0442\u0441\u0442\u0430\u0432\u043a\u0443, \u043f\u043e\u043a\u0430 \u0432\u0435\u0441\u044c \u041e\u0442 \u043d\u0435 \u0440\u0430\u0437\u0432\u0430\u043b\u0438\u043b \u0438 \u043d\u0430 \u043a\u043e\u043c\u043c\u0435\u0440\u0447\u0435\u0441\u043a\u0438\u0435 \u043f\u0435\u0440\u0435\u0432\u043e\u0437\u043a\u0438 \u0432\u0435\u0441\u044c \u0433\u043e\u0440\u043e\u0434 \u043d\u0435 \u043f\u0435\u0440\u0435\u0432\u0435\u043b \u0438 \u0433\u0430\u0437\u0435\u043b\u0438 \u0441 \u043e\u043c\u0441\u043a\u0438\u0445 \u0443\u043b\u0438\u0446 \u0443\u0431\u0440\u0430\u0442\u044c, \u0430 \u043d\u0435 \u043c\u0435\u043d\u044f\u0442\u044c \u005c"\u0442\u0438\u043f\u0430 \u043d\u0430 \u0431\u043e\u043b\u0435\u0435 \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435\u005c".
p467
aV\u041d\u0430\u043a\u043e\u043d\u0435\u0446 \u043d\u0430\u0448\u0451\u043b\u0441\u044f \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0441\u0431\u0440\u043e\u0441\u0438\u043b \u043f\u043e\u0434 \u043e\u0442\u043a\u043e\u0441 \u0432\u0441\u044e \u0436.\u0434.\u0448\u0443\u0448\u0435\u0440\u0443.\u0416\u0430\u043b\u044c, \u042f\u043a\u0443\u043d\u0438\u043d\u0430 \u0432 \u043c\u043e\u0442\u0440\u0438\u0441\u0435 \u043d\u0435\u0431\u044b\u043b\u043e.
p468
aV--- \u0414\u0430 \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u0438\u0437\u043c\u0435\u043d\u0438\u043b\u043e\u0441\u044c, "\u0446\u0438\u0440\u043a \u0443\u0435\u0445\u0430\u043b, \u0430 \u043a\u043b\u043e\u0443\u043d\u044b \u043e\u0441\u0442\u0430\u043b\u0438\u0441\u044c"))) \u0432\u043e\u0442 \u0433\u043b\u044f\u043d\u044c\u0442\u0435: http://www.youtube.com/watch? v=n8mY6Mah1KE
p469
aV\u041e\u043c\u0441\u043a\u0430\u044f \u043e\u0431\u043b\u0430\u0441\u0442\u044c \u0443\u0436\u0435 \u0434\u0430\u0432\u043d\u043e \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442 \u043f\u0435\u0440\u0432\u043e\u0435 \u043c\u0435\u0441\u0442\u043e \u043f\u043e \u0432\u0442\u0438\u0440\u0430\u043d\u0438\u044e \u043e\u0447\u043a\u043e\u0432 \u0438 \u0432\u0435\u0448\u0430\u043d\u0438\u044e \u043b\u0430\u043f\u0448\u0438 \u043d\u0430 \u0443\u0448\u0438.
p470
aV\u0413\u0443\u0441\u0435\u043d\u0438\u0446\u0430 15 \u0441\u043c \u0432 \u0434\u043b\u0438\u043d\u0443 (\u0440\u0430\u0437\u043c\u0435\u0440 \u043c\u0443\u0436\u0441\u043a\u043e\u0439 \u043b\u0430\u0434\u043e\u043d\u0438) \u0432 \u0441\u0438\u0431\u0438\u0440\u0441\u043a\u043e\u043c \u043b\u0435\u0441\u0443- \u044d\u0442\u043e \u043a\u0440\u0443\u0442\u043e! \u0413\u043e\u0442\u043e\u0432\u044b\u0439 \u0441\u044e\u0436\u0435\u0442 \u0434\u043b\u044f \u0444\u0430\u043d\u0442\u0430\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0440\u043e\u043c\u0430\u043d\u0430 "\u0410\u0442\u0430\u043a\u0443\u044e\u0442 \u041b\u043e\u0445\u043c\u0430\u0442\u044b\u0435".
p471
aV\u0414\u043e\u0447\u043a\u0443 \u0438\u043b\u0438 \u043f\u043e\u0447\u043a\u0443?
p472
aV\u041a\u0430\u043a-\u0442\u043e \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043d\u043e \u0435\u0433\u043e \u043e\u0431\u043e\u0437\u0432\u0430\u043b\u0438, \u043d\u0430\u0434\u043e \u043f\u0438\u0441\u0430\u0442\u044c \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a-\u044d\u043a\u0441\u0433\u0438\u0431\u0438\u0446\u0438\u043e\u043d\u0438\u0441\u0442 \u0438\u043d\u0430\u0447\u0435 \u044d\u0442\u043e\u0433\u043e \u0442\u043e\u0432\u0430\u0440\u0438\u0449\u0447\u0430 \u0438 \u043d\u0435 \u043d\u0430\u0437\u043e\u0432\u0435\u0448\u044c
p473
aV\u0435\u0434\u0440\u043e\u0441\u043d\u044b\u0439 \u043f\u0440\u0430\u0437\u0434\u043d\u0438\u043a \u043f\u0443\u0441\u0442\u044c \u0447\u0438\u043d\u0443\u0448\u0438 \u0438 \u043f\u0440\u0430\u0437\u0434\u043d\u0443\u044e\u0442. \u043c\u043d\u0435 \u0441 \u043d\u0438\u043c\u0438 \u0435\u0434\u0438\u043d\u044f\u0442\u044c\u0441\u044f \u043d\u0435 \u0445\u043e\u0446\u0446\u0430. \u0434\u043b\u044f \u043d\u0430\u0440\u043e\u0434\u0430 \u043e\u043d \u043a\u0430\u043a \u0431\u044b\u043b \u0432\u044b\u0445\u043e\u0434\u043d\u043e\u0439 \u0438 \u043d\u0435 \u0431\u043e\u043b\u0435\u0435, \u0442\u0430\u043a \u0438\u043c \u0438 \u043e\u0441\u0442\u0430\u043d\u0435\u0442\u0441\u044f\u2026
p474
aV\u0422\u0430\u043c \u043f\u043e \u043c\u043e\u0435\u043c\u0443 \u0432\u043f\u0435\u0440\u0435\u0434\u0438 \u0432\u0441\u0435\u0445 \u0410\u0443\u0434\u0438 \u0447\u0435\u0440\u043d\u0430\u044f \u0441\u0442\u043e\u044f\u043b\u0430? 
p475
aV\u042d\u0442\u043e \u0442\u0430\u043a \u0441\u0435\u0439\u0447\u0430\u0441 \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f?
p476
aV\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440, \u043d\u0435 \u0441\u0442\u043e\u043b\u044c \u0443\u0436 \u0432\u0430\u0436\u043d\u043e \u2014 \u0441\u044a\u0435\u0441\u0442\u044c \u0438\u043b\u0438 \u043a\u0443\u043f\u0438\u0442\u044c. \u0412\u0430\u0436\u043d\u043e, \u0447\u0442\u043e \u0441\u0438\u044f \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0430 \u043e\u0434\u043d\u043e\u0437\u043d\u0430\u0447\u043d\u043e \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0443\u0435\u0442 \u043e \u0442\u043e\u043c, \u0447\u0442\u043e \u043d\u0430\u0441\u0435\u043b\u0435\u043d\u0438\u0435 \u043e\u043c\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0432\u0441\u0435 \u043c\u0435\u043d\u044c\u0448\u0435 \u043f\u0440\u0435\u0434\u043f\u043e\u0447\u0438\u0442\u0430\u0435\u0442 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u044b \u0441\u0432\u043e\u0435\u0433\u043e \u0440\u0435\u0433\u0438\u043e\u043d\u0430 \u0438\u043d\u043e\u0433\u043e\u0440\u043e\u0434\u043d\u0438\u043c \u0438 \u0438\u043c\u043f\u043e\u0440\u0442\u043d\u044b\u043c. \u0410 \u0412\u044b \u0434\u0443\u043c\u0430\u0435\u0442\u0435 \u0437\u0440\u044f \u0447\u0442\u043e \u043c\u0435\u0441\u0442\u043d\u044b\u0435 \u0431\u0430\u0440\u044b\u0433\u0438 \u043d\u0430 \u0444\u0435\u0434\u0435\u0440\u0430\u043b\u044c\u043d\u044b\u0435 \u0441\u0435\u0442\u0438 \u0442\u0430\u043a \u043a\u0440\u0443\u0442\u043e \u0432\u0437\u044a\u0435\u043b\u0438\u0441\u044c?
p477
aV\u0412\u043e\u0442 \u0438\u043c\u0435\u043d\u043d\u043e, \u0447\u0442\u043e \u0432 \u043a\u043e\u043f\u0435\u0439\u043a\u0443. \u041f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u0438 \u0437\u0430\u043a\u0430\u0437\u044b\u0432\u0430\u044e\u0442 \u0447\u0435\u0440\u0435\u0437 \u042e\u043d\u0438\u0441-\u041b\u0430\u0434\u0443. \u0422\u0430\u043c \u0445\u043e\u0442\u044c \u0438 \u0434\u043e\u0440\u043e\u0436\u0435, \u043d\u043e \u043c\u0430\u0448\u0438\u043d\u044b \u043f\u0440\u043e\u0432\u0435\u0440\u0435\u043d\u043d\u044b\u0435, \u0445\u043b\u0430\u043c \u043e\u043d\u0438 \u043d\u0435 \u0434\u043e\u043f\u0443\u0441\u043a\u0430\u044e\u0442.
p478
aV\u0421\u043f\u0430\u0441\u0438\u0431\u043e \u0433\u043b\u0430\u0432\u043d\u043e\u043c\u0443 \u043f\u0440\u0438\u0441\u0442\u0430\u0432\u0443, \u0447\u0442\u043e \u043d\u0430\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0433\u043b\u0443\u043f\u044b\u0445 \u0434\u0435\u0432\u0443\u0448\u0435\u043a, \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043f\u044b\u0442\u0430\u044e\u0442\u0441\u044f \u043e\u0431\u0434\u0443\u0440\u0438\u0442\u044c \u0442\u0430\u0440\u0430\u043a\u0430\u043d\u044b-\u0434\u043e\u043b\u0436\u043d\u0438\u043a\u0438. \u0423 \u043c\u0435\u043d\u044f 3 \u0432\u043d\u0443\u0447\u043a\u0438, \u043d\u0435 \u043f\u0440\u0438\u0432\u0435\u0434\u0438 \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u043f\u043e\u043f\u0430\u0434\u0443\u0442\u0441\u044f \u043d\u0430 \u0443\u0434\u043e\u0447\u043a\u0443 \u0442\u0430\u043a\u043e\u043c\u0443...
p479
aVC \u043a\u0430\u0436\u0434\u044b\u043c \u0434\u043d\u0435\u043c \u0432\u0441\u0451 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0435\u0435 \u0438 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0435\u0435. \u0422\u043e\u043b\u044c\u043a\u043e \u0432 \u043e\u0434\u043d\u043e\u043c \u044f \u0443\u0432\u0435\u0440\u0435\u043d - \u0440\u043e\u0441\u0441\u0438\u044f \u043e\u0442\u0440\u0435\u0430\u0433\u0438\u0440\u0443\u0435\u0442 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u043e \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u0438\u043c \u0432\u043e\u0437\u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0435\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u0442\u043e\u0433\u0434\u0430, \u043a\u043e\u0433\u0434\u0430 \u043d\u0430 \u043d\u0435\u0451 \u043a\u0438\u043d\u0443\u0442\u044c\u0441\u044f \u0441 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u043c\u0438 \u0444\u0438\u0437\u0438\u0447\u0438\u0441\u043a\u0438\u043c\u0438 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044f\u043c\u0438. \u0418 \u044d\u0442\u043e \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u0432\u0438\u043b\u044c\u043d\u043e.  \u0410 \u043f\u043e\u043a\u0430 \u0442\u0430\u043a \u0438 \u0431\u0443\u0434\u0435\u0442 \u0432\u043e\u0442 \u044d\u0442\u0438 \u0442\u0443\u0434\u0430 \u0441\u044e\u0434\u0430, \u0442\u044b \u043c\u043d\u0435 \u044f \u0442\u0435\u0431\u0435. \u041d\u0443 \u0438 \u0441\u0430\u043c\u043e\u0435 \u0433\u043b\u0430\u0432\u043d\u043e\u0435, \u0447\u0442\u043e \u0431\u044b \u0442\u0435 \u043c\u0440\u0430\u0437\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0434\u043e\u0431\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u043d\u0430\u0447\u0430\u043b\u0430 \u0432\u043e\u0439\u043d\u044b \u043d\u0435 \u0441\u043c\u043e\u0433\u043b\u0438 \u0440\u0435\u0430\u043b\u0438\u0437\u043e\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u0438 \u043f\u043b\u0430\u043d\u044b. \u0414\u0430\u0439 \u0411\u043e\u0433 \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u043b \u043c\u0438\u0440, \u043f\u0443\u0441\u0442\u044c \u0438 \u0441 \u0442\u0430\u043a\u0438\u043c\u0438 \u043f\u043e\u043b\u0438\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u0442\u044b\u0447\u043a\u0430\u043c\u0438 \u0441\u043e \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u044d\u0442\u043e\u0439 \u0443\u043a\u0440\u0430\u0438\u043d\u044b \u0438 \u0435\u0451 \u043f\u0440\u0438\u043b\u0438\u043f\u0430\u043b
p480
aV\u041c\u0438\u043c\u043e \u041d\u0435 \u043f\u0440\u043e\u0445\u043e\u0434\u0438\u043b\u0443:, \u0430 \u044f \u0438 \u043d\u0435 \u0441\u043e\u0431\u0438\u0440\u0430\u043b\u0441\u044f \u0441\u043f\u043e\u0440\u0438\u0442\u044c \u043d\u0438 \u043e \u0447\u0435\u043c))) \u0412\u044b \u0436 \u0440\u0430\u0441\u0441\u0443\u0436\u0434\u0430\u0435\u0442\u0435 \u2014 \u043d\u0438 \u043e \u0447\u0435\u043c!.. \u0417\u043d\u0430\u0447\u0438\u043c\u043e\u0441\u0442\u044c \u043c\u0435\u0442\u043e\u0434\u0438\u0447\u043a\u0438 \u2014 \u044d\u0442\u043e \u043d\u0435 \u0442\u043e, \u043e \u0447\u0435\u043c \u0441\u0442\u043e\u0438\u0442 \u0434\u0438\u0441\u043a\u0443\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c. \u041e\u043d\u0430 \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0438 \u0437\u043d\u0430\u0447\u0438\u043c\u0430, \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u0438 \u043d\u0435\u0442, \u2014 \u0432\u0441\u0435 \u0437\u0430\u0432\u0438\u0441\u0438\u0442 \u043e\u0442 \u0441\u0442\u0435\u043f\u0435\u043d\u0438 \u0438\u043d\u0434\u0438\u0432\u0438\u0434\u0443\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u0430 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f \u0438 \u043c\u0435\u0442\u043e\u0434\u043e\u0432 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f, \u043e\u0442 \u0441\u0442\u0435\u043f\u0435\u043d\u0438 \u043f\u043e\u0434\u0434\u0430\u0432\u0430\u0435\u043c\u043e\u0441\u0442\u0438 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0430 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c\u0438\u0437\u0430\u0446\u0438\u0438. \u0415\u0441\u0442\u044c \u0442\u0430\u043a\u0438\u0435 \u0434\u0438\u0441\u0446\u0438\u043f\u043b\u0438\u043d\u044b, \u043f\u043e \u0447\u0430\u0441\u0442\u0438 \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u0430, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0437\u043d\u0430\u0447\u0438\u043c\u043e\u0441\u0442\u044c \u04
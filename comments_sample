434\u0440\u043e\u0443\u0437\u0435\u043b. \u041f\u043e\u0434\u0440\u043e\u0431\u043d\u0435\u0435 \u043e \u041c\u043e\u0441\u0442\u043e\u0432\u0438\u043a\u0435 \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u043e\u0447\u0438\u0442\u0430\u0442\u044c \u043f\u043e \u0441\u0441\u044b\u043b\u043a\u0435.
p435
aV\u0412\u0440\u044f\u0434 \u043b\u0438 \u0442\u0430\u043c \u0431\u044e\u0434\u0436\u0435\u0442\u043d\u044b\u0435 \u0434\u0435\u043d\u044c\u0433\u0438, \u043b\u0438\u0431\u043e \u0441\u043e\u0432\u0441\u0435\u043c \u043c\u0438\u043d\u0438\u043c\u0430\u043b\u044c\u043d\u044b\u0435, \u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u0434\u0430\u043d\u043d\u044b\u0439 \u0444\u0438\u043b\u044c\u043c \u043e\u0447\u0435\u043d\u044c \u043d\u0438\u0437\u043a\u043e\u043f\u0440\u043e\u0431\u043d\u044b\u0439 \u0438 \u043b\u0436\u0438\u0432\u044b\u0439 \u0438 \u0432 \u043f\u043b\u0430\u043d\u0435 \u0442\u0435\u043c\u044b \u0438 \u0432 \u043f\u043b\u0430\u043d\u0435 \u0435\u0451 \u043f\u043e\u0434\u0430\u0447\u0438, \u043a \u0442\u043e\u0436\u0435 \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u043e \u0431\u0435\u0437\u0434\u0430\u0440\u043d\u044b\u0435 \u0430\u043a\u0442\u0435\u0440\u044b \u0440\u0430\u0437\u044b\u0433\u0440\u0430\u0432\u0448\u0438\u0435 \u043d\u0430\u0438\u0432\u043d\u044b\u0439 \u0431\u0440\u0435\u0434. \u0423 \u043d\u0430\u0441 \u0435\u0441\u0442\u044c \u043d\u0430\u043c\u043d\u043e\u0433\u043e \u0431\u043e\u043b\u0435\u0435 \u0442\u0430\u043b\u0430\u043d\u0442\u043b\u0438\u0432\u044b\u0435 \u0440\u0435\u0431\u044f\u0442\u0430, \u0447\u0435\u043c \u044d\u0442\u0438 \u043d\u043e\u0432\u043e\u044f\u0432\u043b\u0435\u043d\u043d\u044b\u0435 "\u0442\u0432\u043e\u0440\u0446\u044b" \u0441\u043e \u0441\u0442\u0443\u0434\u0438\u0438 \u0410\u0440\u0442 \u043c\u0435\u0434\u0438\u0430.
p436
aV\u0416\u0438\u0440\u0438\u043d\u043e\u0432\u0441\u043a\u0438\u0439 \u043d\u0435 \u043f\u043e\u043d\u044f\u043b, \u0447\u0442\u043e \u0417\u0435\u043b\u0438\u043d\u0441\u043a\u0438\u0439 \u2014 \u0437\u0430\u0441\u043b\u0443\u0436\u0435\u043d\u043d\u044b\u0439 \u044e\u0440\u0438\u0441\u0442 \u041e\u043c\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438. \u0410 \u0441 \u044e\u0440\u0438\u0441\u0442\u043e\u0432 \u0432 \u041e\u043c\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u043a\u0430\u043a\u043e\u0439 \u0441\u043f\u0440\u043e\u0441?
p437
aV\u042f \u0442\u0435\u0431\u0435 \u0432 \u0440\u043e\u0442 \u0438 \u0432 \u043e\u043f\u0443 \u0432\u0433\u043e\u043d\u044f\u044e,\u0448\u0430\u043b\u0430\u0448\u043e\u0432\u043a\u0430 \u0442\u0435\u043f\u043b\u043e\u0442\u0440\u0430\u0441\u043d\u0430\u044f!
p438
aV\u0421\u043e\u043f\u043b\u0438\u0432\u044b\u0439 \u0438 \u043d\u0435 \u0440\u0430\u0437\u0443\u043c\u043d\u044b\u0439 \u0441\u0442\u0443\u0434\u0435\u043d\u0442\u0438\u0448\u043a\u0430\u2026\u0438\u0441\u043f\u043e\u0440\u0442\u0438\u0432\u0448\u0438\u0439 \u0441\u0435\u0431\u0435 \u0436\u0438\u0437\u043d\u044c..\u0432\u043e\u0442 \u043a\u0442\u043e \u043e\u043d.
p439
aV\u0442\u0430\u043a \u043a\u043e\u043b\u043e\u0440\u0430\u0434\u0430\u043c \u0438 \u043d\u0430\u0434\u043e, \u0435\u0449\u0451 \u0443 \u043d\u0438\u0445 \u0441\u043e\u0441\u043a\u043e\u0431 \u0438 1\u0440\u0430\u0434\u0443\u0436\u043a\u0443 \u0437\u0430\u0431\u0438\u0440\u0430\u0439\u0442\u0435 \u0438 \u043f\u0443\u0441\u0442\u044c \u0442\u0430\u043c \u0436\u0438\u0432\u0443\u0442 \u0443 \u0441\u0435\u0431\u044f \u0437\u0430 \u0436\u0435\u043b\u0435\u0437\u043d\u044b\u043c \u0437\u0430\u043d\u0430\u0432\u0435\u0441\u043e\u043c \u0438 \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u044b\u043c \u043b\u044e\u0434\u044f\u043c \u0436\u0438\u0441\u0442\u044c \u043d\u0435 \u043f\u043e\u0440\u0442\u044f\u0442
p440
aV\u0421\u0442\u0430\u0442\u044c\u044e \u0442\u043e \u0447\u0438\u0442\u0430\u043b\u0438? \u0421\u043c\u044b\u0441\u043b \u0431\u043e\u043b\u044c\u043d\u044b\u043c \u0440\u0430\u043a\u043e\u043c \u043f\u043e\u043c\u043e\u0447\u044c, \u043c\u043e\u043b \u0435\u0441\u043b\u0438 \u043d\u0435 \u0441\u0434\u0435\u043b\u0430\u0435\u0448\u044c \u0442\u0430\u043a \u0436\u0435 \u0442\u043e \u043f\u043b\u0430\u0442\u0438 \u0432 \u0444\u043e\u043d\u0434 \u043f\u043e\u043c\u043e\u0449\u0438.
p441
aV\u0412\u043e\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u0430\u0434\u043e \u0431\u044b\u043b\u043e \u0431\u043e\u043b\u044c\u0448\u0435, \u0442\u043e\u0433\u0434\u0430 \u0431 \u041d\u0415 \u0421\u0423\u0414\u0418\u041b\u0418\u2026
p442
aV\u041d\u0430\u0434\u0435\u044e\u0441\u044c \u043f\u0435\u0440\u0435\u0441\u0435\u043b\u0435\u043d\u0446\u044b \u0438\u0437 \u0423\u043a\u0440\u0430\u0438\u043d\u044b \u0443\u0432\u0438\u0434\u044f\u0442 \u0443\u0436\u0430\u0441\u043d\u0443\u044e \u043e\u043c\u0441\u043a\u0443\u044e \u043f\u043e\u0433\u043e\u0434\u0443 \u0438 \u0432\u0435\u0440\u043d\u0443\u0442\u044c\u0441\u044f \u043d\u0430 \u0440\u043e\u0434\u0438\u043d\u0443.
p443
aV\u0418\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e, \u0447\u0442\u043e \u0441\u043a\u0430\u0436\u0443\u0442 \u0412\u0430\u0441\u0438\u043b\u0438\u0439 \u0438 \u0435\u0433\u043e \u0433\u0440\u0443\u043f\u043f\u0430 \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0438, \u043a\u043e\u0433\u0434\u0430 \u043f\u043e\u0439\u043c\u0443\u0442, \u0447\u0442\u043e \u0435\u0441\u0442\u044c \u043f\u043e\u043a\u0443\u0448\u0435\u043d\u0438\u0435 \u043d\u0430 \u0443\u0431\u0438\u0439\u0441\u0442\u0432\u043e, \u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u043d\u043e\u0435 \u0433\u0440\u0443\u043f\u043f\u043e\u0439 \u043b\u0438\u0446 \u043f\u043e \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u043c\u0443 \u0441\u0433\u043e\u0432\u043e\u0440\u0443, \u0438 \u0447\u0438\u0441\u0442\u043e\u0441\u0435\u0440\u0434\u0435\u0447\u043d\u043e\u0435 \u043f\u0440\u0438\u0437\u043d\u0430\u043d\u0438\u0435, \u043d\u043e \u043d\u0435 \u044f\u0432\u043a\u0430 \u0441 \u043f\u043e\u0432\u0438\u043d\u043d\u043e\u0439? \u0414\u0443\u043c\u0430\u044e, \u0447\u0442\u043e \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u044f\u0442\u0438\u044f \u0443\u0436\u0435 \u0441\u0435\u0433\u043e\u0434\u043d\u044f \u0443\u0432\u043e\u043b\u0438\u0442 \u0432\u0441\u044e \u0441\u043c\u0435\u043d\u0443 \u0438 \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a\u0430 \u043e\u0445\u0440\u0430\u043d\u044b, \u0430 \u0441 \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u043c\u0438 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u0442 \u0431\u0435\u0441\u0435\u0434\u0443. \u041d\u0443, \u0430 \u0412\u0430\u0441\u0438\u043b\u0438\u044e \u043f\u0440\u0435\u0434\u0441\u0442\u043e\u044f\u0442 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043b\u0435\u0442 \u043f\u043e\u0434 \u043e\u0445\u0440\u0430\u043d\u043e\u0439 \u0441\u043f\u0430\u0441\u0435\u043d\u043d\u044b\u0445 \u0438\u043c \u0441\u043e\u0431\u0430\u0447\u0435\u043a.
p444
aV\u0421\u0441\u044b\u043a\u043b\u043e \u0442\u044b \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442\u043e\u0432\u0441\u043a\u043e\u0435 \u0438 \u043d\u0435 \u0431\u043e\u043b\u0435\u0435.\u041c\u0430\u043c\u0435\u043d\u044c\u043a\u0438\u043d \u0441\u044b\u043d\u043e\u0447\u0435\u043a \u0438\u0434\u0438 \u0437\u0430 \u043f\u0430\u043b\u0451\u043d\u043e\u0439 \u0432\u043e\u0434\u043a\u043e\u0439 \u0441\u0432\u043e\u0435\u0439 \u043c\u0430\u043c\u0430\u0448\u0435,\u0447\u0442\u043e\u0431 \u043e\u043d\u0430 \u043d\u0430\u0436\u0440\u0430\u043b\u0430\u0441\u044c \u043a\u0430\u043a \u0441\u0432\u0438\u043d\u044c\u044f.\u041e\u043d\u0430 \u044d\u0442\u043e \u043b\u044e\u0431\u0438\u0442.
p445
aV\u041d\u0438\u043a\u043e\u043b\u0430\u0439, \u0430 \u0440\u0430\u0437\u0432\u0435 \u041a\u0430\u0437\u0438\u043c\u0438\u0440\u043e\u0432 \u0433\u043e\u0432\u043e\u0440\u0438\u043b \u043e \u043f\u0440\u0435\u0434\u0435\u043b\u0435??? \u041d\u0423 \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442 \u0438 \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442. \u0418 \u043d\u0435 \u043d\u0430\u0434\u043e \u0442\u0443\u0442 \u0441\u043b\u044e\u043d\u043e\u0439 \u0431\u0440\u044b\u0437\u0433\u0430\u0442\u044c - \u0433\u043e\u0440\u043e\u0434, \u043a\u0430\u043a \u0434\u043e\u043c, \u0438\u043d\u0430\u044f \u0445\u043e\u0437\u044f\u0439\u043a\u0430 \u0442\u043e\u043a \u043f\u0435\u0440\u0435\u0434 \u043f\u0440\u0430\u0437\u0434\u043d\u0438\u043a\u0430\u043c\u0438 \u0438 \u0443\u0431\u0438\u0440\u0430\u0435\u0442\u0441\u044f.
p446
aV\u041e\u043b\u044c\u0433\u0430, \u044d\u0442\u043e \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u0430\u0436\u0435\u0432\u044b\u0439 \u043c\u043e\u0433 \u0442\u0430\u043a \u043d\u0430\u0432\u043e\u043d\u044f\u0442\u044c! \u0425\u043e\u0442\u044c \u043e\u043d\u0438 \u0438 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e \u043e\u0442\u043d\u0435\u043a\u0438\u0432\u0430\u044e\u0442\u0441\u044f!
p447
aV\u0410\u043d\u044f, \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u0435\u0448\u0435\u0445\u043e\u0434\u044b \u043f\u0440\u044f\u043c \u0433\u043e\u0442\u043e\u0432\u044b \u0437\u0430\u0441\u043d\u0443\u0442\u044c \u043d\u0430 \u0434\u043e\u0440\u043e\u0433\u0435. \u0422\u043e\u0436\u0435 \u043d\u0435 \u0435\u0441\u0442\u044c \u0445\u043e\u0440\u043e\u0448\u043e.
p448
aV\u0420\u043e\u0441\u0442\u0435\u043b\u0435\u043a\u043e\u043c \u043c\u0430\u0441\u0442 \u0434\u0430\u0439! \u0418\u043d\u0430\u0447\u0435 \u0438 \u043d\u0435 \u0441\u043a\u0430\u0436\u0435\u0448\u044c\u2026 \u041a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0443\u0441\u043b\u0443\u0433 \u043e\u0442\u0432\u0440\u0430\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0435, \u0442\u0435\u0445\u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0430 \u043d\u0435\u0432\u043c\u0435\u043d\u044f\u0435\u043c\u0430\u044f, \u0430\u0440\u0435\u043d\u0434\u0443\u044e \u0432\u044b\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0439 IP \u2014 \u043e\u043d \u0432 \u043c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u043e\u043c \u0431\u043b\u0435\u043a\u043b\u0438\u0441\u0442\u0435 => \u043e \u0441\u0432\u043e\u0435\u043c \u043f\u043e\u0447\u0442\u043e\u0432\u043e\u043c \u0441\u0435\u0440\u0432\u0435\u0440\u0435 \u0438 \u043d\u0435 \u043c\u0435\u0447\u0442\u0430\u0439\u0442\u0435. \u0414\u043e \u0430\u0434\u043c\u0438\u043d\u043e\u0432 \u043f\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430\u043c, \u0441\u043e\u043e\u0431\u0449\u0435\u043d\u043d\u044b\u043c \u0432 \u0442\u0435\u0445\u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0435, \u0434\u043e\u0437\u0432\u043e\u043d\u0438\u0442\u044c\u0441\u044f \u0443\u0434\u0430\u043b\u043e\u0441\u044c \u0435\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0439 \u0440\u0430\u0437. \u041f\u043e\u043f\u0440\u043e\u0441\u0438\u043b \u043f\u0440\u043e\u043f\u0438\u0441\u0430\u0442\u044c \u0441 \u043c\u043e\u0435\u043c\u0443 IP \u043e\u0431\u0440\u0430\u0442\u043d\u044b\u0439 \u0414\u041d\u0421 (\u0447\u0442\u043e\u0431\u044b \u043f\u043e\u0447\u0442\u043e\u0432\u044b\u0435 \u0441\u0435\u0440\u0432\u0438\u0441\u044b \u043d\u0435 \u043e\u0442\u043f\u0438\u043d\u044b\u0432\u0430\u043b\u0438 \u043f\u043e\u0447\u0442\u0443 \u0441 \u043c\u043e\u0435\u0433\u043e \u043f\u043e\u0447\u0442\u043e\u0432\u043e\u0433\u043e \u0441\u0435\u0440\u0432\u0435\u0440\u0430) \u2014 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u043e\u0432\u0430\u043b\u0438 \u043f\u0440\u043e\u0441\u044c\u0431\u0443 \u043e\u0442\u043f\u0440\u0430\u0432\u0438\u0442\u044c \u043d\u0430 e-mal \u0430\u0434\u043c\u0438\u043d\u0430. \u0414\u0443\u0431\u043b\u0438\u0440\u043e\u0432\u0430\u043b \u0440\u0430\u0437 5 \u0432 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u043c\u0435\u0441\u044f\u0446\u0430 \u2014 \u043d\u0438 \u043e\u0434\u043d\u043e\u0433\u043e \u043e\u0442\u0432\u0435\u0442\u0430 \u043d\u0435 \u043f\u043e\u043b\u0443\u0447\u0438\u043b. \u0420\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u044b\u0435 \u0441\u0431\u043e\u0438, \u043f\u043e\u043b\u043e\u043c\u043a\u0438, \u043e\u0442\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044f, \u0442\u043e\u0440\u043c\u043e\u0437\u0430 \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u043b\u0443\u0436\u0431 \u0438 \u043f\u043e\u043b\u043d\u0430\u044f \u043d\u0435\u043a\u043e\u043c\u043f\u0435\u0442\u0435\u043d\u0442\u043d\u043e\u0441\u0442\u044c! \u041a \u0441\u043e\u0436\u0430\u043b\u0435\u043d\u0438\u044e \u043d\u0435\u0442 \u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u0441\u043c\u0435\u043d\u0438\u0442\u044c \u043f\u0440\u043e\u0432\u0430\u0439\u0434\u0435\u0440\u0430\u2026:( \u0417\u042b: \u041d\u0438\u043a\u043e\u043c\u0443 \u043d\u0435 \u043f\u043e\u0441\u043e\u0432\u0435\u0442\u0443\u044e \u044d\u0442\u0443 \u0448\u0430\u0440\u0430\u0436\u043a\u0438\u043d\u0443 \u043a\u043e\u043d\u0442\u043e\u0440\u0443! \u041f\u043e\u0437\u043e\u0440 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u0443 \u0420\u043e\u0441\u0442\u0435\u043b\u0435\u043a\u043e\u043c\u0430!
p449
aV\u041d\u0435\u043f\u043e\u043d\u044f\u0442\u043d\u043e, \u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e \u043d\u0435 \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0449\u0438\u0439 \u0442\u043e\u0432\u0430\u0440\u0438\u0449 \u0438\u043b\u0438 \u043d\u0435\u0442?
p450
aV\u0436\u0435\u043b\u0430\u044e \u0442\u0435\u0431\u0435 \u0441\u0442\u0430\u0442\u044c \u0440\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u043c.

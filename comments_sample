aV\u0412 \u0435\u0433\u043e \u0440\u0435\u0437\u044e\u043c\u0435 \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u0432\u0434\u043e\u0445\u043d\u043e\u0432\u043b\u044f\u0435\u0442, \u0447\u0442\u043e \u0435\u043c\u0443 \u005c"\u043d\u0443\u0436\u043d\u044b \u0434\u0435\u043d\u044c\u0433\u0438 \u0438 \u043b\u0438\u0447\u043d\u043e\u0435 \u043f\u0440\u043e\u0441\u0442\u0440\u0430\u043d\u0441\u0442\u0432\u043e \u0434\u043b\u044f \u0435\u0433\u043e \u0438\u043d\u0442\u0435\u043b\u043b\u0435\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0445 \u0431\u0435\u0437\u0443\u043c\u0438\u0439\u005c". \u041d\u0443 \u043d\u0430\u0441\u0447\u0435\u0442 \u0434\u0435\u043d\u0435\u0433 \u043e\u043d \u044f\u0432\u043d\u043e \u043f\u0440\u043e\u0441\u0447\u0438\u0442\u0430\u043b\u0441\u044f, \u042e\u0440\u043a\u043e\u0432\u0430 \u0434\u0435\u043b\u0438\u0442\u044c\u0441\u044f \u043d\u0435 \u043b\u044e\u0431\u0438\u0442. \u0410 \u0432\u043e\u0442 \u043d\u0430\u0441\u0447\u0435\u0442 \u005c"\u0431\u0435\u0437\u0443\u043c\u0438\u0439\u005c"\u2026 \u0415\u0441\u043b\u0438 \u0435\u0433\u043e \u0438 \u0434\u0430\u043b\u044c\u0448\u0435 \u0431\u0443\u0434\u0435\u0442 \u043a\u043e\u043b\u0431\u0430\u0441\u0438\u0442\u044c \u0442\u0430\u043a \u0436\u0435, \u043a\u0430\u043a \u0432 \u043f\u0435\u0440\u0432\u044b\u0435 \u0434\u043d\u0438 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0432 \u0434\u043e\u043b\u0436\u043d\u043e\u0441\u0442\u0438 \u0433\u043b\u0430\u0432\u0440\u0435\u0436\u0430, \u0442\u043e \u0441 \u043f\u043e\u0441\u043b\u0435\u0434\u0441\u0442\u0432\u0438\u044f\u043c\u0438 \u0431\u0435\u043b\u043e\u0439 \u0433\u043e\u0440\u044f\u0447\u043a\u0438 \u0438 \u0435\u0433\u043e \u005c"\u0431\u0435\u0437\u0443\u043c\u0438\u044f\u043c\u0438\u005c" \u0441\u043a\u043e\u0440\u0435\u0435 \u0441\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c\u0441\u044f \u0432\u0440\u0430\u0447\u0438 \u043f\u0441\u0438\u0445\u0443\u0448\u043a\u0438, \u0430 \u043d\u0435 \u043e\u043c\u0441\u043a\u0438\u0439 \u0437\u0440\u0438\u0442\u0435\u043b\u044c. \u0425\u043e\u0442\u044f, \u043c\u043d\u0435 \u043a\u0430\u043a \u0437\u0440\u0438\u0442\u0435\u043b\u044e, \u0447\u0442\u043e-\u0442\u043e \u043d\u0435 \u043e\u0447\u0435\u043d\u044c-\u0442\u043e \u0438 \u0445\u043e\u0442\u0435\u043b\u043e\u0441\u044c \u0431\u044b \u043e\u043a\u0443\u043d\u0443\u0442\u044c\u0441\u044f \u0432 \u0435\u0433\u043e \u0438\u043d\u0442\u0435\u043b\u043b\u0435\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u0443\u044e \u0431\u0435\u0437\u0434\u043d\u0443. \u041a\u0430\u043a-\u0442\u043e \u043d\u0435 \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043e\u0436\u0438\u0434\u0430\u0442\u044c \u043d\u0438\u0447\u0435\u0433\u043e \u0445\u043e\u0440\u043e\u0448\u0435\u0433\u043e \u043e\u0442 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0441\u0432\u043e\u0435 \u0437\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u043e \u0441 \u0430\u043a\u0442\u0435\u0440\u0430\u043c\u0438 \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442 \u0441 \u0437\u0430\u044f\u0432\u043b\u0435\u043d\u0438\u0439, \u0447\u0442\u043e \u043e\u043d \u043b\u0443\u0447\u0448\u0438\u0439 \u0440\u0435\u0436\u0438\u0441\u0441\u0435\u0440 \u0420\u043e\u0441\u0441\u0438\u0438, \u0430 \u0433\u043b\u0430\u0432\u043d\u043e\u0439 \u0431\u043b\u0438\u0436\u0430\u0439\u0448\u0435\u0439 \u0446\u0435\u043b\u044c\u044e \u0441\u0442\u0430\u0432\u0438\u0442 \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0435 \u0434\u0440\u0435\u0441\u0441 \u043a\u043e\u0434\u0430 \u0434\u043b\u044f \u0436\u0435\u043d\u0441\u043a\u043e\u0439 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u044b \u0442\u0440\u0443\u043f\u043f\u044b, \u0432 \u0432\u0438\u0434\u0435 \u043c\u0438\u043d\u0438-\u044e\u0431\u043e\u043a, \u0433\u043b\u0443\u0431\u043e\u043a\u0438\u0445 \u0432\u044b\u0440\u0435\u0437\u043e\u0432 \u0438 \u043f\u0440\u043e\u0447\u0435\u0439 \u0441\u0435\u043a\u0441\u0443\u0430\u043b\u044c\u043d\u043e\u0439 \u0430\u0442\u0440\u0438\u0431\u0443\u0442\u0438\u043a\u0438. \u005c"\u0421\u0435\u043a\u0441\u0443\u0430\u043b\u044c\u043d\u043e \u043e\u0437\u0430\u0431\u043e\u0447\u0435\u043d\u043d\u044b\u0439 \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u0438\u043a-\u043c\u0430\u0440\u0430\u0437\u043c\u0430\u0442\u0438\u043a, \u0447\u0442\u043e \u043c\u043e\u0436\u0435\u0442 \u0431\u044b\u0442\u044c \u043b\u0443\u0447\u0448\u0435 \u0434\u043b\u044f \u0442\u0435\u0430\u0442\u0440\u0430\u005c" \u2014 \u0442\u0430\u043a, \u043d\u0430\u0432\u0435\u0440\u043d\u043e\u0435, \u0434\u0443\u043c\u0430\u043b\u0430 \u042e\u0440\u043a\u043e\u0432\u0430, \u043e\u0446\u0435\u043d\u0438\u0432\u0430\u044f \u043f\u0435\u0440\u0441\u043f\u0435\u043a\u0442\u0438\u0432\u044b \u0442\u0435\u0430\u0442\u0440\u0430 \u0438 \u0435\u0433\u043e \u043a\u043e\u043d\u0446\u0435\u043f\u0446\u0438\u044e\u2026 \u0413\u0440\u0443\u0441\u0442\u043d\u043e, \u0434\u0435\u0432\u0443\u0448\u043a\u0438 \u0438\u043c\u0435\u0442\u044c \u0432 \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a\u0430\u0445 \u0441\u0442\u0430\u0440\u043e\u0433\u043e \u0441\u0430\u0442\u0438\u0440\u0430. \u0418 \u0435\u0441\u043b\u0438 \u043b\u044e\u0431\u0438\u043c\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u0440\u0435\u0436\u0438\u0441\u0441\u0435\u0440\u0430, \u0435\u0433\u043e \u0442\u0430\u043a \u0441\u043a\u0430\u0437\u0430\u0442\u044c \u043c\u0443\u0437\u043e\u0439 (\u0438 \u0444\u0440\u0430\u0437\u043e\u0439) \u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u005c"\u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u0441\u0438\u0441\u044c\u043a\u0438\u005c", \u0442\u043e \u0438 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0441\u0442\u0440\u0430\u0448\u043d\u043e, \u0434\u043e \u043a\u0430\u043a\u0438\u0445 \u005c"\u0438\u043d\u0442\u0435\u043b\u043b\u0435\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0445 \u0431\u0435\u0437\u0443\u043c\u0438\u0439\u005c" \u043c\u043e\u0436\u0435\u0442 \u0434\u043e\u0439\u0442\u0438 \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e \u043f\u044c\u044f\u043d\u044b\u0439 \u0445\u0443\u0434\u043e\u0436\u043d\u0438\u043a \u0432 \u0441\u0432\u043e\u0435\u043c \u0432\u0418\u0434\u0435\u043d\u0438\u0438 \u0447\u0435\u0445\u043e\u0432\u0441\u043a\u043e\u0433\u043e \u005c"\u0412\u0438\u0448\u043d\u0435\u0432\u043e\u0433\u043e \u0441\u0430\u0434\u0430\u005c", \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u043e\u043d \u0432\u0437\u044f\u043b\u0441\u044f \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u043d\u0430 \u043f\u043e\u0434\u043c\u043e\u0441\u0442\u043a\u0430\u0445 \u041f\u044f\u0442\u043e\u0433\u043e, \u2014 \u0443\u0436 \u0443\u0432\u043e\u043b\u044c\u0442\u0435 \u043c\u0435\u043d\u044f, \u043a\u0430\u043a \u0437\u0440\u0438\u0442\u0435\u043b\u044f, \u043e\u0442 \u0442\u0430\u043a\u0438\u0445 \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442\u043e\u0432, \u0434\u0430\u0436\u0435 \u0432 \u0441\u0442\u0440\u0430\u0448\u043d\u043e\u043c \u0441\u043d\u0435 \u043d\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u043f\u0440\u0435\u0434\u0443\u0433\u0430\u0434\u0430\u0442\u044c, \u043a\u0443\u0434\u0430 \u0437\u0430\u0432\u0435\u0434\u0435\u0442 \u0442\u0430\u043a\u043e\u0439 \u043a\u0430\u0440\u0430\u0432\u0430\u043d (\u041a\u0420), \u0434\u0430\u0436\u0435 \u0441\u0438\u0434\u044f \u043d\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u043c \u0432\u0435\u0440\u0431\u043b\u044e\u0434\u0435. \u041a\u0441\u0442\u0430\u0442\u0438, \u0430\u0431\u0431\u0440\u0435\u0432\u0438\u0430\u0442\u0443\u0440\u0430 \u041a\u0420, \u044d\u0442\u043e \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u043e \u043d\u0435 \u005c"\u043a\u043e\u0437\u0430 \u0440\u043e\u0433\u0430\u0442\u0430\u044f\u005c"? \u0415\u0441\u043b\u0438 \u0434\u0430, \u0442\u043e \u0442\u043e\u0433\u0434\u0430 \u043d\u0435 \u0443\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u0447\u0442\u043e \u0412\u0430\u043c \u043f\u043e \u043f\u0443\u0442\u0438 \u0441 \u0442\u0430\u043a\u0438\u043c \u005c"\u043a\u0430\u0440\u0430\u0432\u0430\u043d\u043e\u043c\u005c".
p1823
aV\u042f \u0432\u0430\u0441 \u0432\u0441\u0435\u0445 \u0432\u044b\u0435\u0431\u0443
p1824
aV\u041d\u0430\u0431\u043e\u0431 \u0438 \u044d\u0442\u0438\u043c \u0432\u0441\u0451 \u0441\u043a\u0430\u0437\u0430\u043d\u043e. \u041a\u0430\u043a \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u0441\u044f \u2014 \u043a\u0430\u043a \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442 \u2014 \u0442\u0430\u043a \u0438 \u0443\u0445\u043e\u0434\u0438\u0442, \u0435\u0441\u043b\u0438 \u0441\u0432\u043e\u0438\u043c \u0447\u0435\u0441\u0442\u043d\u044b\u043c \u0442\u0440\u0443\u0434\u043e\u043c \u0437\u0430\u0440\u0430\u0431\u043e\u0442\u0430\u043d\u043e \u2014 \u0442\u0430\u043a \u043f\u043e\u043a\u0430\u0437\u0443\u0448\u043d\u043e \u0438 \u0440\u0430\u0441\u0442\u043e\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043d\u0435 \u0442\u0440\u0430\u0442\u0438\u0442\u0441\u044f, \u043a\u0442\u043e \u0443\u043c\u0435\u0435\u0442 \u0437\u0430\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u0442\u044c, \u0442\u043e\u0442 \u0431\u0435\u0440\u0435\u0436\u043d\u043e \u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0441\u044f \u043a \u043a\u0430\u0436\u0434\u043e\u0439 \u043a\u043e\u043f\u0435\u0439\u043a\u0435.
p1825
aVarcush@rambler.ru, \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u043d\u0435 \u043f\u043e\u043d\u0438\u043c\u0430\u044e\u0449\u0438\u0439, \u0437\u0430\u0447\u0435\u043c \u041e\u043c\u0441\u043a\u0443 \u043d\u0443\u0436\u043d\u044b \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u044b \u0432\u044b\u0437\u044b\u0432\u0430\u0435\u0442 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u043e\u043c\u043d\u0435\u043d\u0438\u044f \u0432 \u0441\u0432\u043e\u0435\u0439 \u0430\u0434\u0435\u043a\u0432\u0430\u0442\u043d\u043e\u0441\u0442\u0438.
p1826
aV\u0412\u0441\u0435\u043c \u0431\u0443\u044f\u043c.\u0412\u044b \u0445\u043e\u0442\u044c \u0437\u043d\u0430\u0435\u0442\u0435 \u0433\u0434\u0435 \u043d\u0430 \u043a\u0430\u0440\u0442\u0435 \u0423\u043a\u0440\u0430\u0438\u043d\u0430 \u0442\u043e \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u044c\u0441\u044f? \u0411\u044b\u043b\u0438 \u0442\u0430\u043c \u0445\u043e\u0442\u044c \u0440\u0430\u0437 \u0438\u043b\u0438 \u0432 \u041a\u0440\u044b\u043c\u0443? \u0427\u0442\u043e \u0437\u0430 \u0445\u0440\u0435\u043d\u044c \u0432\u044b \u043d\u0435\u0441\u0451\u0442\u0435.\u0418\u043b\u0438 \u043f\u043e \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0443 "\u043d\u0435 \u0447\u0438\u0442\u0430\u043b, \u043d\u043e \u0441\u043a\u0430\u0436\u0443".
p1827
aV\u0422\u0430\u043a \u043f\u0440\u043e\u0449\u0435 \u043f\u0440\u043e\u0442\u0430\u043b\u043a\u0438\u0432\u0430\u0442\u044c \u0437\u0430\u043a\u043e\u043d\u043e\u043f\u0440\u043e\u0435\u043a\u0442\u044b, \u0432 \u0447\u0430\u0441\u0442\u043d\u043e\u0441\u0442\u0438 \u043f\u043e\u0432\u044b\u0448\u0435\u043d\u0438\u0435 \u043f\u043b\u0430\u0442\u044b \u0437\u0430 \u043f\u0440\u043e\u0435\u0437\u0434. \u0418\u043d\u0430\u043a\u043e\u043c\u044b\u0441\u043b\u044f\u044e\u0449\u0438\u0435 -\u0432 \u044d\u0442\u043e\u0439 \u0441\u0432\u043e\u0440\u0435 \u043d\u0435 \u043d\u0443\u0436\u043d\u044b. \u0413\u043e\u0440\u0441\u043e\u0432\u0435\u0442 - \u0441\u0442\u0430\u043b \u043d\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u0442\u044c \u043c\u0430\u0444\u0438\u043e\u0437\u043d\u0443\u044e \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0443...
p1828
aV\u041d\u0435\u0442 \u0442\u0430\u043a\u043e\u0433\u043e \u0446\u0435\u043d\u043d\u0438\u043a\u0430. \u041a\u0430\u043a \u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c 13 \u0440\u0443\u0431. 
p1829
aV\u043d\u044d\u0442
p1830
aV\u0442\u044b \u0443\u0436\u0435 \u043f\u043e\u0434\u043c\u0430\u0445\u0438\u0432\u0430\u0442\u044c \u0433\u043e\u0442\u043e\u0432\u0438\u0448\u044c\u0441\u044f?
p1831
aV\u042d\u043b, \u044d\u0442\u043e\u0433\u043e \u0410\u043b\u0435\u043a\u0441\u0430 \u0416 \u0437\u0434\u0435\u0441\u044c \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0437 \u0443\u043f\u043e\u043c\u044f\u043d\u0430\u043b\u0438 \u043a\u0430\u043a \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u0430 \u0416\u0438\u0440\u043e\u0432\u0430. \u041d\u0435 \u0437\u043d\u0430\u044e \u0431\u0435\u0434\u043e\u043b\u0430\u0433\u0430 \u043e\u043d \u0438\u043b\u0438 \u043d\u0435\u0442, \u043d\u043e \u043a\u0430\u043a \u044f \u043f\u043e\u043d\u044f\u043b\u0430, \u043e\u043d \u0432\u0441\u0435 \u0436\u0435 \u0443\u043c\u0443\u0434\u0440\u044f\u0435\u0442\u0441\u044f \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442\u043e\u043c, \u0447\u0442\u043e \u043d\u0435 \u0441\u043e\u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0441\u044f \u0441 \u0443\u0440\u043e\u0432\u043d\u0435\u043c \u0435\u0433\u043e \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u043e\u0432 \u043d\u0430 \u0411\u041a. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u044f \u0438 \u0434\u0443\u043c\u0430\u044e, \u0447\u0442\u043e \u044d\u0442\u043e \u043d\u0435 \u0441\u0430\u043c \u0410\u043b\u0435\u043a\u0441 \u0416\u0438\u0440\u043e\u0432, \u0430 \u043d\u0435\u043a\u0442\u043e, \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0449\u0438\u0439 \u0441\u0435\u0431\u044f \u0442\u0430\u043a\u043e\u0432\u044b\u043c. \u0421\u0442\u0440\u0430\u043d\u043d\u043e, \u0447\u0442\u043e \u0411\u041a \u0434\u043e\u043f\u0443\u0441\u043a\u0430\u0435\u0442 \u0442\u0430\u043a\u0443\u044e \u0433\u0440\u043e\u0442\u0435\u0441\u043a\u043d\u043e-\u043e\u0441\u043a\u043e\u0440\u0431\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u0430\u0440\u043e\u0434\u0438\u044e \u043d\u0430 \u0440\u0435\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430. \u0418 \u043d\u0435 \u0432 \u043e\u0434\u043d\u043e\u043c- \u0434\u0432\u0443\u0445 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0430\u0445 \u0441\u043c\u0435\u0445\u0430 \u0440\u0430\u0434\u0438, a \u0441 \u0442\u0430\u043a\u0438\u043c \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u0441\u0442\u0432\u043e\u043c, \u0442\u0430\u043a\u043e\u0435 \u0434\u043e\u043b\u0433\u043e\u0435 \u0432\u0440\u0435\u043c\u044f. \u042d\u0442\u043e \u0443\u0436\u0435 \u0438\u0437\u0434\u0435\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u043e, a \u043d\u0435 \u0431\u0435\u0437\u043e\u0431\u0438\u0434\u043d\u0430\u044f \u043f\u0440\u043e\u0434\u0435\u043b\u043a\u0430. \u0421\u0443\u0442\u044c \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u043e\u0432, \u043f\u0443\u0431\u043b\u0438\u043a\u0443\u0435\u043c\u044b\u0445 \u043f\u043e\u0434 \u0438\u043c\u0435\u043d\u0435\u043c \u0410\u043b\u0435\u043a\u0441 \u0416, \u0438\u043c\u0435\u0435\u0442 \u043f\u043e\u0440\u043e\u0447\u0430\u0449\u0435\u0435 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0435\u0433\u043e \u0410\u043b\u0435\u043a\u0441\u0430, \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442\u0430, \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435. \u0427\u0435\u0433\u043e \u0441\u0442\u043e\u044f\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u0435\u0433\u043e \u043f\u043e\u0440\u043d\u043e \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u044b \u043f\u043e\u0434 \u043f\u0443\u0431\u043b\u0438\u043a\u0443\u0435\u043c\u044b\u043c\u0438 \u043d\u0430 BK \u043f\u043e\u0440\u043d\u043e \u043a\u0430\u0440\u0442\u0438\u043d\u043a\u0430\u043c\u0438! \u0418\u0437 \u044d\u0442\u043e\u0433\u043e \u0410\u043b\u0435\u043a\u0441\u0430 \u0416 \u043f\u0440\u044f\u043c\u043e \u0442\u0430\u043a\u0438 \u0441\u0434\u0435\u043b\u0430\u043b\u0438 \u0437\u0434\u0435\u0441\u044c \u0432\u043d\u0435\u0448\u0442\u0430\u0442\u043d\u043e\u0433\u043e \u0434\u0443\u0440\u043e\u0447\u043a\u0430. \u0422\u043e, \u043a\u0430\u043a \u0437\u0434\u0435\u0441\u044c \u0432\u044b\u0441\u0442\u0430\u0432\u0438\u043b\u0438 \u043d\u043
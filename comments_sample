p1469
aV\u0431\u0440\u0430\u043b\u0438 \u0441\u0442\u0432\u043e\u043b\u043e\u0432\u044b\u0435 \u043a\u043b\u0435\u0442\u043a\u0438 \u0443 \u044d\u043c\u0431\u0440\u0438\u043e\u043d\u043e\u0432 \u0434\u043b\u044f \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u0439 \u043f\u043e \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0435\u043d\u0438\u044e \u043c\u043e\u043b\u043e\u0434\u043e\u0441\u0442\u0438 \u0442\u0435\u0445 \u043e\u043c\u0438\u0447\u0435\u0439, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0437\u0430\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u044e\u0442 \u0431\u043e\u043b\u0435\u0435 500 \u0442\u044b\u0441\u044f\u0447 \u0440\u0443\u0431\u044c\u043b\u0435\u0439 \u0432 \u043c\u0435\u0441\u044f\u0446
p1470
aV\u0410 \u0432\u044b \u043d\u0435 \u043d\u0430\u0440\u0443\u0448\u0430\u0439\u0442\u0435 \u043f\u0440\u0430\u0432\u0438\u043b\u0430 \u0438 \u043d\u0438\u043a\u0442\u043e \u0441\u0442\u0443\u0447\u0430\u0442\u044c \u043d\u0435 \u0431\u0443\u0434\u0435\u0442. \u0430 \u0442\u0430\u043a-\u0442\u043e \u0441\u043b\u0430\u0432\u0430 \u0441\u0442\u0443\u043a\u0430\u0447\u0430\u043c, \u043c\u0430\u0442\u044c \u0438\u0445 \u0437\u0430 \u043d\u043e\u0433\u0443.
p1471
aV\u041c\u043e\u0433\u0443\u0442 \u043e\u0442\u043a\u0440\u044b\u0442\u044c, \u0430 \u043c\u043e\u0433\u0443\u0442 \u0438 \u043d\u0435 \u043e\u0442\u043a\u0440\u044b\u0442\u044c.... \u041c\u043e\u0433\u0443\u0442 \u0434\u043e\u0434\u0435\u043b\u0430\u0442\u044c, \u0430 \u043c\u043e\u0433\u0443\u0442 \u0438 \u043d\u0435 \u0434\u043e\u0434\u0435\u043b\u0430\u0442\u044c... \u0412 \u0420\u043e\u0441\u0441\u0438\u0438 \u0436\u0438\u0432\u0435\u043c!
p1472
aV\u043a\u0430\u043a\u043e\u0435 \u0435\u0449\u0435 \u0427\u041f? \u0442\u0430\u043a, \u043e\u0431\u044b\u0447\u043d\u043e\u0435 \u0434\u0435\u043b\u043e \u043a\u0430\u043a \u0441\u043a\u0430\u0437\u0430\u043b \u041a\u043e\u0440\u0431\u0443\u0442
p1473
aV\u0417\u0430 \u0435\u0436\u0435\u0434\u043d\u0435\u0432\u043d\u043e\u0435 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435 \u0432\u043e\u0434\u043e\u0439 (\u0437\u0430\u0445\u043e\u0442\u0435\u043b-\u043f\u043e\u043c\u044b\u043b\u0441\u044f, \u043f\u043e\u0441\u0443\u0434\u0443 \u043f\u043e\u043c\u044b\u043b) \u043d\u0430\u0434\u043e \u043f\u043b\u0430\u0442\u0438\u0442\u044c, \u0434\u0435\u043b\u043e \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u0441\u043a\u043e\u043b\u044c\u043a\u043e ??? \u041f\u043b\u0430\u0442\u0430 \u0437\u0430 \u041e\u0414\u041d- \u0441\u0447\u0438\u0442\u0430\u044e, \u0447\u0442\u043e \u044d\u0442\u043e \u043e\u0431\u0434\u0438\u0440\u0430\u043b\u043e\u0432\u043a\u0430 \u0438 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u0435 \u0441\u0432\u0435\u0440\u0445\u043f\u0440\u0438\u0431\u044b\u043b\u0438. \u0422\u0435\u043f\u043b\u043e \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u0442\u0430\u043a\u0436\u0435 -\u0432\u0441\u0435\u0433\u0434\u0430 (\u043d\u043e \u0446\u0435\u043d\u0430..., \u0441\u0447\u0438\u0442\u0430\u044e, \u0447\u0442\u043e \u0434\u043e\u0440\u043e\u0433\u043e\u0432\u0430\u0442\u043e, \u043d\u0443 \u0434\u0430 \u043b\u0430\u0434\u043d\u043e). \u0415\u0434\u0438\u043d\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435, \u0447\u0442\u043e \u043d\u0435 \u0432\u044b\u0437\u044b\u0432\u0430\u0435\u0442 \u0432\u043e\u0437\u043c\u0443\u0449\u0435\u043d\u0438\u0435-\u0442\u0430\u0440\u0438\u0444\u044b \u043d\u0430 \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u044d\u043d\u0435\u0440\u0433\u0438\u044e, \u0441\u0447\u0438\u0442\u0430\u044e, \u0447\u0442\u043e \u043e\u043d\u0438 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u0437\u0434\u0440\u0430\u0432\u043e\u043c\u0443 \u0441\u043c\u044b\u0441\u043b\u0443, \u0441 \u0434\u0440\u0443\u0433\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b - \u0441\u0435\u0442\u0438 \u0438\u0437\u043d\u043e\u0448\u0435\u043d\u044b, \u043f\u043e\u0442\u0435\u0440\u0438- \u0443\u0436\u0430\u0441\u043d\u044b\u0435, \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u044d\u043b\u0435\u043a\u0442\u0440\u043e\u044d\u043d\u0435\u0440\u0433\u0438\u0438 \u0441\u0442\u0430\u043b\u043e \u043f\u043e\u0434\u043a\u0430\u0447\u0438\u0432\u0430\u0442\u044c (\u0442\u0430\u043a \u043a\u0430\u043a \u043d\u0435 \u0432\u043d\u0435\u0434\u0440\u044f\u044e\u0442\u0441\u044f \u043d\u043e\u0432\u044b\u0435 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u0432 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u043c \u043f\u043e \u0441\u0442\u0430\u0431\u0438\u043b\u0438\u0437\u0430\u0446\u0438\u0438 \u043d\u0430\u043f\u0440\u044f\u0436\u0435\u043d\u0438\u044f, \u0447\u0442\u043e \u0441\u043a\u043e\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043e \u0431\u043e\u043b\u044c\u0448\u0435 \u0438 \u0431\u0435\u0441\u043f\u043e\u043a\u043e\u0438\u0442 \u043f\u043e\u0442\u0440\u0435\u0431\u0438\u0442\u0435\u043b\u0435\u0439 (\u0441\u043a\u0430\u0447\u043a\u0438, "\u043f\u0440\u043e\u0441\u0430\u0434\u043a\u0438", \u043d\u0438\u0437\u043a\u043e\u0435 \u0438 \u0432\u044b\u0441\u043e\u043a\u043e\u0435 \u043d\u0430\u043f\u0440\u044f\u0436\u0435\u043d\u0438\u0435 \u0432 \u0441\u0435\u0442\u0438) ).
p1474
aV\u0421\u043d\u0435\u0433 \u0443\u0431\u0438\u0440\u0430\u0442\u044c \u043d\u0430\u0434\u043e \u0437\u0438\u043c\u043e\u0439! \u0422\u043e\u0433\u0434\u0430 \u043d\u0435\u0447\u0435\u043c\u0443 \u0431\u0443\u0434\u0435\u0442 \u0437\u0430\u043c\u0435\u0440\u0437\u0430\u0442\u044c \u0432 \u0442\u0440\u0435\u0449\u0438\u043d\u0430\u0445 \u0438 \u0434\u043e\u0440\u043e\u0433\u0438 \u043d\u0435 \u0440\u0430\u0437\u0432\u0430\u043b\u044f\u0442\u0441\u044f
p1475
aV\u043c\u0440\u0430\u0437\u044c!
p1476
aV\u041d\u043e\u0432\u044b\u0435 \u043f\u0435\u0447\u0430\u043b\u044c\u043d\u044b\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e\u0441\u0442\u0438 \u044d\u0442\u043e\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u0438: http://bk55.ru/news/article/25349
p1477
aV\u0414\u0430\u0432\u0438\u0434 \u0424\u0435\u0434\u043e\u0440\u043e\u0432\u0438\u0447 - \u044d\u0442\u043e \u0432\u0435\u0447\u043d\u044b\u0435 \u043f\u0435\u0441\u043d\u0438!!!!
p1478
aV123, \u043d\u0435 \u0432\u043e\u043b\u043d\u0443\u0439\u0442\u0435\u0441\u044c, \u043d\u0435 \u0440\u0430\u0437\u0433\u043e\u043d\u044f\u0442
p1479
aV234, \u044d\u0442\u0438\u043c \u043b\u0435\u0442\u043e\u043c \u043f\u043e\u0431\u044b\u0432\u0430\u043b\u0430 \u0432 \u041a\u0440\u0430\u0441\u043d\u043e\u044f\u0440\u0441\u043a\u0435. \u041e\u0447\u0435\u043d\u044c \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u043e\u0441\u044c. \u041c\u0430\u0440\u0448\u0440\u0443\u0442\u043e\u043a \u043d\u0435\u0442, \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u044b \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u044b\u0435, \u0445\u043e\u0434\u044f\u0442 \u043f\u043e \u0440\u0430\u0441\u043f\u0438\u0441\u0430\u043d\u0438\u044e. \u0412 \u043f\u0440\u043e\u0431\u043a\u0443 \u043d\u0435 \u043f\u043e\u043f\u0430\u043b\u0430 \u043d\u0438 \u0440\u0430\u0437\u0443. \u041a\u0430\u043a \u0442\u0430\u043a)
p1480
aV\u0434\u0430 \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u043e\u0434 \u0448\u0443\u043c\u043e\u043a \u043f\u043e\u0434\u043d\u044f\u043b\u0438 \u0446\u0435\u043d\u044b. \u0414\u0443\u043c\u0430\u0435\u0442\u0435 \u043d\u0430 \u0440\u044b\u043d\u043a\u0430\u0445 \u0438\u043c\u043f\u043e\u0440\u0442\u043d\u043e\u0439 \u0441\u0432\u0438\u043d\u0438\u043d\u043e\u0439 \u0442\u043e\u0440\u0433\u0443\u044e\u0442?
p1481
aV\u0423\u043a\u0440\u043e\u043f\u044b - \u0424\u0430\u0448\u0438\u0441\u0442\u044b \u043d\u043e\u0432\u043e\u0433\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0438!!!
p1482
aV\u041d\u0443 \u0441\u043a\u0430\u0436\u0435\u043c \u0442\u0430\u043a\u0438 \u043f\u0440\u044f\u043c\u043e-\u043a\u0440\u0430\u0441\u043e\u0442\u044b \u043c\u0430\u043b\u043e..\u0438\u043b\u0438 \u043d\u0435\u0442, \u0442\u0430\u043a-"\u043a\u0440\u0430\u0441\u043e\u0442\u0430 \u043c\u0430\u043b\u043e\u0432\u0430\u0442\u0430"
p1483
aV\u041e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0438\u0435 \u0441\u043b\u043e\u0432\u0430, \u0442\u043e\u043b\u044c\u043a\u043e \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u0435\u0442 \u0432\u043e\u043f\u0440\u043e\u0441: \u043a\u0430\u043a\u0438\u0435 \u043c\u0435\u0440\u044b \u0431\u0443\u0434\u0443\u0442 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u044c\u0441\u044f. \u0410\u0436 \u0441\u0442\u0430\u043b\u043e \u043b\u044e\u0431\u043e\u043f\u044b\u0442\u043d\u043e. \u0423\u0436\u0435 \u0441\u0442\u043e\u043b\u044c\u043a\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u0438\u0434\u0435\u0442 \u0432\u043e\u0439\u043d\u0430, \u0430 \u043c\u0435\u0440\u044b \u0431\u0443\u0434\u0443\u0442 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u044c\u0441\u044f \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u0435\u0439\u0447\u0430\u0441.
p1484
aV\u0412 \u0442\u0430\u043a\u043e\u0439 \u0441\u0442\u0440\u0430\u043d\u0435 \u0436\u0438\u0442\u044c \u0445\u043e\u0447\u0435\u0442\u0441\u044f  \u043a\u0430\u043a \u043c\u043e\u0436\u043d\u043e \u043c\u0435\u043d\u044c\u0448\u0435, \u0441\u043f\u0430\u0441\u0438\u0431\u043e \u0447\u0442\u043e \u0435\u0441\u0442\u044c \u0441\u043c\u0435\u0440\u0442\u044c \u043a\u043e\u0433\u0434\u0430 \u043d\u0438\u0431\u0443\u0434\u044c \u0432\u0441\u0435 \u044d\u0442\u043e \u043a\u043e\u043d\u0447\u0438\u0442\u0441\u044f
p1485
aV\u041d\u0443 \u0437\u0430\u0447\u0435\u043c \u0412\u044b \u0442\u0430\u043a? \u0427\u0435\u043b\u043e\u0432\u0435\u043a \u043f\u043e\u0433\u0438\u0431...
p1486
aV\u041f\u043b\u0430\u0442\u0438\u0442\u044c \u043f\u043e\u043b\u0443\u0447\u043a\u0443 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u0441\u043b\u0435 \u043f\u043e\u0431\u0435\u0434,\u0430 \u043f\u043e\u0441\u043b\u0435 \u043f\u0440\u043e\u0438\u0433\u0440\u044b\u0448\u0430 \u0448\u0442\u0440\u0430\u0444
p1487
aV\u041e\u043c\u0441\u043a \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0442\u0430\u043a\u0438\u043c \u043c\u043e\u0449\u043d\u044b\u043c \u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u044b\u043c \u0446\u0435\u043d\u0442\u0440\u043e\u043c, \u0430 \u043d\u0435 \u043c\u043e\u0436\u0435\u0442 \u0437\u0430\u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u0441\u0435\u0431\u0435 \u043d\u0430 \u0440\u0435\u043c\u043e\u043d\u0442 \u0434\u043e\u0440\u043e\u0433 \u0438 \u043d\u0430 \u043d\u043e\u0432\u044b\u0439 \u0442\u0440\u0430\u043c\u0432\u0430\u0439. \u044d\u0442\u043e \u043f\u0435\u0447\u0430\u043b\u044c\u043d\u043e \u043d\u0430 \u0441\u0430\u043c\u043e\u043c \u0434\u0435\u043b\u0435. \u043d\u043e\u0432\u043e\u0441\u0442\u044c \u2014 \u043a\u043b\u0430\u0441\u0441!!
p1488
aV\u0418\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e, \u043a\u0442\u043e-\u043d\u0438\u0431\u0443\u0434\u044c \u043a\u043e\u0433\u0434\u0430-\u043d\u0438\u0431\u0443\u0434\u044c \u0441\u043b\u044b\u0448\u0430\u043b \u043e \u0442\u043e\u043c, \u0447\u0442\u043e \u0437\u0435\u043c\u043b\u0438 \u0432 \u0446\u0435\u043d\u0442\u0440\u0435 \u041e\u043c\u0441\u043a\u0430 \u043f\u0440\u0438\u043d\u0430\u0434\u043b\u0435\u0436\u0430\u0442 \u0410\u0431\u0440\u0430\u043c\u043e\u0432\u0438\u0447\u0443? \u0410 \u043f\u043e\u0447\u0435\u043c\u0443 \u043d\u0435 \u0432\u0435\u0441\u044c \u041e\u043c\u0441\u043a? \u0418 \u043f\u043e\u0447\u0435\u043c\u0443 \u0442\u0435\u043f\u0435\u0440\u044c \u041e\u041c\u0418\u0427\u0418 \u0434\u043e\u043b\u0436\u043d\u044b \u0432\u044b\u043a\u0443\u043f\u0430\u0442\u044c \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u0437\u0435\u043c\u043b\u0438 \u0434\u0430\u043b\u0435\u043a\u043e \u043d\u0435 \u0437\u0430 \u043a\u043e\u043f\u0435\u0435\u0447\u043d\u0443\u044e \u0446\u0435\u043d\u0443 "\u0443 \u043f\u043e\u0434\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u044c\u043d\u043e\u0433\u043e \u0420\u043e\u043c\u0430\u043d\u0443 \u0410\u0431\u0440\u0430\u043c\u043e\u0432\u0438\u0447\u0443 \u041e\u041e\u041e \u041c\u0438\u043b\u043b\u0445\u0430\u0443\u0441"?
p1489
aV17 \u043b\u044f\u043c\u043e\u0432 \u043d\u0430 \u043a\u043e\u043a\u0430-\u043a\u043e\u043b\u0435 \u0441\u044d\u043a\u043e\u043d\u043e\u043c\u0438\u043b\u0438
p1490
aV\u041d\u0430\u0441\u0447\u0435\u0442 \u0438\u0434\u0438\u043e\u0442\u0441\u043a\u0438\u0445 \u0440\u0435\u0448\u0435\u043d\u0438\u0439 \u0432\u043b\u0430\u0441\u0442\u0438 \u0441\u043f\u043e\u0440\u0443 \u043d\u0435\u0442, \u0430 \u0432\u043e\u0437\u0440\u0430\u0441\u0442 \u0443 \u043f\u0435\u0434\u043e\u0444\u0438\u043b\u043e\u0432 \u0441\u0430\u043c\u044b\u0439 \u0440\u0430\u0437\u043d\u044b\u0439. \u0412 \u041d\u043e\u0432\u043e\u0441\u0438\u0431\u0438\u0440\u0441\u043a\u0435, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u0434\u0432\u043e\u0435 \u0434\u0435\u0434\u0443\u0448\u0435\u043a, \u043e\u0434\u043d\u043e\u043c\u0443 54-\u0445 62 -\u0445 \u043b\u0435\u0442 \u0438\u0437\u043d\u0430\u0441\u0438\u043b\u043e\u0432\u0430\u043b\u0438 \u0441\u0432\u043e\u0438\u0445 \u0432\u043d\u0443\u0447\u0435\u043a, \u043e\u0434\u043d\u043e\u0439 8 \u043b\u0435\u0442, \u0434\u0440\u0443\u0433\u043e\u0439 15, \u0442\u0430\u043c \u0436\u0435 40-\u043b\u0435\u0442\u043d\u0438\u0439, \u0447\u0442\u043e \u043f\u043e\u0440\u0430\u0437\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u2014 \u0434\u0438\u0437\u0430\u0439\u043d\u0435\u0440 \u0438\u043d\u0442\u0435\u0440\u044c\u0435\u0440\u0430 \u0441\u043e\u0432\u0435\u0440\u0448\u0438\u043b \u0442\u0430\u043a\u043e\u0435, \u0447\u0435\u0433\u043e \u043b\u0443\u0447\u0448\u0435 \u043d\u0435 \u043f\u0438\u0441\u0430\u0442\u044c. \u0418 \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442, \u0441\u043e\u0432\u0441\u0435\u043c \u044e\u043d\u044b\u0435 \u043c\u0430\u043c\u044b \u0432\u044b\u0431\u0440\u0430\u0441\u044b\u0432\u0430\u0435\u0442 \u0433\u0440\u0443\u0434\u043d\u044b\u0445 \u0434\u0435\u0442\u0435\u0439 \u043d\u0430 \u043f\u043e\u043c\u043e\u0439\u043a\u0443, \u0432 \u043a\u0430\u043d\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044e, \u0430 \u0432 \u0422\u043e\u043c\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0442\u0440\u043e\u0435 \u0434\u0435\u0442\u0434\u043e\u043c\u043e\u0432\u0446\u0435\u0432, \u0432\u0437\u044f\u0442\u044b\u0445 \u0432 \u043f\u0440\u0438\u0435\u043c\u043d\u044b\u0435 \u0441\u0435\u043c\u044c\u0438 \u0432 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u0434\u0432\u0443\u0445 \u043b\u0435\u0442 \u043d\u0430\u0441\u0438\u043b\u043e\u0432\u0430\u043b\u0438 \u0434\u0435\u0442\u0435\u0439 \u043f\u043e\u043c\u043b\u0430\u0434\u0448\u0435, \u0442\u043e\u0436\u0435 \u0438\u0437 \u043f\u0440\u0438\u0435\u043c\u043d\u044b\u0445 \u0441\u0435\u043c\u0435\u0439. \u0423\u0436\u0430\u0441\u043e\u0432 \u0432\u0441\u044f\u043a\u0438\u0445 \u043c\u043d\u043e\u0433\u043e. \u0422\u043e\u0442 \u0432\u043e\u0437\u0440\u0430\u0441\u0442, \u043e \u043a\u043e\u0442\u043e\u0440\u043e\u043c \u0432\u044b \u043f\u0438\u0448\u0435\u0442\u0435, \u043a\u0430\u043a \u0440\u0430\u0437 \u043c\u043d\u0435 \u043d\u0435 \u043f\u043e\u043f\u0430\u0434\u0430\u0435\u0442\u0441\u044f. \u0434\u0430 \u0438 \u0443 \u043d\u0430\u0441 \u043e\u0442\u0446\u0443 \u041a\u043e\u043b\u0438 \u041a\u0443\u043a\u0438\u043d\u0430 \u0442\u043e\u0436\u0435 \u043f\u043e-\u043c\u043e\u0435\u043c\u0443 \u043e\u043a\u043e\u043b\u043e 40. \u0422\u0430\u043a \u0447\u0442\u043e \u043d\u0430\u0432\u0440\u044f\u0434 \u043b\u0438 \u0442\u0443\u0442 \u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u0442\u043b\u0435\u0442\u0432\u043e\u0440\u043d\u043e\u0435 \u0432\u043b\u0438\u044f\u043d\u0438\u0435 90-\u0445.
p1491
aV\u2026\u0431\u043e\u043b\u044c\u0448\u0435\u0432\u0438\u0441\u0442\u0441\u043a\u0438\u0439 \u0441\u043e\u043b\u0434\u0430\u0442 \u043f\u043e\u0442\u0435\u0440\u044f\u043b \u0432\u0441\u044f\u043a\u043e\u0435 \u043f\u0440\u0430\u0432\u043e \u043f\u0440\u0435\u0442\u0435\u043d\u0434\u043e\u0432\u0430\u0442\u044c \u043d\u0430 \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u0435 \u0441 \u043d\u0438\u043c, \u043a\u0430\u043a \u0441 \u0447\u0435\u0441\u0442\u043d\u044b\u043c \u0441\u043e\u043b\u0434\u0430\u0442\u043e\u043c \u0432 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438 \u0441 \u0416\u0435\u043d\u0435\u0432\u0441\u043a\u0438\u043c \u0441\u043e\u0433\u043b\u0430\u0448\u0435\u043d\u0438\u0435\u043c. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u0432\u043f\u043e\u043b\u043d\u0435 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0442\u043e\u0447\u043a\u0435 \u0437\u0440\u0435\u043d\u0438\u044f \u0438 \u0434\u043e\u0441\u0442\u043e\u0438\u043d\u0441\u0442\u0432\u0443 \u0433\u0435\u0440\u043c\u0430\u043d\u0441\u043a\u0438\u0445 \u0432\u043e\u043e\u0440\u0443\u0436\u0435\u043d\u043d\u044b\u0445 \u0441\u0438\u043b, \u0447\u0442\u043e\u0431\u044b \u043a\u0430\u0436\u0434\u044b\u0439 \u043d\u0435\u043c\u0435\u0446\u043a\u0438\u0439 \u0441\u043e\u043b\u0434\u0430\u0442 \u043f\u0440\u043e\u0432\u043e\u0434\u0438\u043b \u0431\u044b \u0440\u0435\u0437\u043a\u0443\u044e \u0433\u0440\u0430\u043d\u044c \u043c\u0435\u0436\u0434\u0443 \u0441\u043e\u0431\u043e\u044e \u0438 \u0441\u043e\u0432\u0435\u0442\u0441\u043a\u0438\u043c\u0438 \u0432\u043e\u0435\u043d\u043d\u043e\u043f\u043b\u0435\u043d\u043d\u044b\u043c\u0438.\u2026\u0427\u0443\u0432\u0441\u0442\u0432\u043e \u0433\u043e\u0440\u0434\u043e\u0441\u0442\u0438 \u0438 \u043f\u0440\u0435\u0432\u043e\u0441\u0445\u043e\u0434\u0441\u0442\u0432\u0430 \u043d\u0435\u043c\u0435\u0446\u043a\u043e\u0433\u043e \u0441\u043e\u043b\u0434\u0430\u0442\u0430, \u043d\u0430\u0437\u043d\u0430\u
p126
aV\u0442\u0435\u043c\u0430 \u043d\u0435 \u0440\u0430\u0441\u043a\u0440\u044b\u0442\u0430\u2026 \u0438, \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u043d\u0435 \u043f\u043e\u043d\u044f\u0442\u043d\u043e, \u043a\u0430\u043a \u0441\u043e \u0441\u0447\u0435\u0442\u043e\u0432 \u0432 \u0421\u0431\u0435\u0440\u0435 (\u043d\u0435 \u043a\u0430\u0440\u0442\u043e\u0447\u043d\u044b\u0445)\u043c\u043e\u0436\u043d\u043e \u0447\u0435\u0433\u043e-\u0442\u043e \u0441\u043f\u0438\u0441\u0430\u0442\u044c \u043f\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0443\u2026
p127
aV\u043f\u043e\u0432\u044b\u0448\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0435\u0437\u0434\u0430 \u0432 \u0440\u0430\u0437\u0434\u043e\u043b\u0431\u0430\u043d\u043d\u044b\u0445 \u0430\u0432\u0442\u043e\u0431\u0443\u0441\u0430\u0445 \u0441 \u043f\u044c\u044f\u043d\u044b\u043c\u0438 \u043a\u043e\u043d\u0434\u0443\u043a\u0442\u043e\u0440\u0448\u0430\u043c\u0438 \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u043b\u043e \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d\u0438\u0435, \u043f\u043e\u0442\u043e\u043c \u0442\u0430\u0440\u0438\u0444 \u043d\u0430 \u0433\u0430\u0437 \u043f\u0440\u043e\u0438\u0437\u0432\u0451\u043b \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u0435\u043d\u0438\u0435, \u0430 \u0442\u0435\u043f\u0435\u0440\u044c \u0438 \u044d\u0442\u043e
p128
aV\u041f\u0440\u0438\u0432\u0435\u0442 
p129
aV\u0412\u043e\u0442 \u043c\u043e\u043b\u043e\u0434\u0435\u0446 \u041a\u043e\u043a\u043e\u0440\u0438\u043d! \u0425\u0443\u0434\u043e\u0436\u043d\u0438\u043a\u0430\u043c \u043d\u0430\u0434\u043e \u043f\u043e\u043c\u043e\u0433\u0430\u0442\u044c \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e.
p130
aV\u0414\u0430 \u0442\u044b \u0435\u0449\u0451 \u0438 \u0432 \u043f\u043b\u0430\u0442\u044c\u0435 \u0445\u043e\u0434\u0438\u0448\u044c, \u043b\u0443\u0447\u0448\u0435 \u0441\u0432\u043e\u0437\u0438 \u0436\u0435\u043d\u0443 \u043d\u0430 \u043f\u0440\u0438\u0440\u043e\u0434\u0443, \u0431\u043e\u043b\u044c\u0448\u0435 \u043f\u043e\u043b\u044c\u0437\u044b 
p131
aV\u0438\u0437 \u0441\u0442\u0430\u0440\u0448\u0435\u0433\u043e \u043f\u043e\u043a\u043e\u043b\u0435\u043d\u0438\u044f \u043a\u0442\u043e \u0442\u043e \u043f\u043e\u043c\u043d\u0438\u0442 70 \u0433\u043e\u0434\u044b. \u043a\u043e\u0433\u0434\u0430 \u041f\u0440\u043e\u043c\u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0438\u043a\u0430 \u0441 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u043c \u043f\u0440\u043e\u0435\u043a\u0442\u0430 \u0425\u0438\u043b\u0430\u0436\u0435\u0432\u044b\u043c \u0441\u043e\u0437\u0434\u0430\u0432\u0430\u043b\u0430 \u043f\u0435\u0440\u0432\u0443\u044e"\u044d\u0435\u043b\u0435\u043d\u0443\u044e" \u0432\u043e\u043b\u043d\u0443? \u041f\u0440\u043e\u0448\u043b\u043e 40 \u043b\u0435\u0442\u2026 \u0410\u043b\u0435\u043a\u0441\u0438\u0439
p132
aV\u0418\u043d\u0434\u0438\u0420\u0430 \u041e\u0442\u0432\u0435\u0442 \u043d\u0430 \u0432\u0430\u0448 \u0432\u043e\u043f\u0440\u043e\u0441 \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442 \u043e\u0447\u0435\u0432\u0438\u0434\u043d\u044b\u0439. \u0411\u044b\u043b\u043e \u0431\u044b \u043f\u0440\u0438\u0441\u043e\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u0435 \u043a \u0420\u043e\u0441\u0441\u0438\u0438.
p133
aV\u0412\u0441\u0435 \u043b\u043e\u043f\u043e\u0447\u0443\u0442 ,\u0432\u0441\u0435 \u0445\u043b\u043e\u043f\u043e\u0447\u0443\u0442.\u041d\u0443 \u0432\u0435\u0434\u044c \u0435\u0441\u0442\u044c \u0441\u0440\u0435\u0434\u0438 \u043d\u0438\u0445 \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u044b\u0435 \u0440\u0435\u0431\u044f\u0442\u0430 \u0433\u043e\u0442\u043e\u0432\u044b\u0435 \u0432\u0437\u044f\u0442\u044c \u043d\u0430 \u0441\u0435\u0431\u044f \u0431\u0440\u0435\u043c\u044f \u0437\u0430\u0431\u043e\u0442 \u0438 \u043f\u0440\u043e\u0431\u043b\u0435\u043c ,\u0438 \u0432\u0441\u0435 \u044d\u0442\u043e \u0434\u0435\u043b\u0430\u0442\u044c \u0434\u0435\u0448\u0435\u0432\u043b\u0435 ,\u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0435\u0439.
p134
aV\u0422\u0430\u043a \u0438\u043c \u0438 \u043d\u0430\u0434\u043e,\u043c\u043e\u0436\u0435\u0442 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043f\u043e\u0441\u043b\u0435 \u044d\u0442\u043e\u0433\u043e \u0437\u0430\u0434\u0443\u043c\u0430\u044e\u0442\u0441\u044f.\u0425\u043e\u0442\u044f \u044d\u0442\u043e \u043c\u0430\u043b\u043e\u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e...
p135
aV\u0434\u0430 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b\u0439 \u043f\u0440\u0438\u0431\u043e\u0440,\u0432 \u043c\u043e\u0440\u043e\u0437.\u0432\u043b\u0430\u0436\u043d\u043e\u0441\u0442\u044c \u0434\u0430 \u0435\u0449\u0435. \u0438. \u0432 5 \u0443\u0442\u0440\u0430.
p136
aV\u0412\u0435\u0440\u043d\u0430\u044f, \u043d\u0435\u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e \u043e\u0442 \u0438\u0445 \u0436\u0435\u043b\u0430\u043d\u0438\u044f?)
p137
aV\u041f\u0430\u0443\u043a\u0438 \u0432 \u0431\u0430\u043d\u043a\u0435 \u043d\u0430\u0447\u0430\u043b\u0438 \u0436\u0440\u0430\u0442\u044c \u0434\u0440\u0443\u0433-\u0434\u0440\u0443\u0433\u0430, \u044d\u0442\u043e \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e \u0438 \u043d\u0435 \u043c\u043e\u0436\u0435\u0442 \u043d\u0435 \u0440\u0430\u0434\u043e\u0432\u0430\u0442\u044c, \u043d\u043e \u043d\u0435 \u0438\u043c\u0435\u0435\u0442 \u043d\u0438\u0447\u0435\u0433\u043e \u043e\u0431\u0449\u0435\u0433\u043e \u0441 \u043e\u0447\u0438\u0449\u0435\u043d\u0438\u0435\u043c \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0430 \u0438 \u0437\u0430\u043a\u043e\u043d\u043d\u043e\u0441\u0442\u044c\u044e \u0432 \u043d\u0435\u043c. \u0417\u0430\u043a\u043e\u043d \u0434\u043b\u044f \u043d\u0438\u0445 \u0442\u0435\u043f\u0435\u0440\u044c, \u0442\u0430\u043a\u043e\u0435 \u0436\u0435 \u043e\u0440\u0443\u0434\u0438\u0435 \u043a\u0430\u043a \u0431\u0440\u0430\u0442\u043a\u0438 \u0441 \u0422\u0422 \u0432 90\u0445..
p138
aV\u041c\u0434\u0430, \u043a\u0443\u0434\u0430 \u043a\u0430\u0442\u0438\u0442\u0441\u044f \u043c\u0438\u0440... \u041f\u043e\u043c\u043d\u0438\u0442\u0441\u044f \u043a\u0430\u043a \u0432 \u0434\u0435\u0442\u0441\u0442\u0432\u0435 \u0438\u0433\u0440\u0430\u043b\u0438 \u043d\u0430 \u0443\u043b\u0438\u0446\u0435, \u0432 \u043f\u0440\u044f\u0442\u043a\u0438, \u0434\u043e\u0433\u043e\u043d\u044f\u043b\u043a\u0438, \u0432 \u043c\u044f\u0447. \u0418 \u0432\u0435\u0434\u044c \u0432\u0435\u0441\u0435\u043b\u043e \u0431\u044b\u043b\u043e \u0438 \u0441\u0447\u0430\u0441\u0442\u043b\u0438\u0432\u044b\u043c\u0438 \u0441\u0435\u0431\u044f \u0447\u0443\u0432\u0441\u0442\u0432\u043e\u0432\u0430\u043b\u0438, \u0445\u043e\u0442\u044f \u0438 \u043d\u0435 \u0431\u044b\u043b\u043e \u043d\u0438 \u043a\u043e\u043c\u043f\u043e\u0432, \u043d\u0438 \u0441\u043e\u0442\u043e\u0432\u044b\u0445, \u043d\u0438 \u043d\u0430\u0432\u043e\u0440\u043e\u0447\u0435\u043d\u043d\u044b\u0445 \u043f\u0440\u0438\u0441\u0442\u0430\u0432\u043e\u043a. \u041b\u044e\u0434\u0438! \u0414\u0430 \u043f\u043e\u0431\u043e\u043b\u044c\u0448\u0435 \u043e\u0431\u0449\u0430\u0439\u0442\u0435\u0441\u044c \u0432\u0436\u0438\u0432\u0443\u044e! \u0417\u0430\u0447\u0435\u043c \u0442\u0440\u0430\u0442\u0438\u0442\u044c \u043a\u0443\u0447\u0443 \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u043d\u0430 \u044d\u0442\u043e \u0431\u0435\u0441\u043f\u043e\u043b\u0435\u0437\u043d\u043e\u0435 \u0437\u0430\u043d\u044f\u0442\u0438\u0435 (\u0438\u0433\u0440\u0443\u0448\u043a\u0438)? \u0421\u043a\u043e\u043b\u044c\u043a\u043e \u043f\u0440\u0438\u043c\u0435\u0440\u043e\u0432 \u043d\u0430\u0431\u043b\u044e\u0434\u0430\u043b, \u043a\u043e\u0433\u0434\u0430 \u043b\u044e\u0434\u0438 \u0438\u0433\u0440\u0430\u044e\u0442 \u0447\u0430\u0441\u0430\u043c\u0438 \u043d\u0430\u043f\u0440\u043e\u043b\u0435\u0442, \u043d\u0435 \u0443\u0434\u0435\u043b\u044f\u044f \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u044f \u043d\u0438 \u0440\u043e\u0434\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a\u0430\u043c, \u043d\u0438 \u0436\u0435\u043d\u0430\u043c, \u043d\u0438 \u0434\u0435\u0442\u044f\u043c. \u0414\u043b\u044f \u0447\u0435\u0433\u043e \u0442\u0430\u043a \u0434\u0435\u043b\u0430\u0442\u044c?
p139
aV\u043a\u0440\u0443\u0442\u043e, \u0447\u0442\u043e \u0438\u043c \u043d\u0430\u0448\u043b\u0438 \u0440\u0430\u0431\u043e\u0442\u0443. \u0430 \u043a\u043e\u0433\u0434\u0430 \u043e\u043c\u0438\u0447\u0430\u043c \u043d\u0430\u0439\u0434\u0443\u0442?

p228
aV\u0415\u043c\u0443 \u043c\u044d\u0440 \u0433\u043e\u0432\u043e\u0440\u0438\u0442, \u0447\u0442\u043e \u0431\u0438\u043b\u0435\u0442\u044b \u0434\u043e\u0440\u043e\u0433\u0438\u0435, \u0430 \u043e\u043d \u043e\u0440\u0435\u0442, \u0434\u0435\u0448\u0435\u0432\u043e, \u0433\u043b\u0443\u043f\u044b\u0439 \u0442\u0440\u043e\u043b\u043b\u044c.
p229
aV\u0410 \u0442\u043e \u0441\u0435\u0433\u043e\u0434\u043d\u044f \u043d\u0435 \u0432\u0435\u0437\u0443\u0442 \u0435\u0435 \u0441 \u043a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0430\u2026 \u2014 \u041d\u0430 \u0443\u043b. \u0412\u043e\u043b\u043a\u043e\u0432\u0430 \u043d\u0430\u043f\u0440\u043e\u0442\u0438\u0432 \u0434\u043e\u043c\u0430 7\u0410 \u0433\u0434\u0435 \u043a\u0443\u043b\u0438\u043d\u0430\u0440\u0438\u044f \u2014 \u043a\u043e\u043d\u0433\u0434\u0430 \u043f\u0430\u043b\u0430\u0442\u043a\u0443 \u043f\u0440\u0438\u043a\u0440\u043e\u0435\u0442\u0435 \u0441 \u043f\u0430\u043b\u0435\u043d\u043a\u043e\u0439 \u0414\u043e\u0431\u043b\u0435\u0441\u0442\u043d\u0430\u044f \u041f\u043e\u043b\u0438\u0446\u0438\u044f. \u041d\u0430 \u043f\u0440\u0438\u043b\u0430\u0432\u043a\u0435 \u0442\u0430\u043c \u043b\u0435\u0436\u0438\u0442 \u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u0430\u044f \u0440\u044b\u0431\u0430, \u043d\u043e \u0432\u0441\u0435 \u0437\u043d\u0430\u044e\u0442 \u0447\u0442\u043e \u043f\u0440\u043e\u0434\u0430\u044e\u0442 \u043a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0441\u043a\u0443\u044e \u0432\u043e\u0434\u043a\u0443 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043d\u0430\u043f\u0438\u0442\u043a\u0438 \u0438\u0437 \u043f\u043e\u0434 \u043f\u043e\u043b\u044b. \u043f\u0440\u0438\u0447\u0435\u043c \u0437\u0430\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u0442 \u044d\u0442\u0438\u043c \u0431\u044b\u0432\u0448\u0438\u0439 \u0441\u043e\u0442\u0440\u0443\u0434\u043d\u0438\u043a \u0423\u0412\u0414, \u0441\u0430\u043c \u043f\u0440\u0438\u0432\u043e\u0437\u0438\u0442 \u043d\u0430 \u0433\u0430\u0437\u0435\u043b\u0435 \u0442\u043e\u0432\u0430\u0440 \u0443\u0442\u0440\u043e\u043c \u0438 \u0432\u0435\u0447\u0435\u0440\u043e\u043c \u0437\u0430\u0431\u0438\u0440\u0430\u0435\u0442 \u043f\u0440\u043e\u0434\u0430\u0432\u0449\u0438\u0446\u0443. \u041c\u043e\u0433\u0443 \u043d\u043e\u043c\u0435\u0440 \u043c\u0430\u0448\u0438\u043d\u044b \u0441\u043a\u0438\u
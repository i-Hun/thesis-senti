p91
aV\u041b\u044e\u044e\u044e\u0434\u0438\u0438!!! \u0432\u044b \u043e \u0447\u0435\u043c??? \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043f\u043e\u0433\u0438\u0431! \u041a\u0430\u043a\u0430\u044f \u0440\u0430\u0437\u043d\u0438\u0446\u0430 \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0434\u0435\u043d\u0435\u0433 \u0443 \u043d\u0435\u0433\u043e \u0431\u044b\u043b\u043e?!!! \u041e\u043d \u0434\u043e\u0431\u0438\u043b\u0441\u044f \u0447\u0435\u0433\u043e-\u0442\u043e \u0432 \u0436\u0438\u0437\u043d\u0438, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0438 \u043c\u043e\u0433 \u0441\u0435\u0431\u0435 \u043f\u043e\u0437\u0432\u043e\u043b\u0438\u0442\u044c \u0438 \u043c\u043e\u0442\u043e\u0446\u0438\u043a\u043b \u0438 \u0432\u0441\u0435 \u0442\u043e, \u0447\u0442\u043e \u043e\u043d \u0438\u043c\u0435\u043b! \u043f\u043e\u043b\u0438\u0432\u0430\u0442\u044c \u0433\u0440\u044f\u0437\u044c\u044e \u0443\u043c\u0435\u0440\u0448\u0438\u0445 \u043c\u0435\u0440\u0437\u043a\u043e\u0435 \u0437\u0430\u043d\u044f\u0442\u0438\u0435! \u0412\u044b\u0448\u0435 \u0431\u0443\u0434\u044c\u0442\u0435!!
p92
aV\u0422\u0430\u043a \u043c\u043e\u0436\u0435\u0442 \u043d\u0438\u043a\u0442\u043e \u0438 \u043d\u0435 \u043e\u0431\u044a\u044f\u0432\u043b\u044f\u043b \u043c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u043e\u0433\u043e \u0440\u043e\u0437\u044b\u0441\u043a\u0430 \u044d\u0442\u043e\u0433\u043e \u041b\u0435\u0431\u0435\u0434\u043e\u0432\u0430.\u041f\u0440\u043e\u0441\u0442\u043e \u0441\u043a\u0430\u0437\u0430\u043b\u0438 \u043b\u044e\u0434\u044f\u043c,\u0447\u0442\u043e\u0431\u044b \u043e\u043d\u0438 \u0443\u0441\u043f\u043e\u043a\u043e\u0438\u043b\u0438\u0441\u044c,\u0410 \u0440\u0435\u0430\u043b\u044c\u043d\u043e \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u0434\u0435\u043b\u0430\u044e\u0442.\u0421 \u043d\u0430\u0440\u043a\u043e\u0431\u0430\u0440\u043e\u043d\u0430\u043c\u0438 \u0438 \u0438\u0445 \u0440\u043e\u0434\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a\u0430\u043c\u0438 \u0431\u043e\u0440\u043e\u0442\u044c\u0441\u044f - \u044d\u0442\u043e \u043d\u0435 \u0431\u0430\u0431\u0443\u0448\u0435\u043a \u0441 \u0443\u043a\u0440\u043e\u043f\u043e\u043c \u0434\u0430 \u0440\u0435\u0434\u0438\u0441\u043a\u043e\u0439 \u0433\u043e\u043d\u044f\u0442\u044c.
p93
aV\u0434\u043e\u043c \u043d\u0435 \u043f\u0440\u043e\u0441\u0442\u043e\u0439. \u0414\u0432\u0430 \u044d\u0442\u0430\u0436\u0430. \u041c\u0438\u043b\u043b\u0438\u043e\u043d\u043e\u0432 20. \u000a
p94
aV\u041d77, \u0414\u0430 \u043a\u0430\u043a\u043e\u0439 \u043e\u043d \u043e\u043b\u0438\u043c\u043f\u0438\u0439\u0441\u043a\u0438\u0439 \u0442\u043e ? \u0435\u0433\u043e \u043e\u0442 \u0437\u0430\u0436\u0438\u0433\u0430\u043b\u043a\u0438 100500 \u0440\u0430\u0437 \u0443\u0436\u0435 \u043f\u043e\u0434\u043e\u0436\u0433\u043b\u0438
p95
aV\u041e\u043d\u0438 \u0438 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u044b \u043d\u0435 \u043f\u043e\u0442\u0440\u0430\u0442\u0438\u043b\u0438, \u0430 \u0432\u0441\u0435 \u0440\u0430\u0437\u0432\u043e\u0440\u043e\u0432\u0430\u043b\u0438. \u0421\u0443\u043a\u0438
p96
aV\u0422\u044b \u0438\u0434\u0438\u043e\u0442?, \u043a\u0430\u043a\u043e\u0439 \u043f\u0440\u0438\u043a\u043e\u043b \u0447\u0442\u043e \u0442\u044b \u043e\u0442 \u043c\u043e\u0435\u0433\u043e \u0438\u043c\u0435\u043d\u0438 \u043f\u0438\u0448\u0435\u0448\u044c?! \u043f\u0440\u0438\u0434\u0443\u043c\u0430\u0439 \u0441\u0432\u043e\u0439 \u043d\u0438\u043a, \u0434\u0443\u0431\u0438\u043d\u0430!
p97
aV\u0422\u0423\u041f\u041e\u0419 \u0433\u043e\u0441\u0442!\u043b\u0438\u0447\u043d\u043e \u0434\u043b\u044f \u0442\u0435\u0431\u044f. \u0438\u0434\u0435\u0430\u043b\u044c\u043d\u044b\u0445 \u043b\u044e\u0434\u0435\u0439 \u043d\u0435 \u0431\u044b\u0432\u0430\u0435\u0442!\u0411\u0435\u0441\u0435\u0434\u0443 \u0437\u0430\u0432\u0435\u043b \u0432 \u044d\u0442\u043e \u043d\u0435 \u043f\u043e\u043d\u044f\u0442\u043d\u043e\u0435 \u043f\u0440\u0435\u0440\u0435\u043a\u0430\u043d\u0438\u0435 \u0438\u043c\u0435\u043d\u043d\u043e \u0442\u044b.\u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u0430\u0431\u0441\u043e\u043b\u044e\u0442\u043d\u043e \u043d\u0435 \u0432\u043b\u0430\u0434\u0435\u0435\u0448\u044c \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0435\u0439!\u041f\u044b\u0442\u0430\u0435\u0448\u044c\u0441\u044f \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0438\u0437 \u0441\u0435\u0431\u044f \u043f\u0440\u043e\u0441\u0442\u0430\u0447\u043a\u0430(\u043c\u043e\u0433\u0435\u0448\u044c \u0438 \u0442\u043f)\u0431\u044b\u043b\u0430 \u0431\u044b \u0432 \u0442\u0432\u043e\u0435\u0439  \u0433\u043e\u043b\u043e\u0432\u0435 \u0445\u043e\u0442\u044c \u043a\u0430\u043f\u043b\u044f \u0443\u043c\u0430 \u043d\u0435 \u0441\u0442\u0430\u043b \u0431\u044b \u0431\u043e\u0434\u0430\u0442\u044c\u0441\u044f \u0441 \u043c\u0430\u043b\u043e\u043b\u0435\u0442\u043a\u043e\u0439.\u0430 \u0442\u0430\u043a. \u043a\u0430\u043a \u0442\u044b \u0442\u0443\u043f \u0438\u0437 \u044d\u0442\u043e\u0433\u043e \u0432\u044b\u0445\u043e\u0434\u0438\u0442 \u0442\u0432\u043e\u044f \u043b\u043e\u0433\u0438\u043a\u0430 \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0439 \u0438 \u0432\u044b\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u043d\u0438\u0439.\u043d\u0435 \u0438\u043c\u0435\u0435\u0442 \u043d\u0438\u043a\u0430\u043a\u043e\u0433\u043e \u0441\u043c\u044b\u0441\u043b\u0430. \u0430 \u0442\u0435\u043c \u0431\u043e\u043b\u0435\u0435 \u043b\u043e\u0433\u0438\u043a\u0438.\u0422\u0432\u043e\u0439 \u0443\u0440\u043e\u0432\u0435\u043d\u044c.\u044d\u0442\u043e \u0448\u043a\u043e\u043b\u044c\u043d\u0438\u043a \u0441\u0442\u0430\u0440\u0448\u0438\u0445 \u043a\u043b\u0430\u0441\u0441\u043e\u0432.\u043f\u044b\u0442\u0430\u044e\u0449\u0438\u0439\u0441\u044f \u0432\u044b\u0434\u0430\u0442\u044c \u0441\u0435\u0431\u044f \u0437\u0430 \u043f\u0440\u043e\u0436\u0436\u043e\u043d\u043e\u0433\u043e \u0436\u0438\u0437\u043d\u044c\u044e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430.\u044d\u0442\u043e \u0433\u043b\u0443\u043f\u043e \u0438 \u043f\u0440\u043e\u0441\u0442\u043e. \u0442\u044b \u0413\u041b\u0423\u041f\u042b\u0419.
p98
aV2DrOnan \u0442\u0440\u043e\u043b\u043b\u044c \u0442\u044b *. \u0447\u0435 \u0441 \u0442\u0435\u0431\u044f \u0432\u0437\u044f\u0442\u044c\u2026 \u0432\u043c\u0435\u0441\u0442\u043e \u0442\u043e\u0433\u043e \u0447\u0442\u043e\u0431 \u0447\u0443\u0448\u044c \u043d\u0435\u0441\u0442\u0438 \u043e\u0442\u043a\u0440\u044b\u043b \u0431\u044b \u041f\u0414\u0414. \u0442\u0430\u043c \u0432\u0441\u0435 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u043d\u043e.
p99
aV\u0421\u0432\u044f\u0442\u044b\u0435 \u0434\u0430. \u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0442\u0435, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0434\u0435\u0442\u0435\u0439 \u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0442 \u0441 \u0433\u043e\u0440\u044f\u0449\u0435\u0439 \u043f\u0435\u0447\u043a\u043e\u0439 \u0440\u044f\u0434\u043e\u043c. \u041d\u0435 \u043c\u043d\u043e\u0433\u043e \u0448\u0430\u043d\u0441\u043e\u0432 \u0434\u043e\u0436\u0438\u0442\u044c \u0434\u0430\u0436\u0435 \u0434\u043e \u043c\u043e\u043b\u043e\u0434\u043e\u0441\u0442\u0438.
p100
aV\u0411\u042b\u0414\u041b\u041e, \u0434\u0430 \u0438 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0445\u043e\u0445\u043b\u044f\u0442\u0441\u043a\u043e\u0435, \u0434\u0443\u043c\u0430\u0435\u0442\u0435 \u0443 \u043d\u0430\u0441 \u0432\u044b \u0433\u043e\u0441 \u0434\u0443\u043c\u0435 \u043b\u0443\u0447\u0448\u0435 \u0447\u0442\u043e-\u043b\u0438. \u0422\u0435 \u0436\u0435 \u0441\u0430\u043c\u044b\u0435 \u0437\u0430\u0436\u0440\u0430\u0432\u0448\u0438\u0435\u0441\u044f \u0447\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043b\u044e\u0431\u043e\u0439 \u0446\u0435\u043d\u043e\u0439 \u043b\u0435\u0437\u0443\u0442 \u043a \u0432\u043b\u0430\u0441\u0442\u0438, \u0447\u0442\u043e\u0431\u044b \u0443\u0440\u0432\u0430\u0442\u044c \u043a\u0443\u0441\u043e\u043a \u043f\u043e\u0431\u043e\u043b\u044c\u0448\u0435. \u0414\u0443\u043c\u0430\u0435\u0442\u0435 \u043e\u043d\u0438 \u0437\u0430 \u0441\u0442\u0440\u0430\u043d\u0443 \u0434\u0435\u0440\u0443\u0442\u0441\u044f, \u041d\u0415\u0422 - \u0417\u0410 \u041c\u0415\u0421\u0422\u0410 \u0414\u0415\u041d\u0415\u0416\u041d\u042b\u0415.
p101
aV\u0417\u0430 \u0432\u0441\u0435 \u0432\u0440\u0435\u043c\u044f \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043e\u0432\u0430\u043d\u0438\u044f \u044d\u0442\u043e\u0433\u043e \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u0430 \u2014 \u044f \u0442\u0430\u043c \u043f\u043e\u0431\u044b\u0432\u0430\u043b \u0432\u0441\u0435\u0433\u043e 1 \u0440\u0430\u0437. \u0426\u0435\u043d\u044b \u0442\u0430\u043c \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043a\u0443\u0441\u0430\u044e\u0442\u0441\u044f, \u0435\u0441\u0442\u044c \u043c\u043d\u043e\u0433\u043e \u043c\u0435\u0441\u0442 \u0433\u0434\u0435 \u0442\u0435 \u0436\u0435 \u0442\u043e\u0432\u0430\u0440\u044b \u0434\u0435\u0448\u0435\u0432\u043b\u0435.
p102
aV\u0411\u041a, \u0432\u044b \u0443\u0436\u0435 \u043d\u0435 \u0437\u043d\u0430\u0435\u0442\u0435 \u043a \u0447\u0435\u043c\u0443 \u043f\u0440\u0438\u0432\u044f\u0437\u0430\u0442\u044c\u0441\u044f, \u0435\u0441\u043b\u0438 \u0431\u044b \u0410\u043d\u0434\u0440\u0435\u0439 \u0422\u043a\u0430\u0447\u0443\u043a \u043f\u043e\u0448\u0435\u043b \u0432 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442 \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u044c, \u0438 \u0432 \u044d\u0442\u043e\u043c \u0431\u044b \u043d\u0430\u0448\u043b\u0438 \u043a\u043e\u0440\u044b\u0441\u0442\u043d\u044b\u0439 \u0443\u043c\u044b\u0441\u0435\u043b, \u043d\u0443 \u0441\u043c\u0435\u0448\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e, \u0432\u043e-\u043f\u0435\u0440\u0432\u044b\u0445, \u043e\u0442 \u0442\u043e\u0433\u043e \u0447\u0442\u043e \u0432\u044b \u0432 \u0442\u0435\u043c\u0435 \u0432\u043e\u043e\u0431\u0449\u0435 \u043d\u0435 \u0440\u0430\u0437\u0431\u0438\u0440\u0430\u0435\u0442\u0435\u0441\u044c, \u0445\u043e\u0442\u044f \u0431\u044b \u043f\u043e\u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043e\u0432\u0430\u043b\u0438\u0441\u044c \u0443 \u0442\u043e\u0433\u043e, \u043a\u043e\u043c\u0443 \u043f\u0440\u0438\u043d\u0430\u0434\u043b\u0435\u0436\u0438\u0442 \u0438\u0434\u0435\u044f \u044d\u0442\u043e\u0433\u043e \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430, \u043a\u0430\u043a \u043e\u0431\u0441\u0442\u043e\u0438\u0442 \u0434\u0435\u043b\u043e \u0441 \u0444\u0438\u043d\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435\u043c, \u043f\u043e\u0442\u043e\u043c\u0443 \u043a\u0430\u043a \u0438\u043d\u0438\u0446\u0438\u0430\u0442\u0438\u0432\u0430 \u0431\u043b\u0430\u0433\u0430\u044f, \u043d\u043e \u0432\u043e\u0442 \u043d\u0438\u043a\u0430\u043a\u0438\u043c "\u043c\u0438\u043b\u043b\u0438\u043e\u043d\u043d\u044b\u043c \u0444\u0438\u043d\u0430\u043d\u0441\u043e\u0432\u044b\u043c \u043f\u043e\u0442\u043e\u043a\u043e\u043
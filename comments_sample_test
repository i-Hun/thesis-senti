p103
aV\u0432 \u041d\u043e\u0432\u043e\u0441\u0438\u0431\u0438\u0440\u0441\u043a\u0435 \u0435\u0441\u0442\u044c \u0442\u0430\u043a\u043e\u0439 \u0440\u0435\u0439\u0441. \u041a\u041b\u0410\u0421\u0421!!
p104
aV\u0422\u0435\u043f\u0435\u0440\u044c-\u0442\u043e, \u0432 \u0434\u0435\u043f\u0443\u0442\u0430\u0442\u0430\u0445, \u043a\u043e\u043d\u0435\u0447\u043d\u043e, \u0433\u0430\u0440\u043c\u043e\u043d\u0438\u044f \u0442\u0430\u043a \u0438 \u043f\u0440\u0435\u0442: \u0438 \u0436\u0438\u0437\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0430 \u0438 \u0436\u0438\u0442\u044c \u0445\u043e\u0440\u043e\u0448\u043e
p105
aV\u0422\u0430\u043d\u044f, \u0443 \u0432\u0430\u0441 \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0441\u044f \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0439 \u043e\u0431\u0440\u0430\u0437\u0435\u0446 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u043e\u043b\u044e\u0431\u0438\u044f, \u0442\u0430\u043a \u043c\u0438\u043b\u043e \u0436\u0435\u043b\u0430\u0442\u044c \u0441\u0442\u0440\u0430\u0445\u0430, \u0431\u043e\u043b\u0438 \u0438 \u0441\u043c\u0435\u0440\u0442\u0438 \u043d\u0435\u0437\u043d\u0430\u043a\u043e\u043c\u044b\u043c \u043b\u044e\u0434\u044f\u043c)
p106
aV\u0412\u0447\u0435\u0440\u0430 \u0432\u043e\u043e\u0431\u0449\u0435 \u0436\u0443\u0442\u044c \u0442\u0432\u043e\u0440\u0438\u043b\u0430\u0441\u044c! \u0412 \u0410\u043c\u0443\u0440\u0441\u043a\u043e\u043c \u043f\u043e\u0441\u0451\u043b\u043a\u0435 \u043c\u0430\u0448\u0438\u043d\u0430 \u0432\u044b\u043b\u0435\u0442\u0435\u043b\u0430 \u043d\u0430 \u0436\u0434.\u043f\u0443\u0442\u044c \u043f\u0435\u0440\u0435\u0434 \u043f\u043e\u0435\u0437\u0434\u043e\u043c, \u0431\u0435\u0437 \u0436\u0435\u0440\u0442\u0432, \u043d\u043e \u043c\u0430\u0448\u0438\u043d\u0430 \u0432 \u0442\u0440\u044f\u043f\u043a\u0438, \u0430 \u0447\u0435\u0440\u0435\u0437 2 \u0447\u0430\u0441\u0430 \u0432 100 \u043c\u0435\u0442\u0440\u0430\u0445 \u0434\u0430\u043b\u0435\u0435 \u0447\u0443\u0434\u0430\u043a \u043a\u0438\u043d\u0443\u043b\u0441\u044f \u043f\u043e\u0434 \u0434\u0440\u0443\u0433\u043e\u0439 \u0443\u0436\u0435 \u043f\u043e\u0435\u0437\u0434, \u0442\u0430\u043a \u0435\u043c\u0443 \u0433\u043e\u043b\u043e\u0432\u0451\u043d\u043a\u0443, \u043a\u0430\u043a \u043a\u043e\u0447\u0430\u043d \u043a\u0430\u043f\u0443\u0441\u0442\u044b \u043e\u0442\u0442\u044f\u043f\u0430\u043b\u043e!
p107
aV\u0412\u043e\u0442 \u0432 \u0441\u043a\u0432\u0435\u0440\u0435 \u0416\u0443\u043a\u043e\u0432\u0430-\u0414\u0443\u043c\u0441\u043a\u0430\u044f \u0432\u043e\u043e\u0431\u0449\u0435 \u0447\u0442\u043e \u0442\u0430\u043c \u043a\u0440\u043e\u043c\u0435 \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a\u0430 \u041a\u0430\u0440\u0431\u044b\u0448\u0435\u0432\u0443 \u0435\u0441\u0442\u044c? \u041a\u0442\u043e \u0442\u0443\u0434\u0430 \u0445\u043e\u0434\u0438\u0442?
p108
aV\u0426\u0435\u043b\u0430\u044f \u043a\u0443\u0447\u0430 \u044d\u0441\u0435\u0441\u043e\u0432\u0446\u0435\u0432 \u043d\u0430\u043a\u0440\u044b\u043b\u0430\u0441\u044c. \u042d\u0442\u043e \u0430\u0434\u0435\u043a\u0432\u0430\u0442\u043d\u043e \u0433\u0438\u0431\u0435\u043b\u0438 \u043b\u044e\u0434\u0435\u0439 \u0432 \u041e\u0434\u0435\u0441\u0441\u0435,\u043f\u0440\u0430\u0432\u0434\u0430 \u0442\u0430\u043c \u0444\u0430\u0448\u0438\u0441\u0442\u044b \u0434\u043e\u0431\u0438\u0432\u0430\u043b\u0438 \u0435\u0449\u0451 \u043c\u0438\u0440\u043d\u044b\u0445 \u0438 \u0431\u0435\u0437\u043e\u0440\u0443\u0436\u043d\u044b\u0445! \u0410 \u044d\u0442\u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u043d\u0435 \u0434\u043e\u0435\u0445\u0430\u043b\u0438 \u0434\u043e \u043e\u0447\u0435\u0440\u0435\u0434\u043d\u043e\u0433\u043e \u043c\u0435\u0441\u0442\u0430 \u0443\u0431\u0438\u0439\u0441\u0442\u0432\u0430 \u043c\u0438\u0440\u043d\u044b\u0445 \u043b\u044e\u0434\u0435\u0439!
p109
aV\u0420\u0435\u0431\u044f\u0442\u0430, \u043d\u0435 \u0434\u0435\u043b\u0430\u0439\u0442\u0435 \u0434\u0440\u0443\u0433 \u0434\u0440\u0443\u0433\u0443 \u043d\u0435\u0440\u0432\u044b, \u0438\u0445 \u0435\u0441\u0442\u044c \u0435\u0449\u0435 \u0433\u0434\u0435 \u0438\u0441\u043f\u043e\u0440\u0442\u0438\u0442\u044c. \u0415\u0441\u043b\u0438 \u0443 \u0432\u0430\u0441 \u043d\u0435\u0442 200\u2013300 \u043b\u0438\u043c\u043e\u043d\u043e\u0432 \u0432 "\u0437\u0430\u043d\u0430\u0447\u043a\u0435" (\u043f\u043e \u0431\u0430\u043d\u043a\u0430\u043c \u0438 \u0431\u0443\u043c\u0430\u0433\u0430\u043c), \u0442\u043e \u0443 \u0432\u0430\u0441 \u0432\u0441\u0435 \u0442\u0438\u043f-\u0442\u043e\u043f!
p110
aV\u0430 \u0432 \u043e\u0442\u0432\u0435\u0442 \u043d\u0430 \u043d\u0430\u0448\u0443 \u043f\u043e\u043c\u043e\u0449\u044c \u043e\u043d\u0438 \u043d\u0430\u0432\u0435\u0440\u043d\u044f\u043a\u0430 \u043f\u0440\u0438\u0433\u043b\u0430\u0441\u044f\u0442 \u043e\u0442\u0434\u043e\u0445\u043d\u0443\u0442\u044c \u043b\u0435\u0442\u043e\u043c \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e)))
p111
aV\u0422\u0443\u043f\u043e\u0439 \u043a\u043e\u043c\u043c\u0435\u043d\u0442! \u0422\u0443\u043f\u044b\u0435 \u043c\u043e\u0434\u0435\u0440\u044b! \u041e\u0442 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u043e\u0432 \u0441\u0442\u044b\u0434\u043d\u043e \u0437\u0430 \u0432\u0430\u0448\u0438\u0445 \u0447\u0438\u0442\u0430\u0442\u0435\u043b\u0435\u0439.
p112
aV\u0421\u0442\u0430\u0440\u043e\u041c\u044b\u043a\u043e\u043b\u0430, \u0443\u0439\u0434\u0438 \u0441 \u0441\u0430\u0439\u0442\u0430 \u043d\u0430 \u2026
p113
aV\u0410\u043b\u0451\u043d\u043a\u0430***, \u0430 \u0447\u0442\u043e, \u0440\u0430\u043d\u044c\u0448\u0435 \u043d\u0435 \u0441\u0445\u043e\u0434\u0438\u043b\u0438 \u0441 \u0443\u043c\u0430 \u0438 \u043d\u0435 \u0432\u043e\u0440\u043e\u0432\u0430\u043b\u0438?
p114
aV\u042d\u0442\u043e\u0433\u043e \u0442\u043e \u043a\u043b\u043e\u0443\u043d\u0430 \u0447\u043e \u0441\u044e\u0434\u0430 \u043f\u0440\u0438\u043d\u0435\u0441\u043b\u043e? \u041d\u0430 \u043a\u043e\u0439 \u0431\u044b \u043d\u0430\u0434\u043e \u0431\u044b\u043b\u043e \u0431\u044b \u043d\u0430 \u0435\u0433\u043e \u0440\u043e\u0436\u0443 \u0437\u0430 5 \u0448\u0442\u0443\u043a \u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c. \u0414\u0430 \u0438 \u043a\u0430\u043a\u043e\u0439 \u043e\u043d \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442 \u2014 \u0442\u0430\u043a, \u041d\u0422\u0412\u0448\u043d\u0438\u043a \u0441\u0440\u0435\u0434\u043d\u0435\u0433\u043e \u043f\u043e\u0448\u0438\u0431\u0430. \u0418\u043b\u0438 \u043d\u0430 \u041d\u0422\u0412 \u0437\u0440\u044f\u043f\u043b\u0430\u0442\u044b \u0443\u0440\u0435\u0437\u0430\u043b\u0438 \u0438 \u043f\u0440\u0438\u0435\u0445\u0430\u043b \u043f\u043e\u0434\u0437\u0430\u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c?
p115
aV\u041e\u0445, \u0430\u0434\u0432\u043e\u043a\u0430\u0442, \u043a\u0430\u043a \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043d\u0430\u043f\u0438\u0441\u0430\u043b, \u041d\u043e \u0445\u043e\u0447\u0435\u0442\u0441\u044f \u0436\u0435 \u0432\u0435\u0440\u0438\u0442\u044c \u0432 \u043b\u0443\u0447\u0448\u0435\u0435
p116
aV\u042f \u0442\u0430\u043a \u043f\u043e\u043d\u0438\u043c\u0430\u044e 8000 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u043e\u0432 \u0443\u0435\u0445\u0430\u043b\u043e, \u043a\u0430\u043a \u0440\u0430\u0437 \u0438\u0437 \u0432\u043e\u0441\u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u043d\u0430 \u043e\u043c\u0441\u043a\u043e\u043c \u0440\u044b\u043d\u043a\u0435 \u0442\u0440\u0443\u0434\u0430 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u043e\u0432 \u0438\u043b\u0438 \u044f \u0447\u0442\u043e \u043d\u0435 \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043f\u043e\u043d\u044f\u043b? \u041a\u043b\u0430\u0441\u0441\u043d\u043e, \u0443\u0436\u0435 \u0432\u0430\u043a\u0430\u043d\u0441\u0438\u0438 \u0440\u0430\u0437\u043d\u043e\u0440\u0430\u0431\u043e\u0447\u0438\u0445 \u043d\u0435 \u043c\u043e\u0433\u0443\u0442 \u0437\u0430\u043f\u043e\u043b\u043d\u0438\u0442\u044c \u0440\u0430\u0431\u043e\u0442\u043e\u0434\u0430\u0442\u0435\u043b\u0438, \u0438 \u0437\u0430 \u0441\u0447\u0451\u0442 \u0447\u0435\u0433\u043e \u043e\u043c\u0441\u043a\u0430\u044f \u043e\u0431\u043b\u0430\u0441\u0442\u044c \u0441\u043e\u0431\u0438\u0440\u0430\u0435\u0442\u0441\u044f \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0442\u044c\u0441\u044f? \u041e\u0442\u043b\u0438\u0447\u043d\u0438\u043a\u0430\u043c \u0443\u0447\u0451\u0431\u044b \u043f\u043e\u0432\u044b\u0448\u0435\u043d\u043d\u044b\u0435 \u0441\u0442\u0438\u043f\u0435\u043d\u0434\u0438\u0438 \u043d\u0443\u0436\u043d\u043e \u043f\u043b\u0430\u0442\u0438\u0442\u044c, \u0430 \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0435 \u043e\u0446\u0435\u043d\u043a\u0438 \u0432 \u0434\u0438\u043f\u043b\u043e\u043c\u0435 \u044d\u0442\u043e \u043a\u0440\u0438\u0442\u0435\u0440\u0438\u0439 \u0434\u043b\u044f \u0432\u044b\u0431\u043e\u0440\u0430 \u0440\u0430\u0431\u043e\u0442\u043e\u0434\u0430\u0442\u0435\u043b\u0435\u043c \u0438\u0437 \u0441\u043e\u0438\u0441\u043a\u0430\u0442\u0435\u043b\u0435\u0439 \u0431\u0443\u0434\u0443\u0449\u0435\u0433\u043e \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u0430. \u0410 \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u0443 \u043f\u043b\u0430\u0442\u044f\u0442 \u043d\u0435 \u0437\u0430 \u043e\u0446\u0435\u043d\u043a\u0438 \u0438\u043d\u0441\u0442\u0438\u0442\u0443\u0442\u0441\u043a\u0438\u0435, \u0430 \u0437\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0440\u0430\u0431\u043e\u0442\u044b.
p117
aV\u041e\u0441\u0442\u0430\u0432\u0438\u043b\u0438 \u0431\u044b \u043f\u0435\u0432\u0438\u0446\u0443 \u0432 \u043f\u043e\u043a\u043e\u0435. \u041f\u0440\u0438\u0448\u043b\u0430 \u043d\u0430 \u043a\u043e\u043d\u0446\u0435\u0440\u0442 \u043a\u0430\u043a \u0437\u0440\u0438\u0442\u0435\u043b\u044c, \u0430 \u0435\u0435 \u0438 \u0437\u0434\u0435\u0441\u044c \u0441 \u0432\u043e\u043f\u0440\u043e\u0441\u0430\u043c\u0438 \u0438 \u0444\u043e\u0442\u0438\u043a\u0430\u043c\u0438 \u0434\u043e\u0441\u0442\u0430\u043b\u0438. \u0414\u0430\u0436\u0435 \u0432 \u043f\u0440\u043e\u0432\u0438\u043d\u0446\u0438\u0438 \u043f\u0440\u043e\u0434\u044b\u0445\u0443 \u043d\u0435 \u0434\u0430\u044e\u0442.
p118
aV\u2026 \u0432\u044b\u0448\u0435\u043b \u0437\u0430 \u043f\u0438\u0432\u043e\u043c.
p119
aV\u044f \u043a\u043e\u0433\u0434\u0430 11 \u043b\u0435\u0442 \u043d\u0430\u0437\u0430\u0434 \u0441\u0434\u0430\u0432\u0430\u043b \u043d\u0430 \u043f\u0440\u0430\u0432\u0430, \u043d\u0430\u043c \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u0435\u043b\u044c \u0433\u043e\u0432\u043e\u0440\u0438\u043b \u0447\u0442\u043e \u0435\u0441\u043b\u0438 \u0441\u0435\u043b \u0437\u0430 \u0440\u0443\u043b\u044c \u043d\u0430\u0434\u043e \u044d\u0442\u043e \u0441\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0442\u044c \u0441 \u0442\u0435\u043c \u043a\u0430\u043a \u0431\u0443\u0434\u0442\u043e \u0442\u044b \u0432\u0437\u044f\u043b \u043f\u0438\u0441\u0442\u043e\u043b\u0435\u0442 \u0432 \u0440\u0443\u043a\u0438 \u0438 \u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043d\u0435\u0441\u0451\u0448\u044c \u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u044c. \u043d\u043e \u0435\u0441\u0442\u044c \u0440\u0430\u0437\u043d\u044b\u0435 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438, \u043c\u043e\u0436\u043d\u043e \u0438 \u043e\u0442\u0441\u0442\u0440\u0435\u043b\u0438\u0432\u0430\u0442\u044c \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043b\u0438\u0446 \u0432 \u0440\u0430\u043c\u043a\u0430\u0445 \u0437\u0430\u043a\u043e\u043d\u0430
p120
aV\u0427\u0442\u043e \u0437\u0430 \u0431\u0430\u0437\u0430 \u043d\u0430\u043f\u0438\u0448\u0438\u0442\u0435, \u0447\u0442\u043e\u0431\u044b \u043d\u0435 \u043f\u043e\u0435\u0445\u0430\u0442\u044c \u0442\u0443\u0434\u0430 \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u043e \u043e\u0442\u0434\u044b\u0445\u0430\u0442\u044c - \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u044e \u043a\u0430\u043a\u043e\u0435 \u0442\u0430\u043c \u0431\u0443\u0434\u0435\u0442 \u043e\u0431\u0441\u043b\u0443\u0436\u0438\u0432\u0430\u043d\u0438\u0435.
p121
aV\u041e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u0435 \u0421\u0435\u0440\u0433\u0435\u044f \u0432\u043f\u0435\u0447\u0430\u0442\u043b\u044f\u0435\u0442. \u0423 \u043d\u0435\u0433\u043e \u0442\u043e\u0447\u043d\u043e \u0432\u0441\u0451 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u0441\u044f! \u0423\u0434\u0430\u0447\u0438 \u0438 \u0443\u0441\u043f\u0435\u0445\u043e\u0432!
p122
aV\u0414\u0432\u0443\u0445 \u041e\u043c\u0438\u0447\u0435\u0439 \u043e\u0448\u0442\u0440\u0430\u0444\u043e\u0432\u0430\u043b\u0438 \u0441 \u043d\u0430\u0447\u0430\u043b\u0430 \u0433\u043e\u0434\u0430 :-) \u041f\u043e \u043e\u0434\u043d\u043e\u043c\u0443 \u0448\u0442\u0440\u0430\u0444\u0443 \u0432 \u043c\u0435\u0441\u044f\u0446, \u043f\u043e\u0442\u0440\u044f\u0441\u0430\u044e\u0449\u0438\u0435 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b.
p123
aV\u0410 \u0447\u0442\u043e \u0441\u043a\u0430\u0436\u0435\u0442 \u043d\u0430\u0448 \u0431\u043e\u0440\u0435\u0446 \u0437\u0430 \u043d\u0430\u0440\u043e\u0434, \u043e\u043d \u0436\u0435 \u0433\u043b\u0430\u0432\u043d\u044b\u0439 \u043a\u0440\u0438\u0442\u0438\u043a \u043f\u0440\u0435\u0434\u044b\u0434\u0443\u0449\u0435\u0433\u043e \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u0430 \u0420\u042d\u041a \u0421.\u0421\u0438\u043d\u0434\u0435\u0435\u0432?
p124
aV\u041d\u0430\u0434\u043e \u0432\u043e\u043e\u0431\u0449\u0435 \u0432\u0441\u044e \u0432\u0435\u0440\u0445\u0443\u0448\u043a\u0443 \u043c\u0435\u043d\u044f\u0442\u044c. \u041e\u0434\u043d\u043e \u0432\u043e\u0440\u044c\u0435 \u0443 \u043d\u0430\u0441
p125
